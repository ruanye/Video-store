@keyframes bounce {
    0%, 100%, 20%, 53%, 80% {
        -webkit-transition-timing-function: cubic-bezier(0.215, .61, .355, 1);
        transition-timing-function: cubic-bezier(0.215, .61, .355, 1);
        -webkit-transform: translate3d(0, 0, 0);
        transform: translate3d(0, 0, 0)
    }
    40%, 43% {
        -webkit-transition-timing-function: cubic-bezier(0.755, .050, .855, .060);
        transition-timing-function: cubic-bezier(0.755, .050, .855, .060);
        -webkit-transform: translate3d(0, -0.46875rem, 0);
        transform: translate3d(0, -0.46875rem, 0)
    }
    70% {
        -webkit-transition-timing-function: cubic-bezier(0.755, .050, .855, .060);
        transition-timing-function: cubic-bezier(0.755, .050, .855, .060);
        -webkit-transform: translate3d(0, -0.234375rem, 0);
        transform: translate3d(0, -0.234375rem, 0)
    }
    90% {
        -webkit-transform: translate3d(0, -0.0625rem, 0);
        transform: translate3d(0, -0.0625rem, 0)
    }
}
@keyframes fadeOut {
    0% {
        opacity: 1
    }
    100% {
        opacity: 0
    }
}
.fadeOut {
    -webkit-animation-name: fadeOut;
    animation-name: fadeOut;
    animation-duration: 4s;
    -webkit-animation-duration: 4s;
    animation-fill-mode: both;
    -webkit-animation-fill-mode: both;
}
@keyframes bouncein {
    0% {
        transform: translate3d(0, 0, 0);
        -webkit-transform: translate3d(0, 0, 0);
    }
    ;

    50% {
        transition-timing-function: ease-in;
        -webkit-transition-timing-function: ease-in;
        transform: translate3d(0, -1.5625rem, 0);
        -webkit-transform: translate3d(0, -1.5625rem, 0);
    }
    100% {
        transition-timing-function: ease-in;
        -webkit-transition-timing-function: ease-in;
        transform: translate3d(0, 0.3125rem, 0);
        -webkit-transform: translate3d(0, 0.3125rem, 0);
    }
}
@keyframes shengqi {
    0% {
        transform: translate3d(0, 5.984375rem, 0);
        -webkit-transform: translate3d(0, 5.984375rem, 0);
    }
    100% {
        transition-timing-function: ease-in;
        transform: translate3d(0, 0, 0);
        -webkit-transform: translate3d(0, 0, 0);
    }
}
@keyframes bounce1 {
    0% {
        opacity: 1;
        -webkit-transition-timing-function: cubic-bezier(0.215, .61, .355, 1);
        transition-timing-function: cubic-bezier(0.215, .61, .355, 1);
        -webkit-transform: translate3d(0, 0, 0);
        transform: translate3d(0, 0, 0)
    }
    20% {
        opacity: 0.8;
        -webkit-transition-timing-function: cubic-bezier(0.755, .050, .855, .060);
        transition-timing-function: cubic-bezier(0.755, .050, .855, .060);
        -webkit-transform: translate3d(0.8rem, -0.46875rem, 0);
        transform: translate3d(0.8rem, -0.46875rem, 0)
    }
    40% {
        opacity: 0.6;
        -webkit-transition-timing-function: cubic-bezier(0.755, .050, .855, .060);
        transition-timing-function: cubic-bezier(0.755, .050, .855, .060);
        -webkit-transform: translate3d(1.6rem, -0.5rem, 0);
        transform: translate3d(1.6rem, -0.5rem, 0)
    }
    60% {
        opacity: 0.4;
        -webkit-transform: translate3d(2.4rem, -0.0625rem, 0);
        transform: translate3d(2.4rem, -0.0625rem, 0)
    }
    80% {
        opacity: 0.2;
        -webkit-transform: translate3d(3.2rem, -0.0625rem, 0);
        transform: translate3d(3.2rem, -0.0625rem, 0)
    }
    100% {
        transform: translate3d(4rem, 10rem, 0);
        opacity: 0;
    }
}
