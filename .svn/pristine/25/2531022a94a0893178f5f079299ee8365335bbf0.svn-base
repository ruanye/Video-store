$gridPercentSwitch: true;
@import "tobe/function";
@import "base.scss";
@import "pre.scss";
@charset "UTF-8";

body, html {
    height: 100%;
}
body {
    font: 12px/1.5 "Microsoft YaHei", tahoma, arial, 'Hiragino Sans GB', '宋体', sans-serif;
}
::-webkit-input-placeholder {

    /* WebKit browsers */
    color: #fff;
}
:-moz-placeholder {

    /* Mozilla Firefox 4 to 18 */
    color: #fff;
}
::-moz-placeholder {

    /* Mozilla Firefox 19+ */
    color: #fff;
}
:-ms-input-placeholder {

    /* Internet Explorer 10+ */
    color: #fff;
}
.page {
    position: relative;
    width: 10rem;
    margin: 0 auto;
    height: 100%;
    background-color: #c9daea;
}
.triangle-left {
    position: absolute;
    left: -0.78125rem;
    top: 0.1875rem;
    width: 0;
    height: 0;
    border-top: 0.390625rem solid transparent;
    border-right: 0.78125rem solid #d65555;
    border-bottom: 0.390625rem solid transparent;
    animation-name: shengqi;
    animation-duration: 4s;
    animation-fill-mode: both;
}
.wrap-page {
    height: 100%;
    width: 10.0rem;
    margin: 0 auto;
    position: relative;
}
.page {
    width: 10.0rem;
    margin: 0 auto;
    height: 100%;
    position: relative;
}
%posa {
    position: absolute;
}
%colorf {
    color: #fff;
}
.text-center {
    text-align: center;
}
span, img {
    @include inline-block;
}
$width: 100%;

.maryman {
    animation-duration: 1s;
    animation-name: bounce;
    transform-origin: center bottom;
    animation-iteration-count: infinite;
    animation-timing-function: steps(120);
    animation-fill-mode: both;
}
.mary1 {
    @extend %posa;
    animation-duration: 1s;
    animation-name: bounce1;
    transform-origin: center bottom;
    animation-iteration-count: infinite;
    animation-timing-function: steps(120);
    animation-fill-mode: both;
}
.mary {
    @extend %posa;
    height: 3.5625rem;
    animation-duration: 2s;
    animation-name: bouncein;
    transform-origin: center bottom;
    animation-fill-mode: both;
    transition-timing-function: ease-in-out;
    right: 0;
}
.top {
    @extend %posa;
    width: 10.0rem;
    height: 5.46875rem;
    top: 0;

    img {
        width: 10.0rem;
        height: 5.46875rem;
    }
}
.botm {
    position: absolute;
