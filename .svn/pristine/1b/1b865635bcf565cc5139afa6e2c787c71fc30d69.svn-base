 $gridPercentSwitch:true;
 @import "tobe/function";
 @import "base.scss";
 @import "pre.scss";
 body {
     background-color: #f3f3f3;
 }
 
 .weui_bar_item_on {
     &.subnav {
         color: #51d164;
         background-color: #fff;
         border-bottom: 2px solid #51d164;
     }

 }
 
 .subnav {
     font-size: 0.40625rem;
     background-color: #fff;
       &:hover{
         color: #51d164;
     }
 }
 
 .weui_navbar_item:after {
     content: " ";
     position: absolute;
     right: 0;
     top: 25%;
     width: 1px;
     height: 50%;
     border-right: 1px solid #ccc;
     color: #ccc;
     -webkit-transform-origin: 100% 0;
     transform-origin: 100% 0;
     -webkit-transform: scaleX(.5);
     transform: scaleX(.5);
 }
 
 .boxquery .list {
     padding-left: 0.5rem;
   
     padding-bottom: 0.15625rem;
     background-color: #fff;
     box-shadow: 0px 5px 10px #dcdcdc;
     margin-top: 10px;
     &:first-of-type {
         margin-top: 0;
     }
 }
 
 .status_bar {
     padding-top: 0.15625rem;
     position: relative;
     height: 1.5rem;
     .bartext,
     .barstaus {
         position: absolute;
     }
     .bartext {
         p {
             font-size: 0.375rem;
             font-weight: 600;
             line-height: 0.75rem;
             span {
                 font-weight: 400;
                 &.nprice {
                     color: #c00;
                 }
             }
         }
     }
     .barstaus {
        display: none;
         right: 0.34375rem;
         top: 0.4rem;
         width: 1.921875rem;
         height: 0.6875rem;
         line-height: 0.6875rem;
         border: 1px solid #51d164;
         text-align: center;
         font-size: 0.40625rem;
         color: #51d164;
         border-radius: 0.178125rem;
     }
 }
 
 .goods {
     clear: both;
     overflow: hidden;
     height: auto;
     padding-top: 0.15625rem;
     &:first-of-type {
         border-top: 1px solid #dcdcdc;
     }
     .gimg,
     .gtext {
         float: left;
          overflow:hidden;
          text-overflow: ellipsis;
          white-space: nowrap;
         .title,span{
             overflow:hidden;
          text-overflow: ellipsis;
          white-space: nowrap;
         	margin-left:0.78125rem;
         }

     }
     .gimg {
         border: 1px solid #dcdcdc;
         width: 1.6875rem;
         height: 1.640625rem;
         img {
             width: 1.6875rem;
             height: 1.640625rem;
         }
     }
     .gtext {
        width:7rem;
         margin-top: 0.25rem;
         font-size: 0.375rem;
         margin-left: 0.5rem;
         line-height: 2;
     }
 }
.goods .gtext span.timetext{
  margin-left: 0.15625rem
}