$gridPercentSwitch: true;
@import "tobe/function";
@mixin font-dpr($fonts-size){
    font-size: $fonts-size;

    [data-dpr="2"] & {
        font-size: $fonts-size * 2;
    }

    [data-dpr="3"] & {
        font-size: $fonts-size * 3;
    }
}
.child2{
  float:left;
  width:getPercent(6);
  text-align: center;
  }
* {
  margin:0;padding:0;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
  -webkit-appearance:none;
}
button{border: none;}
button:focus{outline: none;}
html, body{
  height: 100%;
}
.wrap-page {
    height: 100%;
    padding-bottom:1.09375rem;
    background-origin:content-box; 
    -webkit-background-origin:content-box;
  -webkit-overflow-scrolling: touch;
  background:url(../images/b1.png) no-repeat bottom center,#014993 url(../images/chengdu-bg.png) no-repeat top center;
  background-size:100% auto, cover;
  text-align: center;
display: table;table-layout: fixed;width: 100%;
}
.page{display: table-cell;height:100%;width: 100%;vertical-align: middle;}
.inner{margin:0 0.78125rem;
}
.welcome{color: #fff;line-height:1.09375rem;margin-bottom: 0.3125rem;text-shadow:0.0625rem 0.0625rem #9a9a9a;}
.font-1{@include font-dpr(32px);}
.font-2{@include font-dpr(18px);}
.dotted-btn{height:6.75rem;width: 6.609375rem;background:url(../images/dotted-btn.png) no-repeat center;background-size:100% 100%; }

.dotted-info{@include font-dpr(18px);color: #fff;line-height:0.78125rem;width:3.125rem;background: #fb6a6a;border-radius: 0.078125rem;margin:0.625rem auto;}
.bounceOut{
-webkit-animation:bounceOut 1s .2s ease both;
-moz-animation:bounceOut 1s .2s ease both;}
@-webkit-keyframes bounceOut{
0%{-webkit-transform:scale(1)}
25%{-webkit-transform:scale(.95)}
50%{opacity:1;
-webkit-transform:scale(1.1)}
100%{opacity:0;
-webkit-transform:scale(.3)}
}
@-moz-keyframes bounceOut{
0%{-moz-transform:scale(1)}
25%{-moz-transform:scale(.95)}
50%{opacity:1;
-moz-transform:scale(1.1)}
100%{opacity:0;
-moz-transform:scale(.3)}
}

.fixed-header{height:1.09375rem;border-bottom:1px solid #275078;position: fixed;top:0;left:0;right:0;line-height:1.09375rem;color: #fff;background: #232d63}
.user-photo{height:0.875rem;width:0.875rem;@include border-radius(0.4375rem);background-size:cover;background-repeat: no-repeat;background-position: center;position: fixed; top:0.125rem; left:0.625rem }
.chengdu-hongbao{width: 3.84375rem;margin:0.3125rem auto}
.row:after,.row:before{    display: table;
    content: " ";
    clear:both;
     }
.com-btn{@include font-dpr(14px);background: #eb6766;border-radius:0.234375rem;color: #fff;text-align: center;padding:0.078125rem;box-shadow:0 0 0 0.09375rem #eb6766;border:1px dashed #fff;height: 1.0625rem;min-width: 1.75rem}
.chengdu-share{position: fixed;top:0.40625rem;right:0.234375rem;width:2.96875rem}
.info-box-good1{@include font-dpr(14px);background: #3ac0c8;@include border-radius(0.15625rem);color: #fff;text-align: center;padding:0.15625rem;@include box-shadow(0 0 0.15625rem rgba(0,0,0,.5));margin:0.625rem auto;width: 6.75rem;}
.info-inner1{@include border-radius(0.15625rem);border: 1px dashed #fff;}
.ipt{outline:none;padding:0.234375rem;
    -webkit-appearance: none;
    margin:0;
    @include font-dpr(14px);background: #eb6766;border-radius:0.234375rem;color: #fff;text-align: center;padding:0.078125rem;box-shadow:0 0 0 0.09375rem #eb6766;border:1px dashed #fff;height: 1.0625rem;min-width: 1.75rem;
}
.jiang-type{font-size: 0.59375rem;margin:0 auto 0.390625rem auto;font-weight: bold;color: #fff}

.welcome1{@include font-dpr(14px);color: #fff;line-height:1.09375rem;margin-bottom: 0.3125rem;}
.chengdu-large-btn{margin-top:0.625rem;margin-bottom:0.46875rem;line-height:1.5625rem;height: 1.5625rem;width: 6.84375rem;@include font-dpr(14px);background: #3ac0c8;@include border-radius(0.15625rem);color: #fff;text-align: center;@include box-shadow(0 0 0.15625rem rgba(0,0,0,.5));position: relative;}
.chengdu-large-btn:after{content: "";width: 6.5rem;height: 1.25rem;position: absolute;@include border-radius(0.15625rem);border: 1px dashed #fff;top:0.15625rem;left:0.171875rem; }
.resault-ts-box{margin:0.3125rem 0.234375rem 0 0.234375rem;color: #fff;@include font-dpr(14px);}
.strong-box{@include font-dpr(18px);color: #fee098;font-weight: bold;margin-left: 0.15625rem}
.result-info-box{margin:0.234375rem;color: #fff;@include font-dpr(10px);}
.chengdu-nomarl-btn{line-height: 1.0rem;height: 1.0rem;width: 4.71875rem;@include font-dpr(12px);background: #28557b;@include border-radius(0.15625rem);color: #fff;text-align: center;@include box-shadow(0 0 0.15625rem rgba(0,0,0,.5));}

