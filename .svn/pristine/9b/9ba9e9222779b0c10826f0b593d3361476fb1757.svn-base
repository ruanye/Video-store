var teacherId = "";//教师ID
var courseId = "";//课程ID
var classId = "";//班级ID
var bookName = "";//课程名称
var timetableId = "";//课表ID


$(function() {
    Main.init();

    $(".main-item").bind("click", function() {
        var id = $(this).attr("data-id");
        SS.set(Constant.current,id);
        SS.set(Constant.isMenu,"true");
        switch (id) {
            case "01":
                console.log("show page 成员!!");
                Main.goToPage("member.html");
                break;
            case "02":
                console.log("show page 测试!!");
                Main.goToPage("tests-push.html");
                break;
            case "03":
                console.log("show page 提问!!");
                Main.goToPage('personal-questions.html');
                break;
            case "04":
                console.log("show page 课件!!");
                Main.goToPage('course.html');
                break;
            case "05":
                console.log("show page 签到!!");
                Main.goToPage("check-in.html");
                break;
            case "06":
                console.log("show page 作业!!");
                break;
            default:
                console.log("not found page error!!");
                break;
        }

    });
});
var Main = {
    init : function(){
        SS.remove(Constant.current);
        SS.remove(Constant.isMenu);
        teacherId = SS.get(Constant.teacherId);//教师ID
        courseId = SS.get(Constant.courseId);//课程ID
        classId = SS.get(Constant.classId);//班级ID
        bookName = SS.get(Constant.bookName);//课程名称
        timetableId = SS.get(Constant.timetableId);//课表ID
    },
    goToPage:function(url){
        location.href = url;
    }
}