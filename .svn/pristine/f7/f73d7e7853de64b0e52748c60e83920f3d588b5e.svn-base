-weight: bold;\r\n    -webkit-appearance: none;\r\n    background: #c46fa9;\r\n    border: 0;\r\n    text-align: center;\r\n    @include font-dpr(18px);\r\n    box-shadow: 0 0.25rem 0 0 #bb4493;\r\n    position: relative;\r\n    &:before{\r\n        content:\"\";\r\n        position: absolute;\r\n        width: 0.166667rem;\r\n        height: 0.138889rem;\r\n        background: #e0a5c8;\r\n        border-radius: 0.069444rem;\r\n        top:0.111111rem;\r\n        left:0.166667rem;\r\n        z-index: 2;\r\n\r\n    }\r\n    &:after{\r\n        content:\"\";\r\n        position: absolute;\r\n        width: 1.347222rem;\r\n        height: 0.138889rem;\r\n        background: #e0a5c8;\r\n        border-radius: 0.069444rem;\r\n        top:0.111111rem;\r\n        left:0.527778rem;\r\n        z-index: 2;\r\n    }\r\n    span{position: relative;z-index: 5;letter-spacing: 2px;}\r\n}\r\n.com-btn2{width: 7.361111rem;height: 1.097222rem;border:1px solid #c46fa9;border-radius: 0.208333rem;background: #fff;line-height: 1.097222rem;position: relative;margin:0.180556rem auto;@include font-dpr(16px);\r\n&:after{content: \"\";position: absolute;width: 0.25rem;height:1.097222rem;right:-0.25rem;background: url(../images/p18.png) no-repeat right center;background-size: cover;}\r\n}\r\n\r\n.ipt {\r\n    outline: none;\r\n    width: 6.041667rem;\r\n    height: 0.944444rem;\r\n    line-height: 0.944444rem;\r\n    color: #000;\r\n    -webkit-appearance: none;\r\n    background: none;\r\n    padding: 0.234375rem;\r\n    border: 1px solid #e0a5c8;\r\n    text-align: center;\r\n    @include font-dpr(16px);\r\n    border-radius: 0;\r\n}\r\n\r\n// 文案\r\n.info-lg {\r\n    @include font-dpr(32px);\r\n    font-weight: bold;\r\n    color: #fff\r\n}\r\n\r\n.info-m {\r\n    @include font-dpr(16px);\r\n    color: #000;\r\n}\r\n\r\n.info-lg1 {\r\n    @include font-dpr(28px);\r\n    font-weight: bold;\r\n    color: #000\r\n}\r\n\r\n.info-s {\r\n    @include font-dpr(12px);\r\n    color: #000\r\n}\r\n\r\n\r\n\r\np {\r\n    line-height: 1.5;\r\n}\r\n\r\n// mask\r\n.mask {\r\n    position: fixed;\r\n    top: 0;\r\n    left: 0;\r\n    bottom: 0;\r\n    right: 0;\r\n    background: rgba(0, 0, 0, 0.7);\r\n    z-index: 99;\r\n}\r\n//index-page\r\n#index-page{background: url(../images/b2.png) no-repeat top center,#bce1e0 url(../images/b1.png) no-repeat bottom center;\r\n    background-size: 100% auto,100% auto;\r\n\r\n}\r\n.p1{width: 7.180556rem;margin: 2.916667rem auto 1.111111rem auto;}\r\n.p2{width: 8.166667rem;margin: auto;}\r\n.l1{position: absolute;top:0.513889rem;left:0.555556rem;width: 1.805556rem;}\r\n//game-page\r\n#game-page,#notice-page{background: url(../images/b3.png) no-repeat top right,#bce1e0 url(../images/b1.png) no-repeat bottom center;\r\n    background-size: 6.652778rem auto,100% auto;\r\n\r\n}\r\n.p1{width: 7.180556rem;margin: 2.916667rem auto 1.111111rem auto;}\r\n.p2{width: 8.166667rem;margin: auto;}\r\n.l2{position: absolute;top:0.513889rem;left:0.555556rem;width: 1.805556rem;}\r\n.jiang-out-box{position: relative;height: 12.875rem;width: 8.777778rem;background:url(../images/p10.png) no-repeat center;background-size:100%;margin: auto;}\r\n.button-box{position: absolute;top:10.75rem;left:0;right:0;}\r\n.com-btn1{height: 1.722222rem;width: 5.541667rem;background: url(../images/p3.png) no-repeat center;background-size: 100%;@include font-dpr(18px);color: #fff;font-weight: bold;}\r\n.jiang-box {list-style: none;position: relative;top:3.4rem;margin-left:0.972222rem;margin-right:0.986111rem;}\r\n.jiang-box li{height: 1.083333rem;width: 1.083333rem;border:1px solid #000;border-radius: 0.208333rem;box-shadow:0 0 0 0.097222rem #838384,0.138889rem 0 0 0 #030000;background-color: #fff;background-size: contain;background-repeat: no-repeat; background-position:center;position: absolute;}\r\n.jiang-box .active{box-shadow:0 0 0 0.097222rem #f2eb35,0.138889rem 0 0 0 #030000 !important;}\r\n.jiang-box li:nth-child(1){left:0.291667rem;top:0.208333rem;background-image