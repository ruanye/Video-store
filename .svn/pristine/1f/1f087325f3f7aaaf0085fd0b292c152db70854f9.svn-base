}

$width: 100%;
.maryman{
       @extend %posa;
    animation-duration: 1s;
    animation-name: bounce;
    transform-origin: center bottom;
    animation-iteration-count: infinite;
    animation-timing-function:steps(120);
    animation-fill-mode: both;
}
.mary1 {
    @extend %posa;
    animation-duration: 1s;
    animation-name: bounce1;
