</div>




<script type='text/javascript' src='http://g.alicdn.com/sj/lib/zepto/zepto.min.js' charset='utf-8'></script>
<script type='text/javascript' src='http://g.alicdn.com/msui/sm/0.6.2/js/sm.min.js' charset='utf-8'></script>
<script type='text/javascript' src='http://g.alicdn.com/msui/sm/0.6.2/js/sm-extend.min.js' charset='utf-8'></script>
<script src="js/static/libs/base.js"></script>
<script src="js/static/libs/ls.js"></script>
<script src="js/public/carEdit.js"></script>


<script>
    var strCar=[];
    $("#capital").click(function(){
        strCar.length=0;
        $(".screen_mask1").css('display', 'block');
        $("#maskContent1").css('display', 'block');

    });
    $(".screen_mask1").click(function(){
        $(".screen_mask1").css('display', 'none');
        $("#maskContent1").css('display', 'none');
    });
    $(".capital-btn").click(function(){
        var val1 = $(this).html();
        strCar.push(val1);
        // $("#capital").html(val);
        //$(".screen_mask1").css('display', 'none');
        $("#maskContent1").css('display', 'none');
        $("#maskContent2").css('display', 'block');
        if(strCar.length==2){
            $(".screen_mask1").css('display', 'none');
            $("#maskContent2").css('display', 'none');
            $("#capital").html(strCar[0]+strCar[1]);
        }

        // alert(strCar[0]+strCar[1]);
    });

</script>

</body>
</html>