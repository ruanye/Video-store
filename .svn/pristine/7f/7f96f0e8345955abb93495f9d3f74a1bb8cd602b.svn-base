                <td><p class="capital-btn">沪</p></td>
            </tr>

            <tr>
                <td><p class="capital-btn">苏</p></td>
                <td><p class="capital-btn">浙</p></td>
                <td><p class="capital-btn">皖</p></td>
                <td><p class="capital-btn">闽</p></td>
                <td><p class="capital-btn">赣</p></td>
                <td><p class="capital-btn">鲁</p></td>
                <td><p class="capital-btn">豫</p></td>
                <td><p class="capital-btn">鄂</p></td>
                <td><p class="capital-btn">湘</p></td>
            </tr>

            <tr>
                <td><p class="capital-btn">粤</p></td>
                <td><p class="capital-btn">桂</p></td>
                <td><p class="capital-btn">琼</p></td>
                <td><p class="capital-btn">渝</p></td>
                <td><p class="capital-btn">川</p></td>
                <td><p class="capital-btn">贵</p></td>
                <td><p class="capital-btn">云</p></td>
                <td><p class="capital-btn">藏</p></td>
                <td><p class="capital-btn">陕</p></td>
            </tr>

            <tr>
                <td><p class="capital-btn">甘</p></td>
                <td><p class="capital-btn">青</p></td>
                <td><p class="capital-btn">宁</p></td>
                <td><p class="capital-btn">新</p></td>
            </tr>
            </tbody></table>
    </div>
</div>


<div class="maskcontent" id="maskContent2" style="display: none;">
    <div class="capital_list" id="capital_list2">
        <table>
            <tbody><tr>
                <td><p class="capital-btn">A</p></td>
                <td><p class="capital-btn">B</p></td>
                <td><p class="capital-btn">C</p></td>
                <td><p class="capital-btn">D</p></td>
                <td><p class="capital-btn">E</p></td>
                <td><p class="capital-btn">F</p></td>
                <td><p class="capital-btn">G</p></td>
                <td><p class="capital-btn">H</p></td>
