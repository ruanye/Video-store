                <div class="text">乘</div>
                <div class="text">车</div>
                <div class="text">金</div>
            </div>
            <div class="discount">
                <div class="left">
                    <div class="text">领取优惠</div>
                </div>
                <div class="right"></div>
            </div>
        </section>          
        <section class="page" id="page4" style="display:none">
            <div class="p1">UBER优步</div>
            <div class="p2"></div>
            <div class="content">
                <img src="images/detail.png" alt="" style="width:100%;margin-top:2rem;">
            </div>
            <div class="discount" style="margin-top:0.5rem;">
                <div class="left">
                    <div class="text">领取优惠</div>
                </div>
            </div>
        </section>                    
