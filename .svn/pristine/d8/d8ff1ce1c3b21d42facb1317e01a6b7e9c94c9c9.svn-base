<body>
<!--     <div class="music">
        <audio src="music/icecream.mp3" autoplay="autoplay"></audio>
    </div> -->
    <div class="wrap-page">
        <div id="wx_pic" style="display: none;">
            <img src="images/icon.jpg" />
        </div>
        <section class="page" id="page1">
            <div class="p1">UBER优步</div>
            <div class="p2"></div>
            <div class="p3"><span>恭喜您</span>压对了今日冠军</div>
            <div class="p4">24小时押注赢车费</div>
            <div class="icon"></div>
            <button type="button" class="getBtn"></button>
        </section>           
        <section class="page" id="page2" style="display:none">
            <div class="p1">UBER优步</div>
            <div class="content">
