$gridPercentSwitch: true;
@import "tobe/function";
@import "base.scss";
@import "pre.scss";
@charset "UTF-8";

body, html {
    height: 100%;
}
body {
    font: 12px/1.5 "Microsoft YaHei", tahoma, arial, 'Hiragino Sans GB', '宋体', sans-serif;
}
$bgcolor: #8acbb7;
$width: 10rem;

.wrap-page {
    height: 100%;
    width: 10.0rem;
    margin: 0 auto;
}
.page {
    width: 10rem;
    margin: 0 auto;
    height: 100%;
    position: relative;
}
%rel {
    position: relative;
}
%posa {
    position: absolute;
}
%colorf {
    color: #fff;
}
.text-center {
    text-align: center;
}
img, span, input {
    @include inline-block;
}
button {
    background-color: transparent;
    outline: none;
}
.page1 {
    background: url('../images/Page1.png') #8e8f93 left top no-repeat;
    background-size: 10rem 5.666667rem;

    .t1 {
        padding-top: 4.6rem;
        font-size: 1.0rem;
        font-weight: 700;
    }
    .t2 {
        font-size: 0.533333rem;
    }
    .t3 {
        padding-top: 0.853333rem;
        font-size: 0.426667rem;
        line-height: 1.8;
    }
    .t4 {
        font-size: 0.906667rem;
        color: #4fcfb0;
        font-weight: 700;
    }
    .btnbox {
        margin-top: 1.066667rem;

        img {
            margin: 0 0.44rem;

            &:nth-child(1) {
                width: 3.466667rem;
                height: 3.32rem
            }
            &:nth-child(2) {
                width: 3.466667rem;
                height: 3.2rem
            }
        }
    }
}
.white {
    color: #fff;
}
.yellow {
    color: #ffb26b
}
.page2 {
    background: url('../images/Page2.png') #8e8f93 left top no-repeat;
    background-size: 10rem 12.44rem;

    .text {
        padding-top: 5.12rem;

        p {
            padding-left: 1.36rem;
            font-size: 0.4rem;

            &.bold, span {
                font-size: 0.373333rem;
                font-weight: 700;
            }
            &.jiezhi {
                padding-top: 0.25rem;
            }
            &.last {
                padding-top: 0.3rem;
            }
            &.t8 {
                font-size: 0.426667rem;
                line-height: 1.8;
            }
        }
    }
}
.bimg {
    position: absolute;

    &.a {
        width: 0.56rem;
        height: 1.173333rem;
        top: 7.733333rem;
        left: 0.586667rem;
    }
    &.b {
        width: 0.76rem;
        height: 1.08rem;
        top: 13.32rem;
        left: 0.4rem
    }
    &.c {
        width: 0.586667rem;
        height: 0.826667rem;
        right: 0.16rem;
        bottom: 0.133333rem;
    }
}
.mtext {
    padding-top: 1.866667rem;
    font-size: 0.853333rem;
}
.tpbtn {
    margin-top: 0.64rem;

    span {
        width: 8.133333rem;
        height: 1.12rem;
        line-height: 1.12rem;
        border-radius: 0.56rem;
        background-color: #5dcfad;
        color: #fff;
        font-size: 0.533333rem;
    }
}
.page3 {
    background: url('../images/pg3.png') #8e8f93 left top no-repeat;
    background-size: 10rem 6.0rem;

    .tt1 {
        padding-top: 3.013333rem;
        font-size: 0.933333rem;
        font-weight: 700;

        span {
            font-size: 0.506667rem;
        }
    }
}
