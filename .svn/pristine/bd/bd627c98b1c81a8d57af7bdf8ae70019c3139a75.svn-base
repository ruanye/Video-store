    height:1.2rem
}
.page5 {
    background: url('../images/p5.png') #8e8f93  left top no-repeat;
    background-size: 10rem 4.826667rem;
    .tq{
        font-size: 0.8rem;
        padding-top: 2.666667rem;
        font-weight: 700;
    }
}
