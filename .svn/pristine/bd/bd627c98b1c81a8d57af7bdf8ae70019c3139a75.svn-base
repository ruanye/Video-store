.page4 {
    background: url('../images/pg4.png')  left top no-repeat, url('../images/p4.png')  left bottom no-repeat;
    background-size: 10rem 4.826667rem;

    .t1 {
        padding-top: 1.813333rem;
        font-size: 0.8rem;
        color: #8d9299;
        font-weight: 700;
    }
}
.uberlist {
    margin-top: 0.8rem;

    li {
        margin-bottom: 0.333333rem;
        width: 100%;
        height: 0.8rem;

        img {
            margin-left: 0.533333rem;
            float: left;
            width: 5.746667rem;
            height: 0.8rem;
            position: relative;
            z-index: 200
        }
        span {
            width: 1.466667rem;
            text-align: right;
            float: left;
            margin-left: 1.173333rem;
            height: 0.8rem;
            line-height: 0.8rem;
            font-size: 0.48rem;
        }
        b {
            position: relative;
            display: block;
            width: 4.96rem;
            height: 0.373333rem;
            background-color: #ef404d;
            margin-left: 3.84rem;
            top: 0.22rem;

            &:before {
                position: absolute;
                content: '';
                display: block;
                width: 0.64rem;
                height: 0.64rem;
                background-color: #ef404d;
                border-radius: 50%;
                left: -0.58rem;
                top: -0.1rem;
            }
        }
        b {
            &.b1 {
                background-color: #e16dbb;

                &:before {
                    background-color: #e16dbb;
                }
            } 
            &.b2 {
                background-color: #4fcfb0;

                &:before {
                    background-color: #4fcfb0;
                }
            } 
             &.b3 {
                background-color: #49c8f8;

                &:before {
                    background-color: #49c8f8;
                }
            }

        }
    }
}
.sharebtn{
    margin-top: 1.2rem;
