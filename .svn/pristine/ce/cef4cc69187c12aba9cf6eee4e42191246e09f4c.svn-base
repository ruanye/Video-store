.fade-top-3 {
  -webkit-animation: fadeInDownBig 1s .6s ease both;
  -moz-animation: fadeInDownBig 1s .6s ease both; }

@-webkit-keyframes fadeInDownBig {
  0% {
    opacity: 0;
    -webkit-transform: translateY(-2000px); }
  60% {
    opacity: 1;
    -webkit-transform: translateY(30px); }
  80% {
    -webkit-transform: translateY(-10px); }
  100% {
    opacity: 1;
    -webkit-transform: translateY(0); } }

.fadeIn {
  -webkit-animation: fadeIn 2s .5s ease both;
  -moz-animation: fadeIn 2s .5s ease both; }

@-webkit-keyframes fadeIn {
  0% {
    opacity: 0; }
  100% {
    opacity: 1; } }

.fadeOut {
  -webkit-animation: fadeOut 2s .5s ease both;
  -moz-animation: fadeOut 2s .5s ease both; }

@-webkit-keyframes fadeOut {
  0% {
    opacity: 1; }
  100% {
    opacity: 0; } }

.bounceInDown {
  -webkit-animation: bounceInDown 1s .2s ease both;
  -moz-animation: bounceInDown 1s .2s ease both; }

@-webkit-keyframes bounceInDown {
  0% {
    opacity: 0;
    -webkit-transform: translateY(-2000px); }
  60% {
    opacity: 1;
    -webkit-transform: translateY(30px); }
  80% {
    -webkit-transform: translateY(-10px); }
  100% {
    -webkit-transform: translateY(0); } }

/* Chrome, Safari, Opera */
@-webkit-keyframes progressbar {
  from {
    height: 0; }
  to {
    height: 100%; } }

/* Standard syntax */
@keyframes progressbar {
  from {
    height: 0; }
  to {
    height: 100%; } }

/*按钮展开*/
.bounceInOpen {
  -webkit-transform-origin: center center;
  -moz-transform-origin: center center;
  -webkit-animation: bounceInOpen 1.5s 2s ease both;
  -moz-animation: bounceInOpen 1.5s 2s ease both; }

@-webkit-keyframes bounceInOpen {
  0% {
    opacity: 0;
    width: 0;
    right: 50%; }
  20% {
    opacity: 1;
    width: 0;
    right: 50%; }
  40% {
    opacity: 1;
    width: 0;
    right: 50%; }
  60% {
    width: 100%;
    right: 0; }
  80% {
    width: 90%;
    right: 10%; }
  100% {
    width: 100%;
    right: 0; } }

.cloud1 {
  -webkit-animation: fadeInLeftBig 8s .2s linear both infinite;
  -moz-animation: fadeInLeftBig 8s .2s linear both infinite; }

.cloud2 {
  -webkit-animation: fadeInLeftBig 8s 1.5s linear both infinite;
  -moz-animation: fadeInLeftBig 8s 1.5s linear both infinite; }

@-webkit-keyframes fadeInLeftBig {
  0% {
    opacity: 0;
    -webkit-transform: translateX(0); }
  20% {
    opacity: 1;
    -webkit-transform: translateX(10vw); }
  80% {
    opacity: 1;
    -webkit-transform: translateX(50vw); }
  100% {
    opacity: 0;
    -webkit-transform: translateX(80vw); } }

@-webkit-keyframes fadeInRightBig {
  0% {
    opacity: 0;
    -webkit-transform: translateX(2000px); }
