.clock {
  margin: 0 auto;
  padding: 10px 117px;
  margin-bottom: 10px;
  color: #032052;
  text-align: right; }

.clock span {
  font-family: 'BebasNeueRegular', Arial, Helvetica, sans-serif;
  font-size: 22px;
  color: #032052;
  text-align: center; }

#point {
  position: relative;
  -moz-animation: mymove 1s ease infinite;
  -webkit-animation: mymove 1s ease infinite;
  padding: 0 2px; }

@-webkit-keyframes mymove {
  0% {
    opacity: 1.0; }
  50% {
    opacity: 0; }
  100% {
    opacity: 1.0; } }

