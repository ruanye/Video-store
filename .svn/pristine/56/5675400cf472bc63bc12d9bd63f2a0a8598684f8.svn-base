$gridPercentSwitch:true;
@import "tobe/function";
@import "base.scss";
@import "pre.scss";
@charset "UTF-8";
body,
html {
    height: 100%;
}

body {
    font: 12px/1.5 "Microsoft YaHei", tahoma, arial, 'Hiragino Sans GB', '宋体', sans-serif;
    background-color:#f8f8f8;
}
*{
	-webkit-tap-highlight-color:rgba(0,0,0,0);
}
@font-face {
	font-family: 'iconfont';
    src: url('fonts/iconfont/iconfont.eot'); /* IE9*/
    src: url('fonts/iconfont/iconfont.eot?#iefix') format('embedded-opentype'), /* IE6-IE8 */
    url('fonts/iconfont/iconfont.woff') format('woff'), /* chrome、firefox */
    url('fonts/iconfont/iconfont.ttf') format('truetype'), /* chrome、firefox、opera、Safari, Android, iOS 4.2+*/
    url('fonts/iconfont/iconfont.svg#iconfont') format('svg'); /* iOS 4.1- */
}
.iconfont{
    font-family:"iconfont" !important;
    font-size:0.4rem;
    font-style:normal;
    -webkit-font-smoothing: antialiased;
    -webkit-text-stroke-width: 0.2px;
    -moz-osx-font-smoothing: grayscale;
    color:#333;
}
.wrap-page {
	color:#656565;
	.chufang-Icon{
		height:4.6875rem;
		background-color: #fff;
		.title{
			font-size:0.4225rem;
			margin-left:0.43875rem;
			margin-bottom: 0.15625rem;
		}
		.icon-box{
			height:3.359375rem;
			white-space: nowrap;
			overflow-x: auto;
            overflow-y: hidden;
            position: relative;
			&:after{
				display: block;
				content: '.';
				height: 0;
				clear: both;
				visibility: hidden;
			}
			.item{
				display:inline-block;
				height:3.359375rem;
				width:2.578125rem;
				margin-left:0.078125rem;
				&:nth-child(1){
					margin-left:0.43875rem;
				}
			}
			.item1{
				background:url(../images/1/chufang-Icon.jpg) no-repeat;
				background-size:100% 100%;
			}
			.item2{
				background:url(../images/1/add-icon.png) no-repeat;
				background-size:100% 100%;				
			}
		}

	}
	.storeSelect{
		display: flex;
		height:1.15625rem;
		font-size:0.4325rem;
		background-color: #fff;
		margin-top:0.3125rem;
		border-top:1px solid #dcdcdc;
		.text{
			width:5.9375rem;
			height:1.15625rem;
			line-height:1.15rem;
			text-indent: 0.3325rem;
		}
		.shuhan,.gaoxin{
			height:1.15625rem;
			line-height:1.15rem;	
			width:3.01875rem;	
			position: relative;
			.select-text{
				text-indent: 0.5325rem;
			}
			.select-icon{
				position:absolute;
				left:0;
				color:#ddd;
			}
			.select-icon.active{
				color:#57cf69;
				font-size:0.48rem;
			}
		}
	}
	.numSelect{
		display: flex;
		height:1.15625rem;
		font-size:0.4325rem;
		background-color: #fff;
		border-top:1px solid #dcdcdc;
		.text{
			width:7.9375rem;
			height:1.15625rem;
			line-height:1.15rem;
			text-indent: 0.3325rem;			
		}	
		.number{
			height:1.15625rem;
			line-height:1.15rem;	
			width:3.01875rem;	
			position: relative;
			text-indent: 2.05rem;	
			.num{
				height:0.66875rem;
				width:1.7625rem;
				border:1px solid #dcdcdc;
				position:absolute;
				left:0;
				top:0.2125rem;
				border-radius:0.15625rem;
				text-align: center;
			}		
		}	
	}
	.type{
		display: flex;
		height:1.15625rem;
		font-size:0.4325rem;
		background-color: #fff;
		border-top:1px solid #dcdcdc;
		.text{
			width:4.4375rem;
			height:1.15625rem;
			line-height:1.15rem;
			text-indent: 0.3325rem;			
		}
		.type1,.type2,.type3{
			height:1.15625rem;
			line-height:1.15rem;	
			width:2.01875rem;	
			position: relative;
			.select-text{
				text-indent: 0.5325rem;
			}
			.select-icon{
				position:absolute;
				left:0;
				color:#ddd;
			}
			.select-icon.active{
				color:#57cf69;
				font-size:0.48rem;
			}	
		}		
	}
	.transport{
		display: flex;
		height:1.15625rem;
		font-size:0.4325rem;
		background-color: #fff;
		border-top:1px solid #dcdcdc;
		.text{
			width:4.1675rem;
			height:1.15625rem;
			line-height:1.15rem;
			text-indent: 0.3325rem;			
		}	
		.type1,.type2{
			height:1.15625rem;
			line-height:1.15rem;	
			width:2.71875rem;	
			position: relative;
			.select-text{
				text-indent: 0.5325rem;
			}
			.select-icon{
				position:absolute;
				left:0;
				color:#ddd;
			}
			.select-icon.active{
				color:#57cf69;
				font-size:0.48rem;
			}			
		}		
	}
	.address{
		min-height:1.875rem;
		background-color: #fff;
		border-top:1px solid #dcdcdc;
		position:relative;
		font-size:0.4rem;
		.address-icon{
			position:absolute;
			left:0.425rem;
			top:0.46rem;
			color:#6195c6;
		}
		.addicon{
			position:absolute;
			left:0.3125rem;
			top:50%;
			width:0.46875rem;
			height:0.625rem;
			margin-top:-0.3125rem;
		}
		.ad{
			margin-left:1.5625rem;
			padding-bottom:0.15625rem;
			margin-right:1.25rem;
		}
		.n{
			margin-top:0.2875rem;
		}
		.detail-icon{
			position:absolute;
			right:0.625rem;
			top:50%;
			margin-top:-0.234375rem;
			background:url(../images/1/trigon.png) no-repeat;
			background-size: 100% 100%;
			width:0.28125rem;
			height: 0.484375rem;			
		}
		.text{
			margin:0.19625rem 1.5rem 0 1.5rem;
			font-size:0.375rem;
		}
	}
	.pay{
		display: flex;
		height:1.15625rem;
		font-size:0.4325rem;
		background-color: #fff;
		border-top:1px solid #dcdcdc;	
		.text{
			width:7.5675rem;
			height:1.15625rem;
			line-height:1.15rem;
			text-indent: 0.3325rem;			
		}	
		.type1,.type2,{
			height:1.15625rem;
			line-height:1.15rem;	
			width:2.71875rem;	
			position: relative;
			text-indent: 0.4325rem;	
		}		
	}
	.warn{
		display: flex;
		font-size:0.4325rem;
		background-color: #fff;
		border-top:1px solid #dcdcdc;	
		padding-bottom: 0.3125rem;
		.left{
			width:20%;
			text-indent: 0.3325rem;	
			margin-top:0.15625rem;		
		}	
		.right{
			width:80%;	
			margin-top:0.15625rem;	
			position: relative;
		}			
	}
	.extraShow{
		display: flex;
		font-size:0.4325rem;
		background-color: #fff;
		border-top:1px solid #dcdcdc;	
		padding-bottom: 0.3125rem;
		margin-top:0.3125rem;
		.left{
			width:30%;
			text-indent: 0.3325rem;	
			margin-top:0.15625rem;		
		}	
		.right{
			width:70%;	
			margin-top:0.15625rem;	
			position: relative;
			.extra{
				width:90%;
				height: 1.25rem;
				text-indent: 0.3125rem;
				border-radius:0.15625rem;
			}
		}				
	}
	.submit{
		a.subtn{
			@include a(block,5.62rem,1.03rem,#fff,#51d164)
			line-height:1.03rem;
			font-size:0.46875rem;
			margin: 0 auto;
			border-radius:0.15625rem;
		}
		padding-bottom: 0.3125rem;
		background-color: #fff;
	}
}
.center{
	text-align:center;
}
.t-left{
	text-align:left;
}


