$gridPercentSwitch: true;
@import "tobe/function";
@import "base.scss";
@import "pre.scss";
@charset "UTF-8";

body, html {
    height: 100%;
}
body {
    font: 12px/1.5 "Microsoft YaHei", tahoma, arial, 'Hiragino Sans GB', '宋体', sans-serif;
}
$bgcolor: #2c51be;
$width: 10rem;
$placeholdercolor:#aaa9a9;
::-webkit-input-placeholder {
    color: $placeholdercolor
}
:-moz-placeholder {
    color:$placeholdercolor
}
::-moz-placeholder {
    color: $placeholdercolor
}
:-ms-input-placeholder {
    color: $placeholdercolor
}
.wrap-page {
    height: 100%;
    width: 10.0rem;
    margin: 0 auto;
}
.page {
    width: 10rem;
    height: 100%;
    overflow-x: hidden;
   position: relative;
}
%rel {
    position: relative;
}
%posa {
    position: absolute;
}
%colorf {
    color: #fff;
}
.text-center {
    text-align: center;
}
img, span, input {
    @include inline-block;
}
button {
    background-color: transparent;
    outline: none;
}

.page1{
       background: url(../images/bg1.jpg) no-repeat;
    background-size: 100% 100%; 
}
.page5{
    background: url(../images/bg5.png) no-repeat;
     background-size: 100% 100%;
