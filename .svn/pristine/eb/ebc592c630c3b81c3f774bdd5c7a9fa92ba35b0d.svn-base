%;\r\n                left: 50%;\r\n                transform: translate(-50%,-50%);\r\n                border: 30px solid #FFF;\r\n                background-size: cover;\r\n                background-repeat: no-repeat;\r\n                background-color: #CCC;\r\n            }\r\n            >.menu{\r\n                position: absolute;\r\n                bottom: 0px;\r\n                top:50%;\r\n                left: 50%;\r\n                width: 520px;\r\n                height: 285px;\r\n                // overflow: hidden;\r\n                // border-radius: 50%;\r\n                transform: translate(-50%,-50%);\r\n                >.items{\r\n                    // width: 520px;\r\n                    // height: 285px;\r\n                    >.item{\r\n                        position: absolute;\r\n                        width: 260px;\r\n                        height: 260px;\r\n                        left:0px;\r\n                        bottom: 125px;\r\n                        transform-origin: 100% 100%;\r\n                        overflow: hidden;\r\n                        cursor: pointer;\r\n                        background-color: rgb(185,185,185);\r\n                        &.active{\r\n                            background-color: rgb(70,205,215);\r\n                        }\r\n                        &:nth-child(1){\r\n                            transform: rotate(0deg) skew(40deg);\r\n                        }\r\n                        &:nth-child(2){\r\n                            transform: rotate(60deg) skew(30deg);\r\n                        }\r\n                        &:nth-child(3){\r\n                            transform: rotate(130deg) skew(40deg);\r\n                        }\r\n                        >span{\r\n                            // display: none;\r\n                            text-align: center;\r\n                            background-color: blue;\r\n                            width: 30px;\r\n                  