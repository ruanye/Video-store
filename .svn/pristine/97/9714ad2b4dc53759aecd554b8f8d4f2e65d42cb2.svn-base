<img src="images/close.png" alt="关闭" class="close" onclick="$('[class^=mask]').hide();">
<div class="extmenu">
    <div class="menu left" data-menuid="1" onmouseover="showExtMenu(1);" onmouseout="hideExtMenu(1);" data-id="<%= data.id %>">
        <p onclick="kaoqin(2);">迟到</p>
        <p onclick="kaoqin(4);">请假</p>
        <p onclick="kaoqin(6);">替签</p>
    </div>
</div>

<div class="content">
    <div class="menu" data-id="<%= data.id %>">
        <div class="items">
            <div class="item" onmouseover="showExtMenu(1);" onmouseout="hideExtMenu(1);"><span>考勤</span></div>
            <div class="item" onmouseover="showExtMenu(2);" onmouseout="hideExtMenu(2);"><span>评分</span></div>
            <div class="item" onclick="window.location.href='choujiang.html?id=<%=data.id%>'"onmouseover="showExtMenu(3);" onmouseout="hideExtMenu(3);"><span>抽奖</span></div>
        </div>
        <div class="tips">
            <span onmouseover="showExtMenu(1);" onmouseout="hideExtMenu(1);">考勤</span>
            <span onmouseover="showExtMenu(2);" onmouseout="hideExtMenu(2);">评分</span>
            <span onclick="window.location.href='choujiang.html?id=<%=data.id%>'" onmouseover="showExtMenu(3);" onmouseout="hideExtMenu(3);">抽奖</span>
        </div>
    </div>
    <img class="avatar"  src="<%=data.icon%>?imageView2/1/w/200/h/200" alt=""  onerror="noFindImg();">
</div>

<div class="extmenu" >
    <div class="menu right" data-menuid="2" onmouseover="showExtMenu(2);" onmouseout="hideExtMenu(2);" data-id="<%= data.id %>">
        <div class="stars" onmouseover="showStars(1);" onmouseout="hideStars(1);" onclick="stars(1);"></div>
        <div class="stars" onmouseover="showStars(2);" onmouseout="hideStars(2);" onclick="stars(2);"></div>
        <div class="stars" onmouseover="showStars(3);" onmouseout="hideStars(3);" onclick="stars(3);"></div>
        <div class="stars" onmouseover="showStars(4);" onmouseout="hideStars(4);" onclick="stars(4);"></div>
        <div class="stars" onmouseover="showStars(5);" onmouseout="hideStars(5);" onclick="stars(5);"></div>
    </div>
</div>

<div class="extmenu-footer">
    <div class="stu_name"><span>姓名：</span><span><%=data.name%></span></div>
    <div class="stu_no"><span>学号：</span><span><%=data.studentNo%></span></div>
</div>
