.fadeOut{
-webkit-animation:fadeOut 2s .5s ease both;
-moz-animation:fadeOut 2s .5s ease both;}
@-webkit-keyframes fadeOut{
0%{opacity:1;}
100%{opacity:0;}
}
@-moz-keyframes fadeOut{
0%{opacity:1;}
100%{opacity:0;}
}