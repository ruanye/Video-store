* {
  margin: 0;
  padding: 0;
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
  -webkit-appearance: none;
  font-family: "Microsoft YaHei",tahoma, arial, 'Hiragino Sans GB', '宋体', sans-serif; }

button {
  border: none; }

button:focus {
  outline: none; }

html,
body {
  height: 100%; }

.wrap-page {
  height: 100%;
  background-origin: content-box;
  -webkit-background-origin: content-box;
  -webkit-overflow-scrolling: touch;
  background: #fff;
  text-align: center;
  max-width: 760px;
  width: 100%;
  margin: auto; }

.page {
  margin: 0 auto;
  min-height: 100%;
  width: 100%;
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex;
  background: url(../images/bg_bottom.png) center bottom no-repeat;
  background-size: 100% auto;
  -webkit-flex-direction: column;
  -moz-flex-direction: column;
  flex-direction: column; }

.row:after,
.row:before {
  display: table;
  content: " ";
  clear: both; }

.flex-box {
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex; }

.flex-1 {
  -webkit-box-flex: 1;
  -ms-flex: 1;
  flex: 1; }

.flex-m {
  -webkit-box-pack: center;
  -ms-flex-pack: center;
  justify-content: center;
  -webkit-box-align: center;
  -ms-flex-align: center;
  align-items: center;
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex; }

.flex-d {
  -webkit-box-pack: center;
  -ms-flex-pack: center;
  justify-content: center;
  -webkit-box-align: center;
  -ms-flex-align: center;
  align-items: flex-end;
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex; }

