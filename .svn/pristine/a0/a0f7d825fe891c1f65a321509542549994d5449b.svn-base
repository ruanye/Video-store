.bounceInOpen {
    -webkit-transform-origin: center center;
    -moz-transform-origin: center center;
    -webkit-animation: bounceInOpen 1.5s .2s ease both;
    -moz-animation: bounceInOpen 1.5s .2s ease both;
}

@-webkit-keyframes bounceInOpen {
    0% {
        opacity: 0;
        width: 0;
    }
    20% {
        opacity: 1;
        width: 0;
    }
    40% {
        opacity: 1;
        width: 0;
    }
    60% {
        width: 100%;
    }
    80% {
        width: 70%;
    }
    100% {
        width: 100%;
    }
}
