@import "tobe/function";
@mixin font-dpr($fonts-size) {
    font-size: $fonts-size;
    [data-dpr="2"] & {
        font-size: $fonts-size * 2;
    }
    [data-dpr="3"] & {
        font-size: $fonts-size * 3;
    }
}

* {
    margin: 0;
    padding: 0;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
    -webkit-appearance: none;
    font-family: "Microsoft YaHei";
}

button {
    border: none;
}

button:focus {
    outline: none;
}

html,
body {
    height: 100%;
}

.wrap-page {
    height: 100%;
    background-origin: content-box;
    -webkit-background-origin: content-box;
    -webkit-overflow-scrolling: touch;
    background: #FFFFFF;
    background-size: 100% auto;
    text-align: center;
    max-width: 760px;
    width: 100%;
    margin: auto;
}

.page {
    min-height: 100%;
    width: 100%;
    display: flex;
    @include flex-direction;
}

.inner {
    width: 100%;
}

.flex-box {
    display: flex;
}

.flex-1 {
    flex: 1;
}

.flex-m {
    -webkit-box-pack: center;
    -ms-flex-pack: center;
    -webkit-justify-content: center;
    justify-content: center;
    -webkit-box-align: center;
    -ms-flex-align: center;
    -webkit-align-items: center;
    align-items: center;
    display: flex;
}

.flex-d {
    -webkit-box-pack: center;
    -ms-flex-pack: center;
    -webkit-justify-content: center;
    justify-content: center;
    -webkit-box-align: center;
    -ms-flex-align: center;
    -webkit-align-items: flex-end;
    align-items: flex-end;
    display: flex;
}

.ipt-type {
    @include font-dpr(20px);
    margin: 0 auto .390625rem;
    font-weight: 700;
    color: #000000;
}

.ipt {
    outline: none;
    width: 7.625rem;
    height: 1.578125rem;
    line-height: 0.826667rem;
    border-radius: none;
    border: 3px solid #152964;
    color: #152964;
    -webkit-appearance: none;
    background: #fff;
    text-align: center;
    @include font-dpr(16px);
}

input::-webkit-input-placeholder,
textarea::-webkit-input-placeholder {
    color: #152964;
}

input:-moz-placeholder,
textarea:-moz-placeholder {
    color: #152964;
}

input::-moz-placeholder,
textarea::-moz-placeholder {
    color: #152964;
}

input:-ms-input-placeholder,
textarea:-ms-input-placeholder {
    color: #000000;
}

.fixed-logo {
    width: 2.03125rem;
    position: absolute;
    top: 0.4375rem;
    right: 0.59375rem;
}

.row:after,
.row:before {
    display: table;
    content: " ";
    clear: both
}

.alert-mask {
    position: fixed;
    height: 100%;
    width: 100%;
    top: 0px;
    left: 0px;
    background-color: rgba(0, 0, 0, 0.5);
    z-index: 99999;
    .alert-modal {
        position: fixed;
        top: 40%;
        left: 50%;
        transform: translate(-50%, -50%);
        width: 280px;
        background-color: #ffffff;
        border-radius: 3px;
        overflow: hidden;
        .alert-title {
            font-size: 14px;
            height: 40px;
            line-height: 30px;
            text-align: center;
            padding: 10px;
        }
        .alert-body {
            font-size: 14px;
            height: 45px;
            text-align: center;
            border-bottom: 1px solid #cccccc;
        }
        .alert-footer {
            text-align: center;
            cursor: pointer;
            height: 40px;
            .alert-btn {
                width: 100%;
                height: 40px;
                line-height: 40px;
                padding: 0px;
                font-size: 14px;
                color: #ffffff;
                float: left;
                &:last-child {
                    border-left: 1px solid #cccccc;
                }
                &.btn-cancle {
                    color: #de0313;
                    font-weight: 500;
                    font-size: 16px;
                    cursor: pointer;
                }
            }
            &:after {
                content: '';
                display: block;
                height: 0;
                clear: both
            }
        }
    }
}

//confim
.confirm-mask {
    position: fixed;
    height: 100%;
    width: 100%;
    top: 0px;
    left: 0px;
    background-color: rgba(0, 0, 0, 0.5);
    z-index: 99999;
    .confirm-modal {
        position: fixed;
        top: 40%;
        left: 50%;
        transform: translate(-50%, -50%);
        width: 280px;
        background-color: #ffffff;
        border-radius: 3px;
        overflow: hidden;
        .confirm-title {
            font-size: 14px;
            height: 40px;
            line-height: 30px;
            text-align: center;
            padding: 10px;
        }
        .confirm-body {
            font-size: 14px;
            height: 45px;
            text-align: center;
            border-bottom: 1px solid #cccccc;
        }
        .confirm-footer {
            text-align: center;
            cursor: pointer;
            height: 40px;
            .confirm-btn {
                width: 49%;
                height: 40px;
                line-height: 40px;
                padding: 0px;
                font-size: 14px;
                color: #ffffff;
                float: left;
                &:last-child {
                    border-left: 1px solid #cccccc;
                }
                &.btn-cancle {
                    color: #000000;
                    font-weight: 500;
                    font-size: 16px;
                    cursor: pointer;
                }
                &.btn-submit {
                    color: #36B9BF;
                    font-weight: 500;
                    font-size: 16px;
                    cursor: pointer;
                    &:hover {
                        color: #de1313;
                    }
                }
            }
            &:after {
                content: '';
                display: block;
                height: 0;
                clear: both
            }
        }
    }
}

