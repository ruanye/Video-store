.alert-mask {
  position: fixed;
  height: 100%;
  width: 100%;
  top: 0px;
  left: 0px;
  background-color: rgba(0, 0, 0, 0.5);
  z-index: 1000; }

.alert-mask .alert-modal {
  position: fixed;
  top: 40%;
  left: 50%;
  -ms-transform: translate(-50%, -50%);
      transform: translate(-50%, -50%);
  -webkit-transform: translate(-50%, -50%);
  width: 280px;
  background-color: #ffffff;
  border-radius: 3px;
  overflow: hidden; }

.alert-mask .alert-modal .alert-title {
  font-size: 14px;
  height: 20px;
  line-height: 20px;
  text-align: center;
  padding: 10px;
  border-bottom: 1px solid #cccccc; }

.alert-mask .alert-modal .alert-body {
  font-size: 14px;
  height: 45px;
  padding: 15px;
  text-align: center;
  border-bottom: 1px solid #cccccc; }

.alert-mask .alert-modal .alert-footer {
  text-align: center;
  cursor: pointer;
  height: 40px; }

.alert-mask .alert-modal .alert-footer .alert-btn {
  width: 49%;
  height: 40px;
  line-height: 40px;
  padding: 0px;
  font-size: 14px;
  color: #ffffff;
  float: left; }

.alert-mask .alert-modal .alert-footer:after {
  content: '';
  display: block;
  height: 0;
  clear: both; }

.alert-mask .alert-modal .alert-footer .alert-btn:last-child {
  border-left: 1px solid #cccccc; }

.alert-mask .alert-modal .alert-footer .alert-btn.btn-cancle {
  color: #000000;
