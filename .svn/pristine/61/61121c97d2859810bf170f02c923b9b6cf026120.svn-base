.alert-mask {
    position: fixed;
    height: 100%;
    width: 100%;
    top: 0px;
    left: 0px;
    background-color: rgba(0, 0, 0, 0.5);
    z-index: 99999;
    .alert-modal {
        position: fixed;
        top: 40%;
        left: 50%;
        transform: translate(-50%, -50%);
        width: 280px;
        background-color: #ffffff;
        border-radius: 3px;
        overflow: hidden;
        .alert-title {
            font-size: 14px;
            height: 40px;
            line-height: 30px;
            text-align: center;
            padding: 10px;
        }
        .alert-body {
            font-size: 14px;
            height: 45px;
            text-align: center;
            border-bottom: 1px solid #cccccc;
        }
        .alert-footer {
            text-align: center;
            cursor: pointer;
            height: 40px;
            .alert-btn {
                width: 100%;
                height: 40px;
                line-height: 40px;
                padding: 0px;
                font-size: 14px;
                color: #ffffff;
                float: left;
                &:last-child {
                    border-left: 1px solid #cccccc;
                }
                &.btn-cancle {
                    color: #de0313;
                    font-weight: 500;
                    font-size:16px;
                    cursor: pointer;
                }
            }
            &:after {
                content: '';
                display: block;
                height: 0;
                clear: both
            }
        }
    }
}

//confim
.confirm-mask {
    position: fixed;
    height: 100%;
    width: 100%;
    top: 0px;
    left: 0px;
    background-color: rgba(0, 0, 0, 0.5);
    z-index: 99999;
    .confirm-modal {
        position: fixed;
        top: 40%;
        left: 50%;
        transform: translate(-50%, -50%);
        width: 280px;
        background-color: #ffffff;
        border-radius: 3px;
        overflow: hidden;
        .confirm-title {
            font-size: 14px;
            height: 40px;
            line-height: 30px;
            text-align: center;
            padding: 10px;
        }
        .confirm-body {
            font-size: 14px;
            height: 45px;
            text-align: center;
            border-bottom: 1px solid #cccccc;
        }
        .confirm-footer {
            text-align: center;
            cursor: pointer;
            height: 40px;
            .confirm-btn {
                width: 49%;
                height: 40px;
                line-height: 40px;
                padding: 0px;
                font-size: 14px;
                color: #ffffff;
                float: left;
                &:last-child {
                    border-left: 1px solid #cccccc;
                }
                &.btn-cancle {
                    color: #000000;
                    font-weight: 500;
                    font-size:16px;
                    cursor: pointer;
                }
                &.btn-submit {
                    color: #36B9BF;
                    font-weight: 500;
                    font-size:16px;
                    cursor: pointer;
                    &:hover {
                        color: #de1313;
                    }
                }
            }
            &:after {
                content: '';
                display: block;
                height: 0;
                clear: both
            }
        }
    }
}


