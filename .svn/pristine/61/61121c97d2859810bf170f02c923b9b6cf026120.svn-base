@import "tobe/function";
@mixin font-dpr($fonts-size) {
    font-size: $fonts-size;
    [data-dpr="2"] & {
        font-size: $fonts-size * 2;
    }
    [data-dpr="3"] & {
        font-size: $fonts-size * 3;
    }
}

* {
    margin: 0;
    padding: 0;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
    -webkit-appearance: none;
    font-family: "Microsoft YaHei";
}

button {
    border: none;
}

button:focus {
    outline: none;
}

html,
body {
    height: 100%;
    background: #1e4ea0;
}

.wrap-page {
    height: 100%;
    background-origin: content-box;
    -webkit-background-origin: content-box;
    -webkit-overflow-scrolling: touch;
    background: #1e4ea0;
    background-size: 100% auto;
    text-align: center;
    max-width: 760px;
    width: 100%;
    margin: auto;
}

.page {
    min-height: 100%;
    width: 100%;
    display: flex;
    @include flex-direction;
}

.inner {
    width: 100%;
}

.flex-box {
    display: flex;
}

.flex-1 {
    flex: 1;
}

.flex-m {
    -webkit-box-pack: center;
    -ms-flex-pack: center;
    -webkit-justify-content: center;
    justify-content: center;
    -webkit-box-align: center;
    -ms-flex-align: center;
    -webkit-align-items: center;
    align-items: center;
    display: flex;
}

.flex-d {
    -webkit-box-pack: center;
    -ms-flex-pack: center;
    -webkit-justify-content: center;
    justify-content: center;
    -webkit-box-align: center;
    -ms-flex-align: center;
    -webkit-align-items: flex-end;
    align-items: flex-end;
    display: flex;
}

.fixed-logo {
    width: 2.106667rem;
    position: fixed;
    top: 0.826667rem;
    left: 0.826667rem;
    z-index: 9;
}

.com-btn {
    padding-left: 0.390625rem;
    padding-right: 0.390625rem;
    height: 1.546667rem;
    line-height: 1.546667rem;
    border-radius: 0.2rem;
    color: #ffffff;
    font-weight: bold;
    -webkit-appearance: none;
    background: #db6a6a;
    border: 0;
    text-align: center;
    @include font-dpr(22px);
    border-radius: 0.15625rem;
    box-shadow: 0.2rem 0 #193070;
}

.com-btn1 {
    padding-left: 0.390625rem;
    padding-right: 0.390625rem;
    width: 6.506667rem;
    height: 1.04rem;
    line-height: 1.04rem;
    border-radius: 0;
    color: #ffffff;
    font-weight: bold;
    -webkit-appearance: none;
    background: #db6a6a;
    text-align: center;
    @include font-dpr(18px);
    border-radius: 0;
    position: relative;
    z-index: 9;
    &:after {
        content: '';
        width: 6.453333rem;
        height: 1rem;
        position: absolute;
        top: -4px;
        right: -4px;
        border-top: 2px dotted #000;
        border-right: 2px dotted #000;
        z-index: 9999;
    }
}

.line-btn {
    padding-left: 0.390625rem;
    padding-right: 0.390625rem;
    height: 1rem;
    line-height: 1rem;
    border-radius: 0;
    color: #ffffff;
    font-weight: bold;
    -webkit-appearance: none;
    background: none;
    border: 1px solid #fff;
    text-align: center;
    @include font-dpr(14px);
    border-radius: 0.15625rem;
}

.ipt {
    outline: none;
    width: 6.506667rem;
    height: 1.04rem;
    line-height: 1.04rem;
    color: #db3b3b;
    -webkit-appearance: none;
