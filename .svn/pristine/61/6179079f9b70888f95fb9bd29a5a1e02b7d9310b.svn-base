$gridPercentSwitch:true;
@import "tobe/function";
@import "base.scss";

@charset "UTF-8";
.wrap-page,
body,
html {
    height: 100%
}
body {
    font:12px/1.5 "Microsoft YaHei",tahoma,arial,'Hiragino Sans GB','宋体',sans-serif
}
.page{
    width:100%;
    height: 100%;
    overflow: hidden;
    position:relative;
}
.flex{
    display: flex;
}
.top-title{
	background-color:#11357C;
	height:0.875rem;
	line-height:0.875rem;
	text-align:center;
	color:#fff;
	font-size:0.41875rem;
}
@keyframes rotate{
	0%{
		transform:rotate(0deg)
	}	
	100%{
		transform:rotate(30deg)
	}
}

@keyframes leftRight{
	0%{
		transform:translate(0.46875rem,0)
	}		
	50%{
		transform:translate(0.46875rem,0)
	}	
	100%{
		transform:translate(-0.46875rem,0)
	}

}
@keyframes fromButtom{
	0%{
		transform:translate3d(0,100%,0)
	}		
	100%{
		transform:translate3d(0,0,0)
	}
}
@keyframes sacle{
	0%{
		transform:scale3d(0,0,0)
	}		
	100%{
		transform:scale3d(1,1,1)
	}
}
#page1{
	background:url(../images/page1-bg.png) no-repeat center center;
	background-size:100% 100%;
	.content{
		width:8.671875rem;
		height: 13.296875rem;
		background:url(../images/page1-1.png) no-repeat center center;
		background-size:100% 100%;
		margin: 0.25625rem auto;
		position:relative;
		.p1{
			position: absolute;
			font-size:1.2625rem;
			color:#fff;
			left:1.6rem;
		}
		.p2{
			padding-top:0.11875rem;
		}
		.p2,.p3{
			text-align:right;
			margin-right:0.925rem;
			color:#fff;
			font-size:0.56875rem;
		}
		.p4{
			position: absolute;
			bottom:0.15625rem;
			color:#fff;
			font-size:0.55rem;
			text-align: center;
			width:100%;
			text-indent: 0.725rem;
		}
		.p5{
			width:1.84375rem;
			height: 1.796875rem;
			position:absolute;
			top:7.125rem;
			left:50%;
			transform-origin: left bottom;
			// animation: rotate 2s infinite;
			img{
				width:100%;
				height: 100%;
			}
		}
		.cloud1{
			width:3.265625rem;
			height: 0.765625rem;
			position:absolute;
			bottom:1.18125rem;
			left:1.2825rem;
			// animation: leftRight 2s infinite;
			img{
				width:100%;
				height: 100%;
			}
		}		
		.cloud2{
			width:3.265625rem;
			height: 0.765625rem;
			position:absolute;
			bottom:1.18125rem;
			right:-0.46875rem;;
			img{
				width:100%;
				height: 100%;
			}
		}
	}
}

