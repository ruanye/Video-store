.choujiang {
    text-align: center;
    position: relative;
    z-index: 3;
    top: 2rem;
    .circle {
        width: 8.1875rem;
        height: 8.203125rem;
        -webkit-transform: rotate(0deg);
        transform: rotate(0deg);
        transition: all 3s cubic-bezier(.33, .45, 0, .84);
        &.an{
            -webkit-transform: rotate(1440deg);
             transform: rotate(1440deg);
           
         }
        &.angel{
            -webkit-transform: rotate(1570deg);
             transform: rotate(1570deg);
        } 
        &.fourzhe{
            -webkit-transform: rotate(1690deg);
             transform: rotate(1690deg);
        }
       
    }
    .zhizhen {
        position: absolute;
        width: 2.46875rem;
        height: 2.71875rem;
        top: 50%;
        left: 50%;
        margin-top: -1.35938rem;
        margin-left: -1.23438rem;

    }
}

