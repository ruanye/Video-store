$gridPercentSwitch:true;
@import "tobe/function";
@import "base.scss";
@import "pre.scss";
@charset "UTF-8";
body,
html {
    height: 100%;
}

body {
    font: 12px/1.5 "Microsoft YaHei", tahoma, arial, 'Hiragino Sans GB', '宋体', sans-serif;
}

.wrap-page {
    height: 100%;
    max-width: 10.0rem;
    margin: 0 auto;
    position:relative;
}

.page {
    height: 100%;
}
.icon{
	width:2.0rem;
	height:2.0rem;
	position:absolute;
	left:0.46875rem;
	top:0.46875rem;
	background:url(../images/icon.png) no-repeat;
	background-size:100% 100%;
}
#page1 {
    background: url(../images/page1-bg.jpg) center bottom no-repeat;
    background-size: 100% 100%;
    .quickClick{
    	width:5.90625rem;
    	height: 7.125rem;
  		margin:0 auto;
        padding-top:3.125rem;
		.back-icon{
            width:5.90625rem;
            height: 7.125rem;
            background:url(../images/qiang.png) no-repeat;
            background-size:100% 100%;			
		}
    }
    .click{
        width:6.875rem;
        height: 1.29375rem;
        margin:0.58125rem auto 0;
        .btn{
            @include a(block,100%,100%,#fff,#B5000D);
            line-height: 1.29375rem;
            font-size:0.56875rem;
        }
    }
}
#page2{
    background: url(../images/page2-bg.jpg) center center no-repeat;
    background-size: 100% 100%;	
    .content{
    	width:8.45rem;
    	height:8.35625rem;
    	background:#fff;
    	position: absolute;
    	left:50%;
    	margin-left:-4.22rem;
    	top:3rem;
    	.page2-icon{
    		width:6.546875rem;
    		height: 4.359375rem;
    		margin: 0 auto;
		     background: url(../images/page2-icon.png) center center no-repeat;
		    background-size: 100% 100%;	
    	}
    	.p1{
    		color:#B5000D;
    		text-align: center;
    		font-size: 0.5625rem;
    	}
    	.tel{
    		outline: none;
    		border:2px solid #FFD7D2;
    		display: block;
    		width:7.65625rem;
    		height: 1.16875rem;
    		margin: 0 auto;
    		text-align: center;
    		font-size:0.425rem;
    		margin-top:0.46875rem;
            background:#F1F1F1;
    	}
    	.getPot{
    		@include a(block,7.65625rem,1.46875rem,#fff,#B5000D);
    		line-height:1.46875rem;
    		font-size:0.5625rem;
    		margin: 0 auto;
    		margin-top:0.46875rem;
    	}
    }
}
#page3{
    background: url(../images/page3-bg.jpg) center center no-repeat;
    background-size: 100% 100%;	
    .content{
        z-index: 11;
    	position: relative;
    	border-top:1px solid transparent;
    	padding-top:2.48125rem;
    	p{
			text-align: center;
    	}
    	.p1{
    		font-size:0.5875rem;
    		color:#B5000D;
    	}
    	.p2{
    		font-size:0.4375rem;
            color:#E83821;
            span{
                font-weight: 600;
            }
    	}
    	.register{
    		@include a(block,7.15625rem,1.26875rem,#fff,#B5000D);
    		line-height:1.26875rem;
    		font-size:0.5625rem;
    		margin: 0 auto;
    	}
    	.page3-icon{
    		width:5.671875rem;
    		height: 5.359375rem;
    		margin:0 auto;
		    background: url(../images/content.png) center center no-repeat;
		    background-size: 100% 100%;	    		
    	}
    }	
    .bottom{
        position:absolute;
        bottom:0;
    	width:10rem;
    	height: 3.065625rem;
	    background: #FFD7D2; 
        display: flex;
        flex-direction:column;
        justify-content: center;
        p{
            text-align: center;
            font-size:0.49875rem;
            span{
                background:#B5000D; 
                color:#fff;
                font-size:0.6rem;
            }
        }
        .p2{
            font-weight: 700;
        }
    }
}
#page4{
    background: url(../images/page4-bg.jpg) center center no-repeat;
    background-size: 100% 100%;	
    .content{
    	position: relative;
    	border-top:1px solid transparent;
    	padding-top:2.48125rem;
    	p{
			text-align: center;
			
    	}
    	.p1{
    		font-size:0.5875rem;
    		color:#B5000D;
    	}
    	.p2{
    		font-size:0.4375rem;
            color:#E83821;
    	}
    	.share{
    		@include a(block,6.15625rem,1.06875rem,#fff,#B5000D);
    		line-height:1.06875rem;
    		font-size:0.5025rem;
    		margin: 0 auto;
    	}
    	.getmoney{
    		@include a(block,6.15625rem,1.06875rem,#fff,#B5000D);
    		line-height:1.06875rem;
    		font-size:0.5025rem;
    		margin: 0.15625rem auto 0;    		
    	}
    	.page3-icon{
    		width:5.671875rem;
    		height: 5.359375rem;
    		margin:0 auto;
		    background: url(../images/content.png) center center no-repeat;
		    background-size: 100% 100%;	    		
    	}
    }	
}
#page5{
	background: url(../images/page5-bg.jpg) center center no-repeat;
    background-size: 100% 100%;	
    padding-top:1px;
    .icon{
    	width:1.5625rem;
    	height: 1.5625rem;
    }
   	.content{
   		z-index:22;
   		width:10rem;
   		height: 12.0625rem;
		background: url(../images/page5-icon.png) center center no-repeat;
	    background-size: 8.203125rem 11.453125rem;
	    position:absolute;
	    p{
	    	color:#fff;
	    	text-indent:0.8125rem;
	    }
	    .p1{
	    	margin-top:3.5625rem;
	    	font-size:0.47875rem;
	    }	    
	    .p2{
	    	font-size:0.47875rem;
	    }
	    .box{
	    	margin-top:4.1625rem;
	    	transform: scale(0.9,0.9);
	    	.p3{
	    		margin-left:0.2125rem;
	    	}	    	
	    	.p4{
	    		margin-left:0.395rem;
	    	}	    	
	    	.p5{
	    		margin-left:0.8425rem;
	    	}	    	
	    	.p6{
	    		// margin-left:0.6425rem;
	    		text-indent:0.5rem;
	    	}
	    }
   	}
   	.bottom{
        position:absolute;
        bottom:0;
        width:10rem;
        height: 3.065625rem;
        background: #FFD7D2; 
        display: flex;
        flex-direction:column;
        justify-content: center;
        p{
            text-align: center;
            font-size:0.49875rem;
            span{
                background:#B5000D; 
                color:#fff;
                font-size:0.6rem;
            }
        }
        .p2{
            font-weight: 700;
        }	
   	}
}
#page6{
	background: url(../images/page6-bg.jpg) center center no-repeat;
    background-size: 100% 100%;	
    border-top:1px solid transparent;
    .content{
    	background:#fff;
    	width:8.63125rem;
    	margin:0 auto;
    	margin-top:1.5625rem;
    	padding-bottom: 0.56875rem;
    	.p1{
    		padding-top:0.66875rem;
    		font-size:0.525rem;
    		color:#B5000D;
    		text-align: center;
    	}    	
    	.p2{
    		font-size:0.7375rem;
    		color:#B5000D;
    		text-align: center;
    	}    	
    	.p3{
    		font-size:0.525rem;
    		color:#B5000D;
    		text-align: center;
    	}
    	.page6-icon{
    		width:7.046875rem;
    		height: 5.96875rem;
    		margin:0 auto;
			background: url(../images/page6-icon.png) center center no-repeat;
		    background-size: 100% 100%;	
    	}
    	.p4{
    		color:#E83821;
    		text-align: left;
            text-indent: 1.3125rem;
    	}    	
    	.p5{
    		color:#E83821;
    		text-align: left;
            text-indent: 1.3125rem;
    	}    	
    	.p6{
    		color:#E83821;
    		text-align: left;
            text-indent: 1.3125rem;
    	}
        .p7{
            text-align: left;
            color:#E83821;
            font-weight: 600;
            font-size:0.46875rem;
            text-indent: 1.3125rem;            
        }
    }	
}
#page7{
	background: url(../images/page7-bg.png) center center no-repeat;
    background-size: 100% 100%;	
    .content{
    	background:#E83821;
    	width:7.8125rem;
    	height:7.8125rem;
    	position:absolute;
    	left:50%;
    	margin-left:-3.9rem;
    	top:4rem;
    	.p1{
    		font-size:1.09375rem;
    		color:#fff;
    		margin-top:0.76875rem;
    		text-align: center;
    	}    	
    	.p2{
    		font-size:1.09375rem;
    		color:#fff;
    		text-align: center;
    	}    	
    	.p3{
    		font-size:1.29375rem;
    		color:#fff;
    		text-align: center;
    	}
    }	
}

@include max-height-screen(480px){
	#page1{
		.title{
			padding-top:2.425rem;
			p{
				font-size:0.7rem;
			}
		}
	}
	#page3{
		.content{
			padding-top:1.48125rem;
			.p1{
				font-size:0.5rem;
			}
    		.page3-icon{
	    		width:5.271875rem;
	    		height:4.659375rem;
    		}
		}
	}	
	#page4{
		.content{
			padding-top:1.48125rem;
			.p1{
				font-size:0.5rem;
			}
    		.page3-icon{
	    		width:5.271875rem;
	    		height:4.659375rem;
    		}
		}
	}
	#page5{
	   	.content{
	   		top:-1rem;
	   		transform: scale(0.85,0.85);

   		}		
	}
	#page6{
		.icon{
			width:1.5625rem;
			height: 1.5625rem;
		}
		.content{
			padding-bottom: 0;
			.p1{
				padding-top:0.26875rem;
				font-size:0.3rem;
			}
			.p2{
				font-size:0.5rem;
			}
			.p3{
				font-size:0.3rem;
			}
		}
	}
}
