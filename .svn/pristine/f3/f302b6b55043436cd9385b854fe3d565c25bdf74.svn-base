.share-line{
  position:relative;
  margin-left:8.2rem;
  margin-top: -13.3rem;
  height:12.8125rem;
  width:0.984375rem;
  z-index:109;
  .share-circle{
    margin-left: 0.4375rem;
    margin-bottom: -1.015625rem;
    width:1.015625rem;
    height:1.015625rem;
    border-radius:50%;
    background-color:#fff;
  }
}
.share-line::before{
    position:absolute;
    bottom:0;
    content:'';
    display:block;
    width:100%;
    height:0.078125rem;
    border-bottom:0.078125rem dashed #fff;
    -webkit-animation:v-line 1s .2s ease both;
    -moz-animation:v-line 1s .2s ease both;
}
.share-line::after{
    position:absolute;
    right: 0;
    content: '';
    display:block;
    width:0.078125rem;
    height:100%;
    border-right:0.078125rem dashed #fff;
    -webkit-animation:h-line 1s 1.2s ease both;
    -moz-animation:h-line 1s 1.2s ease both;
    }

