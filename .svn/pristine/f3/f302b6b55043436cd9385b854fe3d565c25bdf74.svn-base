@import "tobe/function";
@mixin font-dpr($fonts-size) {
    font-size: $fonts-size;
    [data-dpr="2"] & {
        font-size: $fonts-size * 2;
    }
    [data-dpr="3"] & {
        font-size: $fonts-size * 3;
    }
}

* {
    margin: 0;
    padding: 0;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
    -webkit-appearance: none;
    font-family:"Microsoft YaHei",tahoma, arial, 'Hiragino Sans GB', '宋体', sans-serif;
    line-height:1;
}

button {
    border: none;
}

button:focus {
    outline: none;
}

html,
body {
    height: 100%;
}

.wrap-page {
    height: 100%;
    background-origin: content-box;
    -webkit-background-origin: content-box;
    -webkit-overflow-scrolling: touch;
    background: #fff;
    background-size: 100% auto;
    text-align: center;
    width: 100%;
    max-width: 760px;
    margin: auto;
    color:#fff;
}

.page{
    position:relative;
    margin:0 auto;
    min-height: 100%;
    width: 10.0rem;
    display: flex;
    background: #78bac8 url(../images/bg.png) center 0.9375rem no-repeat;
    background-size:100% 12.96875rem;
    @include flex-direction;
    color:#fff;
}

.row:after,
.row:before {
    display: table;
    content: " ";
    clear: both;
}

.flex-box {
    display: flex;
}

.flex-1 {
    flex: 1;
}

.flex-m {
    -webkit-box-pack: center;
    -ms-flex-pack: center;
    -webkit-justify-content: center;
    justify-content: center;
    -webkit-box-align: center;
    -ms-flex-align: center;
    -webkit-align-items: center;
    align-items: center;
    display: flex;
}

.flex-d {
    -webkit-box-pack: center;
    -ms-flex-pack: center;
    -webkit-justify-content: center;
    justify-content: center;
    -webkit-box-align: center;
    -ms-flex-align: center;
    -webkit-align-items: flex-end;
    align-items: flex-end;
    display: flex;
}

input::-webkit-input-placeholder, textarea::-webkit-input-placeholder {
    color:#ddd;
    padding-left:1.484375rem;
}
input:-moz-placeholder, textarea:-moz-placeholder {
    color:#ddd;
    padding-left:1.484375rem;
}
input::-moz-placeholder, textarea::-moz-placeholder {
    color:#ddd;
    padding-left:1.484375rem;
}
input:-ms-input-placeholder, textarea:-ms-input-placeholder {
    color:#ddd;
    padding-left:1.484375rem;
}

