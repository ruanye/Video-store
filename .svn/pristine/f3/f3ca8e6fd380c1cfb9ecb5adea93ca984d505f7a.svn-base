    <script>
        $('#mmbtn').on('click', function(event) {
             $('.mask').show();
             $('.ziti').show();
            event.preventDefault();
             event.stopPropagation()

            });
        $('.ziti').on('click',  function(event) {
             $('.gif').show();
            event.preventDefault();
             event.stopPropagation()
          
        });
        $(document).on('click', function(event) {
             $('.mask').hide();
             $('.ziti').hide();
