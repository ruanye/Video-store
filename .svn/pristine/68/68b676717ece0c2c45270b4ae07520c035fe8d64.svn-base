var SET = {
    init:function() {
        $.init();
        $(".item-input").find("input")[2].value = sessionStorage.contactAddr || "";
        $(".item-input").find("input")[0].value = sessionStorage.contactName || "";
        $(".item-input").find("input")[1].value = sessionStorage.contactNumber || "";
    }
};

SET.init();

function returnPolicyYes() {
    sessionStorage.contactAddr = $(".item-input").find("input")[2].value;
    sessionStorage.contactName = $(".item-input").find("input")[0].value;
    sessionStorage.contactNumber = $(".item-input").find("input")[1].value;
    window.location.href = "policyYes.html";
}
