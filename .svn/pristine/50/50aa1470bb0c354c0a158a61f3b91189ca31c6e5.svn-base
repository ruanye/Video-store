$gridPercentSwitch: true;
@import "tobe/function";
@import "base.scss";
@import "pre.scss";
@charset "UTF-8";
body,
html {
    height: 100%;
}

body {
    font: 12px/1.5 "Microsoft YaHei", tahoma, arial, 'Hiragino Sans GB', '宋体', sans-serif;
}

$bgcolor: #f1f1f1;
.wrap-page {
    height: 100%;
    width: 10.0rem;
    margin: 0 auto;
    position: relative;
    background-color: #13a4ab;
}

.page {
    height: 100%;
    background-color: $bgcolor;
    position: relative;
}

.text-center {
    text-align: center;
}

$width: 100%;
img,
span,
input {
    @include inline-block;
}

.top {
    width: 10.0rem;
    height: 11.8125rem;
    background: url(../images/onepage.png) no-repeat;
    background-size: 100% 100%;
}

.page1 {
    .mbtn {
        width: 4.1875rem;
        height: 1.5rem;
        line-height: 1.5rem;
        background-color: red;
        color: #fff;
        font-size: 0.75rem;
        margin-top: 0.15625rem;
    }
    p {
        &.text {
            font-size: 0.5rem;
            margin-top: 0.390625rem;
        }
    }
}

.page2 {
    .zhuan {
        position: absolute;
        bottom: 0;
        right: 0;
        img {
            width: 9.015625rem;
            height: 12.828125rem;
        }
    }
}

.car {
    position: absolute;
    left: 0;
    bottom: 0;
    img {
        width: 6.140625rem;
        height: 3.140625rem;
    }
}

.inputbox {
    position: relative;
    top: 0.9375rem;
    margin: 0 auto;
    width: 8.28125rem;
    height: 1.40625rem;
    line-height: 1.40625rem;
    border: 1px solid #15aeb4;
    input,
    span {
        float: left;
    }
    input {
        width: 6.5625rem;
        height: 1.40625rem;
        border: none;
        font-size: 0.4375rem;
        text-indent: 0.3125rem;
    }
    span {
        width: 1.71875rem;
        height: 1.40625rem;
        background-color: #15aeb4;
        .sanjiao {
            width: 0.625rem;
            height: 0.625rem;
            margin-left: 0.46875rem;
            margin-top: 0.359375rem;
        }
    }
}

.choujiang {
    text-align: center;
    position: relative;
    z-index: 3;
    top: 2rem;
    .circle {
        width: 7.859375rem;
        height: 7.875rem;
        -webkit-transform: rotate(0deg);
        transform: rotate(0deg);
        transition: all 3s cubic-bezier(.33, .45, 0, .84);
        &.an{
            -webkit-transform: rotate(1462.5deg);
             transform: rotate(1462.5deg);
           
         }
       
    }
    .zhizhen {
        position: absolute;
        width: 2.703125rem;
        height: 3.265625rem;
        top: 50%;
        left: 50%;
        margin-top: -1.640625rem;
        margin-left: -1.34375rem;

    }
}

.shadow {
    position: absolute;
    width: 7.828125rem;
    height: 11.640625rem;
    right: 0;
    top: 3rem;
    z-index: 1;

}
.page4{
    .four{
        margin-top: 0.3125rem;
        img{
            width: 8.390625rem;
            height: 6.515625rem;
        }
         span{
            position: relative;
            width:6.5625rem;
            height: 0.625rem;
            border: 2px solid #15aeb4;
            color: #15aeb4;
            border-radius: 0.3125rem;
            bottom: 0.46875rem;
         }
    }
    .share{
         position: absolute;
         top: 0.1875rem;
         right: 0.3125rem;
        img{
            width: 0.8125rem;
            height: 2.25rem;
        }
    }
    .textn{
        margin-top: 1.5625rem;
        width: 5.0rem;
        height: 0.8125rem;
        line-height:  0.8125rem;
        border-radius: 0.40625rem;
        color: #fff;
        background-color: #15aeb4;
        font-size: 0.4375rem;
    }
    .bottom{
        height: 3.296875rem;
        img{
            width: 10.0rem;
            height: 3.296875rem;
        }
    }
    .title{
        text-align: center;
         img{
            position: relative;
            width: 2.25rem;
            height: 0.859375rem;
            top: 1.25rem;
         }
    }
    .btext{
        padding: 0.375rem 0;
        background-color: #0a545a;
        color: #fff;
        p{
            margin-left: 0.3125rem;
            font-size: 0.34375rem;
        }
    }
}




.animated {
    -webkit-animation-duration: 1s;
    animation-duration: 1s;
    -webkit-animation-fill-mode: both;
    animation-fill-mode: both
}
.animated.infinite {
    -webkit-animation-iteration-count: infinite;
    animation-iteration-count: infinite
}

@-webkit-keyframes bounce {
0%, 100%, 20%, 53%, 80% {
-webkit-transition-timing-function:cubic-bezier(0.215, .61, .355, 1);
transition-timing-function:cubic-bezier(0.215, .61, .355, 1);
-webkit-transform:translate3d(0, 0, 0);
transform:translate3d(0, 0, 0)
}
40%, 43% {
-webkit-transition-timing-function:cubic-bezier(0.755, .050, .855, .060);
transition-timing-function:cubic-bezier(0.755, .050, .855, .060);
-webkit-transform:translate3d(0, -30px, 0);
transform:translate3d(0, -30px, 0)
}
70% {
-webkit-transition-timing-function:cubic-bezier(0.755, .050, .855, .060);
transition-timing-function:cubic-bezier(0.755, .050, .855, .060);
-webkit-transform:translate3d(0, -15px, 0);
transform:translate3d(0, -15px, 0)
}
90% {
-webkit-transform:translate3d(0, -4px, 0);
transform:translate3d(0, -4px, 0)
}
}
@keyframes bounce {
0%, 100%, 20%, 53%, 80% {
-webkit-transition-timing-function:cubic-bezier(0.215, .61, .355, 1);
transition-timing-function:cubic-bezier(0.215, .61, .355, 1);
-webkit-transform:translate3d(0, 0, 0);
-ms-transform:translate3d(0, 0, 0);
transform:translate3d(0, 0, 0)
}
40%, 43% {
-webkit-transition-timing-function:cubic-bezier(0.755, .050, .855, .060);
transition-timing-function:cubic-bezier(0.755, .050, .855, .060);
-webkit-transform:translate3d(0, -30px, 0);
-ms-transform:translate3d(0, -30px, 0);
transform:translate3d(0, -30px, 0)
}
70% {
-webkit-transition-timing-function:cubic-bezier(0.755, .050, .855, .060);
transition-timing-function:cubic-bezier(0.755, .050, .855, .060);
-webkit-transform:translate3d(0, -15px, 0);
-ms-transform:translate3d(0, -15px, 0);
transform:translate3d(0, -15px, 0)
}
90% {
-webkit-transform:translate3d(0, -4px, 0);
-ms-transform:translate3d(0, -4px, 0);
transform:translate3d(0, -4px, 0)
}
}
.bounce {
    -webkit-animation-name: bounce;
    animation-name: bounce;
    -webkit-transform-origin: center bottom;
    -ms-transform-origin: center bottom;
    transform-origin: center bottom
}