$gridPercentSwitch:true;
@import "tobe/function";
@import "base.scss";
@charset "UTF-8";

html{
    background:#ffffff;
}
.w {
    width: 990px;
    margin: 0 auto;
}

#login {
    position: relative;
    width: 300px;
    margin: 25px 0 5px 0;
    height: 60px;
}

#content {
    border-top: 2px solid #de0313;
    background:#eeeeee;
}
/******** 注册页面 style **********/
.reg-wrap{
    position: relative;
    height: 720px;
    margin: 0 0 20px;
    z-index: 2;
}
.reg-title{
    height: 40px;
    line-height: 50px;
    text-align: right;
    padding-right:15px;
}
.reg-title a{
    color: #d51313;
}
.reg-form{
    background: #ffffff;
    min-height: 650px;
    border-top:2px solid #de1313; 
}
.regin-content{
    position: absolute;
    width: 990px;
    margin: 0 auto;
    min-height: 550px;
}
.regin-content:after{
    content: '';
    display: block;
    height: 0;
    clear: both
}
.regin-left{
    float: left;
    width: 800px;
    min-height: 470px;
}
.regin-ul{
    margin-top: 40px;
    margin-left: 80px;
    margin-bottom:40px; 
}
.msgs1{
    background-color: red;
}
.regin-ul .item{
    width: 720px;
    height: 40px;
    position:relative;
    margin-bottom: 28px;
}
.regin-span1{
    float: left;
    color: #aaaaaa;
    display: block;
    width: 90px;
    height: 40px;
    line-height: 40px;
    text-align: right;
    font-size: 12px;
    padding-right:10px;
}
.span1-ti{
    color: #d51313;
    padding-right: 5px;
}
.regin-span2{
    padding-left: 5px;
    float: left;
    display: block;
    height: 40px;
}
.regin-span2 input[type='text'],.regin-span2 input[type='password']{
    width: 276px;
    height: 16px;
    padding:10px;
    border: 1px solid #dddddd;
}
.regin-span2 input[type='text']:focus,.regin-span2 input[type='password']:focus{
    border-color:#7adb54;
}
.regin-span2 .input-phnoe{
    background: url("../images/phone.png") no-repeat 270px;
}
.regin-span2 .input-user{
    background: url("../images/user.png") no-repeat 270px;
}
.regin-span2 .input-email{
    background: url("../images/yx.png") no-repeat 270px;
}
.regin-span2 .input-passw{
    background: url("../images/lock.png") no-repeat 270px;
}
.regin-span2 .yz{
    width: 180px !important;
}
.regin-span2 .recodeagain{
    margin-left: 195px;

}
.regin-span2 .recodeagain span{
    color: #d51c24;

}
.regin-span2 .hqyz{
    cursor:pointer;
    width: 90px;
    height: 38px;
    line-height: 38px;
    border: 1px solid #dddddd;
    background-color: #f4f4f4;
    margin-top:-4px; 
    margin-left: 6px;
}
.regin-span2 .valid-code{
    width: 155px !important;
}
.regin-span2 .yzimg{
    float: right;
    width: 90px;
    height: 36px;
    text-align: center;
}
.regin-span2 .yzimg img{
    margin-left: 7px;
}
.regin-span2 .yzimg img:hover{
    cursor: pointer;
}

.regin-right{
    float: left;
    width: 190px;
    min-height: 475px;
}
.regin-right-2{
    position: relative;
    float: left;
    width: 300px;
    min-height: 475px;
}
.right-ul{
    margin-top: 40px;
    padding-top:10px;
    >li label{
        height: 16px;
        padding:10px;
        margin-top:5px; 
    }
}
.right-ul-2{
    position: relative;
    width: 300px;
    height: 30px;
    margin-top: 40px;

}
.regin-span2 .changecode{
    cursor:pointer;
    color: blue;
}
.regin-ul .item .focus-msg{
    display: block;
}

.regin-ul .item .error-msg{
    color: #f00 !important;
    background: #FFEBEB !important;
    border: 1px solid #ffbdbe !important;
    display: block !important;
}
.regin-ul .item .succeed-msg{
    border-color:#7adb54;
    color:#7adb54;
    display: block !important;
}
.regin-ul .item .succeed{
  background: url(../images/sucess.png) 273px no-repeat #ffffff;
  border-color:#7adb54;
}
.regin-ul .item .error-ico{background:url(../images/error.png) 273px no-repeat #ffffff;}
.input-error{
    color: #f00 !important;
    border: 1px solid #ffbdbe !important;
}

.regin-ul .item label{
    margin-left:5px;
    width: 300px;
    top:0;
    color: #999999;
    position: absolute;
    right: 0;
    border: 1px solid #dcdcdc;
    background-color: #f4f4f4;
    height: 36px;
    line-height: 36px;
    padding:0 5px;
    display: none;
}
.tip{
    margin-left:37px !important;
}
.hide{
    display: none;
}
.show{
    display: block;
}
.regin-now{
    text-align: center;
    margin-top: 25px;
}
.my-button{
    width: 300px;
    height: 42px;
    padding:10px;
    margin: 15px auto;
    text-align: center;
    border-radius:3px;
    font-size: 16px;
    display: block;
    color: #ffffff;
    background-color: #d51c24;
}
.my-button:hover{
    cursor: pointer;
}
.my-button:active{
    background-color: #de1313;
}
/******** 注册页面 style **********/

.footer {
    width: 990px;
    margin: 0 auto;
    text-align: center;
    font: 12px/150% Arial, Verdana, "宋体";
    color: #000000;
}

.footer .links a,
.footer .links span {
    margin: 0px 0px;
}

.footer .copyright {
    margin: 10px 0px;
}
