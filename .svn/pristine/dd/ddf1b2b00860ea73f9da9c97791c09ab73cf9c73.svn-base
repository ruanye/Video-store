$gridPercentSwitch:true;
@import "tobe/function";
@import "base.scss";
@import "pre.scss";
@charset "UTF-8";
body,
html {
    height: 100%;
}

body {
    font: 12px/1.5 "Microsoft YaHei", tahoma, arial, 'Hiragino Sans GB', '宋体', sans-serif;
}

.wrap-page {
    height: 100%;
    max-width: 10.0rem;
    margin: 0 auto;
    position: relative;
}

.page {
    height: 100%;
}

%rel {
    position: relative;
}

%posa {
    position: absolute;
}

%colorf {
    color: #fff;
}

.text-center {
    text-align: center;
}
.red{
    color: #e50012;
}

$width: 100%;
.page1 {
    @extend %rel;
    overflow-y: scroll;
    min-height: 500px;
    background-size: 100% 100%;
    background-repeat: no-repeat;
    background-position: center center;
    background-image: url(../images/bg1.png);
    .container {
        width: 100%;
        @extend %posa;
        margin-top: 4.6875rem;
        text-align: center;
        .text1 {
            @extend %colorf;
            font-size: 0.5rem;
        }
        input,
        .sure {
            margin-top: 0.46875rem;
        }
        input {
            width: 7.0625rem;
            height: 1.25rem;
            font-size: 0.5rem;
            line-height: 1.25rem;
        }
        .sure {
            background-color: #e50012;
            @include inline-block;
            width: 2.25rem;
            height: 0.8125rem;
            line-height: 0.8125rem;
            color: #fff;
            text-align: center;
            font-size: 0.5rem;
        }
    }
    .text2 {
        @extend %posa;
        @extend %colorf;
        width: $width;
        text-align: center;
        font-size: 0.375rem;
       bottom: 3.125rem;
    }
}

.page2 {
     background-color: #fbce33;
    @extend %rel;
    .text1,
    .text2,
    .text3,
    .text4 {
        @extend %colorf;
        @extend %posa;
        width: 100%;
    }
    .text1 {
        margin-top: 0.15625rem;
        font-size: 0.6rem;
       }
    .text2 {
        top: 4rem;
    }
    .text3 {
        width:7.96875rem;
        left: 50%;
        margin-left: -3.984375rem;
        top: 10rem;
        
        font-size: 0.3125rem;
        span{
            font-size: 0.1875rem;
        }
    }
    .text4 {
        bottom: 1.6rem;
          
    }
    .text2,
    .text3.text4 {
        font-size: 0.375rem;
    }
    .imghbox {
        width: 100%;
        z-index: 2;
        text-align: center;
        @extend %posa;
        top: 3rem;
        img {
            width: 1.3125rem;
            height: 5.328125rem;
            @include inline-block;
          
        }
    }
    .btn {
        @extend %posa;
        width: $width;
        top: 8.8rem;
        text-align: center;
        a {
            @include inline-block;
            width: 2.8125rem;
            height: 0.84375rem;
            line-height: 0.84375rem;
            background-color: #e50012;
            color: #fff;
            line-height: 0.84375rem;
            font-size: 0.5rem;
           
        }
    }
    .car {
        @extend %posa;
        width: 3.359375rem;
        height: 1.453125rem;
        left: 0;
        bottom: 0;
    }
}

.page3 {
     background-color: #fbce33;
    .text1 {
        @extend %colorf;
        @extend %posa;
        width: 100%;
        font-size: 0.53125rem;
        top: 2.84375rem;
    }
    .imghbox {
        @extend %posa;
        width: $width;
        text-align: center;
        top: 6.875rem;
        span {
            @include inline-block;
            color: #fff;
            width: 7.09375rem;
            height: 1.953125rem;
            line-height: 1.953125rem;
            font-size: 0.53125rem;
            background-image: url(../images/rebtn.png);
            background-size: 100% 100%;
        }
    }
    .text2 {
        @extend %colorf;
        @extend %posa;
        width: $width;
        top: 9.375rem;
        font-size: 0.5625rem;
        span {
           color: #e50012;
        }
    }
    .text3 {
        width: $width;
        @extend %colorf;
        font-size: 0.28125rem;
        @extend %posa;
        top: 10.625rem;
    }
    .bolang {
        @extend %posa;
        width: 100%;
        bottom: 0.15625rem;
        img {
            width: 100%;
            height: 2.875rem;
        }
    }
}

.page5 {
    background-color: #fbce33;
    .cloud {
        width: 4.0rem;
        height: 1.71875rem;
        @extend %posa;
        left: 1.09375rem;
        top: 1.03125rem;
    }
    .car {
        width: 1.359375rem;
        height: 0.40625rem;
        @extend %posa;
        right: 1.5rem;
        top: 4.578125rem;
    }
    .text1 {
        width: $width;
        @extend %colorf;
        @extend %posa;
        font-size: 0.75rem;
        top: 5.28125rem;
    }
    .btnbox {
        width: $width;
        text-align: center;
        @extend %posa;
        top: 6.5625rem;
        .btn {
            @include inline-block;
            width: 4.90625rem;
            height: 1.0625rem;
            line-height: 1.0625rem;
            background-color: #e60012;
            color: #fff;
            @include border-radius(8px);
            font-size: 0.5rem;
        }
    }
    .shou {
        @extend %posa;
        width: $width;
        text-align: center;
        top: 8.09375rem;
        img {
            @include inline-block;
            width: 1.734375rem;
            height: 5.046875rem;
        }
    }
    .bottom {
        @extend %posa;
        width: $width;
        height: 1.40625rem;
        text-align: right;
        bottom: 0;
        .logo {
           
            @include inline-block;
            width: 1.640625rem;
            height:1.40625rem;

        }
    }
}

.page7 {
     background-color: #fbce33;
    @extend %rel;
    .car {
        @extend %posa;
        width: 5.515625rem;
        height:1.6875rem;
        bottom: 0;
        right: 0.46875rem;
    }
    .share {
        @extend %posa;
        width: 4.4375rem;
        height: 1.734375rem;
        right: 0.703125rem;
        top: 0.46875rem;
    }
    .text1 {
        @extend %posa;
        @extend %colorf;
        font-size: 0.5625rem;
        left: 1.65625rem;
        top: 3.375rem;
        span {
            font-size: 0.9375rem;
            color: #e50012;
        }
    }
    .xiaou {
        width: 6.328125rem;
        height: 5.3125rem;
        @extend %posa;
        left: 1.65625rem;
        top: 6.6rem;
    }
}

.page6 {
    background-color: #fbce33;
    background-image: url('../images/bg6.jpg');
    background-size: 100% 100%;
    .textbox {
        @extend %posa;
        left: 3.34375rem;
        top: 47%;

    }
    .starbox {
        width: $width;
        text-align: center;
        @extend %posa;
        top: 0.8125rem;
        z-index: 500;
        .star {
            width: 8.21875rem;
            height: 10.953125rem;
        }
    }
    .text1 {
        text-align: left;
        @extend %colorf;
        font-size: 0.9375rem;
    }
    .text2 {
        @extend .text1;
        font-size: 0.375rem;
    }
    .imghbox {
     
        width: 3.34375rem;
        height:3.828125rem;
       
         background-image: url(../images/hb1.png);
        background-size: 100% 100%;
       

    }
}

.logobox {
    @extend %posa;
    width: $width;
    text-align: center;
    bottom: 0.9375rem;
    .logo {
        @include inline-block;
        width: 2.6875rem;
        height: 0.46875rem;
    }
}
.page2-3 .imghbox img{
    margin-top: 0.625rem;
    width: 4.046875rem;
    height: 2.59375rem;
}
.page2-4 .imghbox img{
    margin-top: 0.625rem;
    width: 4.859375rem;
    height: 4.09375rem;
}
.page2-5 .imghbox img{
    margin-top: 0.625rem;
    width: 6.71875rem;
    height: 2.484375rem;
}
img.j6{
    position: absolute;
    right:0.625rem;
    top: 3.125rem;
    z-index: 1;
    width: 0.953125rem;
   height:1.765625rem;
}

