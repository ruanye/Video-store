@import "/Users/mjr/tobe/function";
@mixin font-dpr($fonts-size){
    font-size: $fonts-size;

    [data-dpr="2"] & {
        font-size: $fonts-size * 2;
    }

    [data-dpr="3"] & {
        font-size: $fonts-size * 3;
    }
}

* {
  margin:0;padding:0;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
  -webkit-appearance:none;
  font-family: "Microsoft YaHei";
}
button{border: none;}
button:focus{outline: none;}
html, body{
  height: 100%;
}
.wrap-page {
    height: 100%;
    background-origin:content-box; 
    -webkit-background-origin:content-box;
  -webkit-overflow-scrolling: touch;
  background:#1e3788;
  background-image: -webkit-radial-gradient(#5dabff, #3157d6 50%, #25419f);
  background-image: -moz-radial-gradient(#5dabff, #3157d6 50%, #25419f);
  background-image: -o-radial-gradient(#5dabff, #3157d6 50%, #25419f);
  background-image: -ms-radial-gradient(#5dabff, #3157d6 50%, #25419f);
  background-image: radial-gradient(#5dabff, #3157d6 50%, #25419f);
  background-size: 100% auto;
  text-align: center;
  max-width: 760px;
width: 100%;
margin:auto;
}
.page{min-height:100%;width: 100%;display: flex;@include flex-direction;
  -webkit-animation:fadeInRightBig 1s .2s ease both;
-moz-animation:fadeInRightBig 1s .2s ease both;
}
.inner{width: 100%;
}
.flex-box{display: flex;}
.flex-1{flex: 1;}
.flex-m{-webkit-box-pack: center;
    -ms-flex-pack: center;
    -webkit-justify-content: center;
    justify-content: center;
    -webkit-box-align: center;
    -ms-flex-align: center;
    -webkit-align-items: center;
    align-items: center;
    display: flex;
  }
.flex-d{-webkit-box-pack: center;
    -ms-flex-pack: center;
    -webkit-justify-content: center;
    justify-content: center;
    -webkit-box-align: center;
    -ms-flex-align: center;
    -webkit-align-items: flex-end;
    align-items: flex-end;
    display: flex;
  }
.blue-btn{background: #2fc5cc;}
.red-btn{background: #fb1d1d;}
.com-btn {
  width: 3.540541rem;
  height: 1.837838rem;
  line-height: 1.837838rem;
  border-radius: 0.918919rem;
  color: #ffffff;
  font-weight: bold;
  -webkit-appearance: none;
 background-image: -webkit-linear-gradient(bottom, #fff, rgba(255,255,255,.3) 10%, rgba(255,255,255,0) 15%, rgba(255,255,255,0) 50%, rgba(255,255,255,0) 85%, rgba(255,255,255,.3) 90%, #fff);
  background-image: -moz-linear-gradient(bottom, #fff, rgba(255,255,255,.3) 10%, rgba(255,255,255,0) 15%, rgba(255,255,255,0) 50%, rgba(255,255,255,0) 85%, rgba(255,255,255,.3) 90%, #fff);
  background-image: -o-linear-gradient(bottom, #fff, rgba(255,255,255,.3) 10%, rgba(255,255,255,0) 15%, rgba(255,255,255,0) 50%, rgba(255,255,255,0) 85%, rgba(255,255,255,.3) 90%, #fff);
  background-image: -ms-linear-gradient(bottom, #fff, rgba(255,255,255,.3) 10%, rgba(255,255,255,0) 15%, rgba(255,255,255,0) 50%, rgba(255,255,255,0) 85%, rgba(255,255,255,.3) 90%, #fff);
  background-image: linear-gradient(to top, #fff, rgba(255,255,255,.3) 10%, rgba(255,255,255,0) 15%, rgba(255,255,255,0) 50%, rgba(255,255,255,0) 85%, rgba(255,255,255,.3) 90%, #fff);
  border: 2px solid rgba(0,0,0,.3);
  text-align: center;
  box-shadow:inset rgba(0,0,0,0.2) 0 0 .5em,inset rgba(255,254,255,0.6) 0 0.3em .3em, inset rgba(0,0,0,0.15) 0 -0.1em .3em, /* inner shadow */  /* color border */ rgba(0,0,0,0.5) 0.135135rem 0.135135rem 0.067568rem;
  @include font-dpr(32px);
 }
 .com-btn1 {
  width: 3.243243rem;
  height: 1.081081rem;
  line-height: 1.081081rem;
  border-radius: 0.135135rem;
  color: #ffffff;
  font-weight: bold;
  -webkit-appearance: none;
  border: 2px solid #000000;
  background: #2fc5cc;
  text-align: center;
  @include font-dpr(18px);
 }
.ipt{
    outline:none;
    width: 7.135135rem;
  height: 1.081081rem;
  line-height: 1.081081rem;
  border-radius: 0.135135rem;
    color: #2fc5cc;
    -webkit-appearance: none;background:#fff; padding:0.234375rem;  border: 2px solid #000000;text-align: center;@include font-dpr(18px);}
.fixed-logo{width: 3.581081rem;position: fixed;top:0.351351rem;left:0.351351rem;z-index: 9999;}
.fixed-logo1{width: 1.851351rem;position: fixed;top:0.459459rem;right:0.459459rem;z-index: 9999;}
.info-lg{@include font-dpr(24px);font-weight: bold;color: #fff}
.info-m{@include font-dpr(16px);font-weight: bold;color: #000}
.info-lg1{@include font-dpr(18px);font-weight: bold;color: #fff;line-height: 1.5;}
.info-s{@include font-dpr(14px);color: #fff}
.inner img{margin:auto;}
// mask
.mask{min-height:100%;width: 100%;display: flex;@include flex-direction;position: fixed;top:0;left:0;bottom:0;right:0;background: rgba(0,0,0,0.7);-webkit-animation:fadeIn 1s .2s ease both;
-moz-animation:fadeIn 1s .2s ease both;z-index: 99;}
// index-page
.p1{width: 8.959459rem}
.p7{width: 8.959459rem}
// mobile-page
.p5{
  width: 7.486486rem;
}
//result-page
.p2{width: 9.608108rem;}
.p3{width: 7.337838rem;}
.p4{width: 8.567568rem;}
//share-page
.share{position: fixed;right:0;top:0;width: 4.756757rem}
// 动效1

// 淡入
.fadeIn{
-webkit-animation:fadeIn 2s .7s ease both;
-moz-animation:fadeIn 2s .7s ease both;}
.fadeIn1{
-webkit-animation:fadeIn 2s 1s ease both;
-moz-animation:fadeIn 2s 1s ease both;}
@-webkit-keyframes fadeIn{
0%{opacity:0}
100%{opacity:1}
}
@-moz-keyframes fadeIn{
0%{opacity:0}
100%{opacity:1}
}

// 页面入场
@-webkit-keyframes fadeInRightBig{
0%{opacity:0;
-webkit-transform:translateX(2000px)}
100%{opacity:1;
-webkit-transform:translateX(0)}
}
@-moz-keyframes fadeInRightBig{ 
0%{opacity:0;
-moz-transform:translateX(2000px)}
100%{opacity:1;
-moz-transform:translateX(0)}
}

