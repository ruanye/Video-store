  to {
    -webkit-transform: translate3d(-800px, 0, 0);
    transform: translate3d(-800px, 0, 0); } }

@keyframes toLeft_1 {
  from {
    -webkit-transform: translate3d(0, 0, 0);
    transform: translate3d(0, 0, 0); }
  to {
    -webkit-transform: translate3d(-800px, 0, 0);
    transform: translate3d(-800px, 0, 0); } }

@-webkit-keyframes toLeft_2 {
  from {
    -webkit-transform: translate3d(0, 0, 0);
    transform: translate3d(0, 0, 0); }
  to {
    -webkit-transform: translate3d(-800px, 0, 0);
