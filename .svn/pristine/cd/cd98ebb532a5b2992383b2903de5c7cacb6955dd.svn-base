$gridPercentSwitch: true;
@import "tobe/function";
@import "base.scss";
@import "pre.scss";
@charset "UTF-8";

body, html {
    height: 100%;
}
body {
    font: 12px/1.5 "Microsoft YaHei", tahoma, arial, 'Hiragino Sans GB', '宋体', sans-serif;
}
$bgcolor: #8acbb7;
$width: 10rem;

.wrap-page {
    height: 100%;
    width: 10.0rem;
}
.page {
    width: 10rem;
    overflow-x: hidden;
    height: 100%;
    position: relative;
}
@keyframes toLeft_1{
    from{
        transform:translate3d(0,0,0)
    }
    to{
        transform: translate3d(-800px,0,0)
    }
}
@keyframes toLeft_2{
    from{
        transform:translate3d(0,0,0)
    }
    to{
        transform: translate3d(-800px,0,0)
    }
}
@keyframes toRight{
    from{
        transform:translate3d(-300px,0,0)
    }
    to{
        transform: translate3d(0,0,0)
    }
}
#page1{
    background-color:#13a4ab;
    .title{
        color:#fff;
        text-align:center;
        font-size:0.5rem;  
        padding-top:0.6125rem;      
        padding-bottom:0.4125rem;      
    }
    .p1,.p2{
        color:#fff;
        text-align:center;
        font-size:0.5rem;
    }
    .content{
        background:url(../images/center.png) no-repeat center center;
        background-size:100% 100%;
        width:9.046875rem;
        height: 8.4375rem;
        margin:0.3125rem auto;
        position:relative;
        .cloud{
            position:absolute;
            right:-200px;
        }
        .cloud-1{
            background:url(../images/y1.png) no-repeat center center;
            background-size:100% 100%;
            width:2.359375rem;
            height: 0.640625rem;
            animation: toLeft_1 12s linear infinite;
        }        
        .cloud-2{
            background:url(../images/y2.png) no-repeat center center;
            background-size:100% 100%;
            width:1.46875rem;
            height:0.390625rem;
            top:1.25rem;
            animation: toLeft_2 12s 4s linear infinite;

        }
        .car{
            width:4.859375rem;
            height: 1.28125rem;
            background:url(../images/car.png) no-repeat center center;
            background-size:100% 100%;
            position:absolute;
            top:3.725rem;
            left:0.78125rem;
            animation: toRight 0.5s ease-out;
        }
    }
    .button{
        width:5.46875rem;
        height: 1.25rem;
        border-radius:0.55625rem;
        background-color:#fff;
        margin: 0 auto;
        text-align:center;
        line-height:1.25rem;
        font-size:0.49rem;
        color:#13a4ab;
    }
}
#page2{
    background:#13a4ab url(../images/bg_2.png) no-repeat center center;    
    background-size:100% 100%;
    .title{
        color:#fff;
        text-align:center;
        font-size:0.5rem;  
        padding-top:0.6125rem;      
        padding-bottom:0.4125rem;      
    }
    .content{
        background:#13a4ab url(../images/center2.png) no-repeat center center;    
        background-size:100% 100%;  
        width:9.4375rem;
        height: 10.21875rem;
        margin:0 auto;
        position:relative;
        input{
            display: block;
            width:6.171875rem;
            height: 1.015625rem;
            text-align:center;
            font-size:0.625rem;
            position:absolute;
            left: 50%;
            transform: translate(-50%,0);
            top:2.65rem;
            border:none;
            outline: none;
        }   
        .button{
            width:2.5625rem;
            height: 0.9375rem;
            margin:0 auto;
            background:#035157;
            color:#fff;
            position:absolute;
            left: 50%;
            transform: translate(-50%,0);
            bottom:1.78125rem;
            text-align: center;
            line-height: 0.9375rem;
            font-size:0.625rem;
            border-radius:0.178125rem;
        }   
    }
}
