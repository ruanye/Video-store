$gridPercentSwitch:true;
@import "tobe/function";
@import "base.scss";

@charset "UTF-8";
.wrap-page,
body,
html {
    height: 100%
}
body {
    font:12px/1.5 "Microsoft YaHei",tahoma,arial,'Hiragino Sans GB','宋体',sans-serif
}
.wrap-page {
  height: 100%;
  background-origin: content-box;
  -webkit-background-origin: content-box;
  -webkit-overflow-scrolling: touch;
  background: #FFFFFF;
  background-size: 100% auto;
  text-align: center;
  max-width: 760px;
  width: 100%;
  margin: auto;
}

.page {
  min-height: 100%;
  width: 100%;
  display: flex;
  @include flex-direction;
}

.inner {
  width: 100%;
}

.flex{
    display: flex;
}
.top-title{
  background-color:#11357C;
  height:0.875rem;
  line-height:0.875rem;
  text-align:center;
  color:#fff;
  font-size:0.41875rem;
}
@keyframes rotate{
  0%{
    transform:rotate(0deg)
  } 
  100%{
    transform:rotate(30deg)
  }
}

@keyframes leftRight{
  0%{
    transform:translate(0.46875rem,0)
  }   
  50%{
    transform:translate(0.46875rem,0)
  } 
  100%{
    transform:translate(-0.46875rem,0)
  }

}
.flex-box{display: flex;}
.flex-1{flex: 1;width: 100%;}
.flex-m{-webkit-box-pack: center;
    -ms-flex-pack: center;
    -webkit-justify-content: center;
    justify-content: center;
    -webkit-box-align: center;
    -ms-flex-align: center;
    -webkit-align-items: center;
    align-items: center;
    display: flex;
  }
.flex-d{-webkit-box-pack: center;
    -ms-flex-pack: center;
    -webkit-justify-content: center;
    justify-content: center;
    -webkit-box-align: center;
    -ms-flex-align: center;
    -webkit-align-items: flex-end;
    align-items: flex-end;
    display: flex;
  }
.footer{
  position:relative;
  margin: 0 auto;
  padding: 0;
  width: 100%;
  height: 1.28125rem;
  background: url(../images/bottom.png) no-repeat bottom center;
  background-size: 100% auto;
}

/* indexPage */
.food{
  position: relative;
  margin-bottom: 2.6rem;
}
#indexPage{
  background: url(../images/top.png) no-repeat top center,#3aafb4 url(../images/bg.png) no-repeat center center;
  background-size: 100% auto,cover;
}
.logo{
  width: 7.1875rem;
  height: auto;
  margin:0 0 0.703125rem 1.71875rem;
}
.bg{
  margin-top: 0.4375rem;
  width: 4.546875rem;
  height: auto;
}
.text{
  font-size:0.653125rem;
  color: #fff;
  font-weight: 500;
  letter-spacing: 0.125rem;
}
.spe{
  font-size: 0.693125rem;
  color: #165155;
  font-weight: 600;
}
.phoneP{
  margin-top: 0.36875rem;
  font-size: 0.46875rem;
  color: #fff;
  font-weight: 500;
  letter-spacing: 0.046875rem;
}
.phone{
  display: block;
  width: 6.046875rem;
  height: 1.0625rem;
  margin: -0.078125rem auto 0.46875rem auto;
  border-radius: 1.3125rem;
  border: none;
  text-align: center;
  font-size: 0.625rem;
  font-weight: 700;
}
.okBtn{
  width: 2.34375rem;
  height: 1.09375rem;
  background: #165155;
  color: #fff;
  outline: none;
  font-size: 0.625rem;
  letter-spacing: 0.078125rem;
  border-radius: 1rem;
}
.p1,.p2,.p3,.p4,.p6,.p7,.p8{
  position: absolute;
}
.p6{
  width: 1.90625rem;
  height: auto;
  top: 2.8rem;
  left: .3rem;
}
.p4{
  width: 1.34375rem;
  height: auto;
  top: 2.6rem;
  right: 1.1rem;
}
.p1{
  width: 1.875rem;
  height: auto;
  top: 5.80625rem;
  left: 1.4225rem;
}
.p3{
  width: 2.171875rem;
  height: auto;
  top: 6.30625rem;
  right: .2rem;
}
.p2{
  width: 1.125rem;
  height: 0.96875rem;
  top: 6.80625rem;
  right: -.2rem;
}
.p7{
  width: 3.90625rem;
  height: auto;
  top: 10.6rem;
  left: 1.025rem;
}
.p8{
  width: 2.171875rem;
  height: auto;
  top: 9rem;
  right: 1.4625rem;
}
.car{
  position: absolute;
  width: 8.125rem;
  height: auto;
  top: -.1rem;
  left: 1.5rem;
}
/* resualtPage */

#resualtPage{
  background: url(../images/top.png) no-repeat top center,#3aafb4 url(../images/bg.png) no-repeat center center;
  background-size: 100% auto,cover;
}
.prize{
  width: 5.265625rem;
  height: 2.765625rem;
}
.load{
  width: 4.84375rem;
  height: 0.78125rem;
  background: #f3cb44;
  display: block;
  margin: 1.46875rem auto 0 auto;
  font-size:0.390625rem;
  color: #be622d;
  letter-spacing: 0.046875rem;
  font-weight: 600;
}


.bounceInDown{
  -webkit-animation:bounceInDown 1s ease both;
  -moz-animation:bounceInDown 1s ease both;}
  @-webkit-keyframes bounceInDown{
  0%{opacity:0;
  -webkit-transform:translateY(-2000px)}
  60%{opacity:1;
  -webkit-transform:translateY(30px)}
  80%{-webkit-transform:translateY(-10px)}
  100%{-webkit-transform:translateY(0)}
  }
  @-moz-keyframes bounceInDown{
  0%{opacity:0;
  -moz-transform:translateY(-2000px)}
  60%{opacity:1;
  -moz-transform:translateY(30px)}
  80%{-moz-transform:translateY(-10px)}
  100%{-moz-transform:translateY(0)}
}
.fadeInDownBig{
-webkit-animation:fadeInDownBig 1s 0s ease both;
-moz-animation:fadeInDownBig 1s 0s ease both;}
@-webkit-keyframes fadeInDownBig{
0%{opacity:0;
-webkit-transform:translateY(-2000px)}
100%{opacity:1;
-webkit-transform:translateY(0)}
}
@-moz-keyframes fadeInDownBig{
0%{opacity:0;
-moz-transform:translateY(-2000px)}
100%{opacity:1;
-moz-transform:translateY(0)}
}
.shake{
-webkit-animation:shake 1s .2s ease both;
-moz-animation:shake 1s .2s ease both;}
@-webkit-keyframes shake{
0%,100%{-webkit-transform:translateX(0);}
10%,30%,50%,70%, 90%{-webkit-transform:translateX(-10px);}
20%,40%,60%,80%{-webkit-transform:translateX(10px);}
}
@-moz-keyframes shake{
0%,100%{-moz-transform:translateX(0);}
10%,30%,50%,70%, 90%{-moz-transform:translateX(-10px);}
20%,40%,60%,80%{-moz-transform:translateX(10px);}
}

.fadeInLeftBig{
-webkit-animation:fadeInLeftBig 1s 1s ease both;
-moz-animation:fadeInLeftBig 1s 1s ease both;}
@-webkit-keyframes fadeInLeftBig{
0%{opacity:0;
-webkit-transform:translateX(-2000px)}
100%{opacity:1;
-webkit-transform:translateX(0)}
}
@-moz-keyframes fadeInLeftBig{
0%{opacity:0;
-moz-transform:translateX(-2000px)}
100%{opacity:1;
-moz-transform:translateX(0)}
}