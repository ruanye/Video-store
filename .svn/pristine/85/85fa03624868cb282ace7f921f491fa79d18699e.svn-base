        <link href="css/main.css" rel="stylesheet" media="screen">
        <link rel="stylesheet" href="css/mdialog.css">
        <link rel="stylesheet" href="http://chongqing.sinaimg.cn/mobilecssjs/swiper.min.css">
       
    </head>
    <body>
        <div class="wrap-page">
            <div class="swiper-container ">
                <div class="swiper-wrapper">
                    <div class="swiper-slide slide1">
                        <img class="animated hide logo" data-class="bounceIn" data-delay='1s' src="images/logo.png" alt="">
                        <img class="returns" src="images/returns.png" alt="">
                    </div>
                    <div class="swiper-slide slide2">
                        <img class="animated hide returns a b" data-class="lightSpeedIn" data-delay="2s" src="images/returns1.png" alt="">
                        <img class="b1" src="images/p2-1.png" alt="">
                    </div>
                    <div class="swiper-slide slide3">
                         <img class="animated hide returns a b" data-class="lightSpeedIn" data-delay="2s" src="images/returns1.png" alt="">
                        <img class="b1" src="images/pg3-1.png" alt="">
                    </div>  
                     <div class="swiper-slide slide4">
                         <img class="animated hide returns a b" data-class="lightSpeedIn" data-delay="2s" src="images/returns1.png" alt="">
                        <img class="b1 a" src="images/pg4-1.png" alt="">
                    </div>  
                     <div class="swiper-slide slide5">
                      <img class="animated hide returns a b" data-class="lightSpeedIn" data-delay=".5s" src="images/returns1.png" alt="">
                      <div class="text-center text"><img src="images/pg5-1.png" alt=""></div>
                       <div class="text-center text a"><img src="images/pg5-2.png" alt=""></div>
                       <div class="text-center text b"><img src="images/pg5-3.png" alt=""></div>
                        <div class="text-center text c"><input type="text" id="phoneNumber"></div> 
                        <div class="text-center text d"><img class="save-btn" src="images/page5-5.png" alt=""></div>
                    </div>
                   
                </div>
                <!-- Add Pagination -->
                <div class="swiper-pagination"></div>
            </div>
             <section class="page success hide">
               <img class="animated  returns a b" src="images/returns1.png" alt="">
               <img src="images/shareicon.png" alt="" class="share">
               <div class="text-center centerimg"><img src="images/pg6-2.png" alt=""></div>
            </section>
             <section class="page new-user hide">
               <img class="animated  returns a b" src="images/returns1.png" alt="">
               <img src="images/shareicon.png" alt="" class="share">
               <div class="text-center register"><img src="images/pg7-1.png" alt=""></div>
               <div class="text1 text-center"><img src="images/pg7-2.png" alt=""></div>
                <div class="fx text-center"><img src="images/pg7-3.png" alt=""></div>
            </section>
             <section class="page end hide">
               <img class="animated  returns a b" src="images/returns1.png" alt="">
              
               <div class="text-center centerimg"><img src="images/p8-1.png" alt=""></div>
            </section>
        </div>
        <script type="text/javascript" src="http://apps.bdimg.com/libs/jquery/2.1.4/jquery.min.js"></script>
         <script src="https://leancloud.cn/scripts/lib/av-0.5.1.min.js"></script>
        <script type="text/javascript" src="../../javascript/common.js"></script>
        <script type="text/javascript" src="../../javascript/AV.analytics.js"></script>
        <script type="text/javascript" src="../../javascript/event.js"></script>
        <script src="js/main.js"></script>
        <script src="js/mdialog.js"></script>
        <script src="http://chongqing.sinaimg.cn/mobilecssjs/swiper.min.js"></script>
         <script>
~(function() {
        var dom = {};
        dom.hasClass = function hasClass(curEle, strClass) {
            var reg = new RegExp("(^| +)" + strClass + "( +|$)");
            return reg.test(curEle.className);
        }
        dom.addClass = function addClass(curEle, strClass) {
            if (!this.hasClass(curEle, strClass)) {
                curEle.className += " " + strClass;
            }
        }
        dom.removeClass = function removeClass(curEle, strClass) {
            var reg = new RegExp("(^| +)" + strClass + "( +|$)", "g");
            if (this.hasClass(curEle, strClass)) {
                curEle.className = curEle.className.replace(reg, " ");
            }
        };

        var thisDiv = document.querySelectorAll('.swiper-slide')[0];
        var cur = thisDiv.querySelectorAll('.animated');
        for (var i = 0; i < cur.length; i++) {
            dom.removeClass(cur[i], 'hide');
            dom.addClass(cur[i], cur[i].dataset.class);
            cur[i].style.animationDelay = cur[i].dataset.delay;
        };
        window.dom = dom;
    }())

    var swiper = new Swiper('.swiper-container', {
        pagination: '.swiper-pagination',
        paginationClickable: true,
        direction: 'horizontal',
        effect: 'fade',
        onSlideChangeStart: function() {
            var thisDiv = swiper.slides[swiper.activeIndex]
            var cur = thisDiv.querySelectorAll('.animated');
            for (var i = 0; i < cur.length; i++) {
                dom.addClass(cur[i], 'hide');
                dom.removeClass(cur[i], cur[i].dataset.class);
            }
        },
        onSlideChangeEnd: function() {
            var thisDiv = swiper.slides[swiper.activeIndex]
            var cur = thisDiv.querySelectorAll('.animated');
            for (var i = 0; i < cur.length; i++) {
                dom.removeClass(cur[i], 'hide');
                dom.addClass(cur[i], cur[i].dataset.class)
                cur[i].style.animationDelay = cur[i].dataset.delay;

            }
        }
    });


 
    </script>
    </body>
</html>