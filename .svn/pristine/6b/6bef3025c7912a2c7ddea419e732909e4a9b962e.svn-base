/**
 * Custom checkboxes
 */
label.label-checkbox input[type=checkbox],
label.label-checkbox input[type=radio] {
  position: relative;
  margin-right: 10px;
  background-color: #FFF;
  border: 2px solid #aaa;
  border-radius: 0.15625rem;
  background-clip: padding-box;
  -webkit-appearance: none; }

label.label-checkbox input[type=checkbox] {
  width: 0.9375rem;
  height: 0.9375rem;
  background: #ffffff;
  display: inline-block;
  vertical-align: text-top; }

label.label-checkbox input[type="checkbox"] + label:before {
  display: inline-block;
  vertical-align: text-top; }

label.label-checkbox input[type="checkbox"] + label:active {
  background: #ffffff; }

label.label-checkbox input[type=checkbox]:checked:after,
label.label-checkbox input[type=radio]:checked:after {
  display: inline-block;
  font-family: icomoon;
  color: #a6ce38;
  font-size: .1875rem;
  position: absolute;
  top: 0;
  left: 0;
  width: 0.78125rem;
  height: 0.78125rem;
  line-height: 0.78125rem;
  background: #ffffff;
  text-align: center;
  font-weight: 700; }

label.label-checkbox input[type=checkbox]:checked,
label.label-checkbox input[type=radio]:checked {
  background-color: #fff;
  border: 2px solid #a6ce38; }

label.label-checkbox input[type=checkbox]:checked:after {
  content: '';
  background: url("../images/checked.png") center center no-repeat; }

label.label-checkbox input[type=checkbox]:focus,
label.label-checkbox input[type=checkbox]:hover,
label.label-checkbox input[type=radio]:focus,
label.label-checkbox input[type=radio]:hover {
  outline: 0; }

