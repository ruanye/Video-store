$gridPercentSwitch:true;
@import "tobe/function";
@import "base.scss";
@import "pre.scss";
@charset "UTF-8";
body,
html {
    height: 100%;
}

body {
    font: 12px/1.5 "Microsoft YaHei", tahoma, arial, 'Hiragino Sans GB', '宋体', sans-serif;
    background-color:#f8f8f8;
}
*{
	-webkit-tap-highlight-color:rgba(0,0,0,0);
}
.special{
	color:#ddd!important;
	border-color:#ddd!important;
}
.wrap-page {
	.order-list{
		.list-item{
			font-size:0.38625rem;
			margin-bottom:0.1525rem;
			padding-left:0.3125rem;
			padding-bottom:0.15625rem;
			position:relative;
			background-color:#fff;
			.order-btn{
				position: absolute;
				right:0.3125rem;
				top:0.2125rem;
				border:1px solid #57cf69;
				color:#57cf69;
				width:1.875rem;
				height: 0.625rem;
				border-radius:0.15625rem;
				text-align:center;
				line-height: 0.625rem;
			}
			.order-number{
				padding-top:0.3125rem;
			}
			.docutor-name,.price,.time{
				margin-top:0.2125rem;
			}
		}
	}
}


