    <script src="js/circles.js"></script>
    <script type="text/javascript" src="http://apps.bdimg.com/libs/jquery/2.1.4/jquery.min.js"></script>
    <script src="js/swiper.min.js"></script>
    <script>
    var page = document.querySelectorAll('.swiper-container');
    var doch = window.innerHeight;
    console.log(doch);
    for (var i = 0; i < page.length; i++) {
        page[i].style.height = doch + 'px';
    }
    var value = 80; //圆环变量；
    Circles.create({
        id: 'circles-1',
        percentage: 43,
        radius: getWidth(),
        width: 5,
        value: value,
        text: function(value) {
            return '<p id="top-info">过去一年<br>您的里程数超过</p><p id="percentage-num">' + value + '%</p><p id="bottom-info">的司机</p>';
        },
        colors: ['#88cbb7', '#dc2672'],
        duration: 400,
        styleText: false

    })

    function getWidth() {
        return document.getElementById('circles-1').offsetHeight / 2;
    }
    //加载百分比
    var per = 0;
    var timer = setInterval(function() {
        if (per < 100) {
            per += 10;
            $('.per1').html(per);

            if (per == 100) {
                $('.a1').removeClass('rotate');
                swiper.slideNext();
                $('page1').hide();
                setTimeout(function() {
                    $('.light').addClass('flash')
                }, 2000);
                clearInterval(timer);
            }
        }
    }, 500)

    ~(function() {
        var dom = {};
        dom.hasClass = function hasClass(curEle, strClass) {
            var reg = new RegExp("(^| +)" + strClass + "( +|$)");
            return reg.test(curEle.className);
        }
        dom.addClass = function addClass(curEle, strClass) {
            if (!this.hasClass(curEle, strClass)) {
                curEle.className += " " + strClass;
            }
        }
        dom.removeClass = function removeClass(curEle, strClass) {
            var reg = new RegExp("(^| +)" + strClass + "( +|$)", "g");
            if (this.hasClass(curEle, strClass)) {
                curEle.className = curEle.className.replace(reg, " ");
            }
        };

        var thisDiv = document.querySelectorAll('.swiper-slide')[0];
        var cur = thisDiv.querySelectorAll('.animated');
        for (var i = 0; i < cur.length; i++) {
            dom.removeClass(cur[i], 'hide');
            dom.addClass(cur[i], cur[i].dataset.class);
            cur[i].style.animationDelay = cur[i].dataset.delay;
        };
        window.dom = dom;
    }())

    var swiper = new Swiper('.swiper-container', {
        pagination: '.swiper-pagination',
        paginationClickable: true,
        direction: 'horizontal',
        effect: 'fade',
        onSlideChangeStart: function() {
            var thisDiv = swiper.slides[swiper.activeIndex];
               
            var cur = thisDiv.querySelectorAll('.animated');
            for (var i = 0; i < cur.length; i++) {
                dom.addClass(cur[i], 'hide');
                dom.removeClass(cur[i], cur[i].dataset.class);
            }
        },
        onSlideChangeEnd: function() {
            var thisDiv = swiper.slides[swiper.activeIndex];

            var cur = thisDiv.querySelectorAll('.animated');
            for (var i = 0; i < cur.length; i++) {
                dom.removeClass(cur[i], 'hide');
                dom.addClass(cur[i], cur[i].dataset.class)
                cur[i].style.animationDelay = cur[i].dataset.delay;

            }
        }
    });



    </script>
</body>

