l(../images/bg.png) center top no-repeat;\r\n  background-size: 100%;\r\n  >.tips{\r\n    width: 7rem;\r\n    position: absolute;\r\n    left: 50%;\r\n    bottom: 45%;\r\n    transform: translate(-50%,-50%);\r\n  }\r\n  >.tips2{\r\n    width: 6rem;\r\n    background: rgba(0,0,0,.7);\r\n    color: #FFF;\r\n    text-align: center;\r\n    padding: 0.3rem;\r\n    font-size: .3rem;\r\n    line-height: .5rem;\r\n    position: absolute;\r\n    left: 50%;\r\n    bottom: 50%;\r\n    transform: translate(-50%,-50%);\r\n  }\r\n  >.content{\r\n    position: absolute;\r\n    left: 50%;\r\n    bottom: 5%;\r\n    transform: translate(-50%,0%);\r\n  }\r\n  &.active{\r\n    display: block;\r\n  }\r\n}\r\n\r\n.page1{\r\n  >.content{\r\n    >.input{\r\n      padding: 0.7407407407407407rem 0;\r\n      background: url(../images/line1.png) center top no-repeat;\r\n      background-size: 100%;\r\n      text-align: center;\r\n      width: 9rem;\r\n      >input{\r\n        width: 7rem;\r\n        text-align: center;\r\n        border: 0;\r\n        background-color: #fdd23e;\r\n        color: #000;\r\n        padding: 0.09259259259259259rem;\r\n        font-size: 0.46296296296296297rem;\r\n      }\r\n    }\r\n    >.button{\r\n      padding: 0.8rem 0;\r\n      background: url(../images/line2.png) center top no-repeat;\r\n      background-size: 100%;\r\n      text-align: center;\r\n      width: 9rem;\r\n      >button{\r\n        // width: 4rem;\r\n        text-align: center;\r\n        border: 0.1111111111111111rem solid #FFF;\r\n        background-color: #fdd23e;\r\n        color: #b22b24;\r\n        padding: 0.1rem .8rem;\r\n        font-size: 0.5555555555555556rem;\r\n      }\r\n      >.link{\r\n        text-align: right;\r\n        margin-right: 2rem;\r\n        margin-top: .3rem;\r\n        color: #FFF;\r\n      }\r\n    }\r\n  }\r\n}\r\n\r\n.page2{\r\n  >.content{\r\n    background: url(../images/line.png) center bottom no-repeat;\r\n    margin-bottom: .8rem;\r\n    >.button{\r\n      padding: 0.7407407407407407rem 0;\r\n      background-size: 100%;\r\n      text-align: center;\r\n      width: 8rem;\r\n\r\n      >button{\r\n        // width: 4rem;\r\n        text-align: center;\r\n        border: 0.1111111111111111rem solid #FFF;\r\n        background-color: #fdd23e;\r\n        color: #b22b24;\r\n        padding: 0.2rem .5rem;\r\n        font-size: 0.5555555555555556rem;\r\n      }\r\n      >.text{\r\n        font-size: .533rem;\r\n        color: #FFF;\r\n        margin-top: .2rem;\r\n        &:last-child{\r\n          color:#fdd23e;\r\n          margin-top: 1rem;\r\n        }\r\n      }\r\n    }\r\n  }\r\n}\r\n.mask{\r\n  display: none;\r\n  position: fixed;\r\n  top:0;\r\n  left: 0;\r\n  height: 100%;\r\n  width: 100%;\r\n  background-color: rgba(0,0,0,.9);\r\n  &.active{\r\n    display: block;\r\n  }\r\n}\r\n"],"sourceRoot":"/source/"}