$gridPercentSwitch:true;
@import "tobe/function";
@import "base.scss";

@charset "UTF-8";
.wrap-page,
body,
html {
    height: 100%
}
body {
    font:12px/1.5 "Microsoft YaHei",tahoma,arial,'Hiragino Sans GB','宋体',sans-serif
}
.wrap-page {
  height: 100%;
  background-origin: content-box;
  -webkit-background-origin: content-box;
  -webkit-overflow-scrolling: touch;
  background: #FFFFFF;
  background-size: 100% auto;
  text-align: center;
  max-width: 760px;
  width: 100%;
  margin: auto;
}

.page {
  min-height: 100%;
  width: 100%;
  display: flex;
  @include flex-direction;
}

.inner {
  width: 100%;
}

.flex{
    display: flex;
}
.top-title{
  background-color:#11357C;
  height:0.875rem;
  line-height:0.875rem;
  text-align:center;
  color:#fff;
  font-size:0.41875rem;
}
@keyframes rotate{
  0%{
    transform:rotate(0deg)
  } 
  100%{
    transform:rotate(30deg)
  }
}

@keyframes leftRight{
  0%{
    transform:translate(0.46875rem,0)
  }   
  50%{
    transform:translate(0.46875rem,0)
  } 
  100%{
    transform:translate(-0.46875rem,0)
  }

}
.flex-box{display: flex;}
.flex-1{flex: 1;width: 100%;}
.flex-m{-webkit-box-pack: center;
    -ms-flex-pack: center;
    -webkit-justify-content: center;
    justify-content: center;
    -webkit-box-align: center;
    -ms-flex-align: center;
    -webkit-align-items: center;
    align-items: center;
    display: flex;
  }
.flex-d{-webkit-box-pack: center;
    -ms-flex-pack: center;
    -webkit-justify-content: center;
    justify-content: center;
    -webkit-box-align: center;
    -ms-flex-align: center;
    -webkit-align-items: flex-end;
    align-items: flex-end;
    display: flex;
  }



.content{
  position: relative;
}
#indexPage{
  background: url(../images/logo.png) no-repeat bottom right,#13a6ab url(../images/bg1.png) no-repeat center center;
  background-size: 4.28125rem 1.546875rem,100% auto;
}
#indexPage{
  .index{
  width: 10.0rem;
  height: 15.515625rem;
  margin: 0 auto;
  background: url(../images/b.png) no-repeat center center;
  background-size: 100% auto;
}
.okBtn{
  width: 5.03125rem;
  height: 1.375rem;
  position: absolute;
  bottom: 1.546875rem;
  left: 50%;
  margin-left: -2.515625rem;
  font-size: 0.5625rem;
  background: #fff;
  outline:none;
  color: #13a4ab;
  font-weight: 500;
  letter-spacing: 0.125rem;
  border-radius: 1.3125rem;
}
}


#phonePage{
  background: url(../images/logo.png) no-repeat bottom right,#13a6ab url(../images/bg1.png) no-repeat center center;
  background-size: 4.28125rem 1.546875rem,100% auto;
}
#phonePage{
  .phone{
  position: relative;
  width: 10.0rem;
  margin: 0 auto;
  height: 14.609375rem;
  background: url(../images/phone.png) no-repeat center center;
  background-size: 100% auto;
}
.phoneNum{
  width: 8.75rem;
  height: 1.46875rem;
  margin-left: -4.375rem;
  position: absolute;
  top: 2.7125rem;
  left: 50%;
  border: none;
  background: none;
  outline: none;
  text-align: center;
  font-size: 0.84375rem;
  font-weight: 700;
  letter-spacing: 0.078125rem;
}
.saveBtn{
  width: 2.6875rem;
  height: 1.0625rem;
  outline: none;
  position: absolute;
  top: 7.2rem;
  left: 50%;
  margin-left: -1.34375rem;
  background: #754e28;
  font-size: 0.546875rem;
  color:#fff;
  font-weight: 700;
  border-radius: 0.15625rem;
  outline: none;
  letter-spacing: 0.078125rem;
}
}



#prizePage{
  background: url(../images/logo.png) no-repeat bottom right,#13a6ab url(../images/bg1.png) no-repeat center center;
  background-size: 4.28125rem 1.546875rem,100% auto;
}

#prizePage{
  .prize{
    position: relative;
    width: 10.0rem;
    height: 15.515625rem;
    margin: 0 auto;
    background: url(../images/b2.png) no-repeat center center;
    background-size: 100% auto;
  }
  .cong{
    position: absolute;
    top: 2.9999rem;
    left: 2.05rem;
    font-size: 0.725rem;
    color: #fff;
    letter-spacing: 0.046875rem;
  }
  .cong2{
    width: 100%;
    position: absolute;
    top: 4.3rem;
    text-align: center;
    font-size: 0.76875rem;
    color: #13a4ab;
  }
  .cong3{
    width: 100%;
    position: absolute;
    top: 5.3rem;
    text-align: center;
    font-size: 0.3125rem;
    color: #13a4ab;
    letter-spacing: 0.046875rem;
  }
  .shareBtn{
    width: 7.125rem;
    height: 1.375rem;
    position: absolute;
    bottom: 1.8125rem;
    left: 50%;
    margin-left: -3.5625rem;
    font-size: 0.46875rem;
    background: #fff;
    outline:none;
    color: #13a4ab;
    letter-spacing: 0.03125rem;
    border-radius: 1.3125rem;
    &:after{
      content: "";
      width: .75rem;
      height: 0.1rem;
      position: absolute;
      background: #fff;
      bottom: 0.640625rem;
      backface-visibility:visible;
      transform-origin:left center;
      animation:scaleX 0.5s 0s 1 ease normal none ;
    };
    &:before{
      content: "";
      width: 0.1rem;
      height: 12.184375rem;
      position: absolute;
      bottom:  0.640625rem;
      right: -.6rem;
      background: #fff;
      backface-visibility:visible;
      transform-origin:center bottom;
      animation:scaleY 2s 0s 1 ease normal none ;
    };
  }
  @keyframes scaleX{
    0%{transform:scaleX(0);
    } 100%{
    transform:scaleX(.75rem);
    } 
  }
  @keyframes scaleY{
    0%{transform:scaleY(0);
    } 25%{
      transform:scaleY(0);
    }100%{
    transform:scaleY(12.184375rem);
    } 
  }
}


#overPage{
  background: url(../images/logo.png) no-repeat bottom right,#13a6ab url(../images/bg1.png) no-repeat center center;
  background-size: 4.28125rem 1.546875rem,100% auto;
}
#overPage{
  .over{
    position: relative;
    width: 10.0rem;
    height: 15.515625rem;
    margin: 0 auto;
    background: url(../images/b2.png) no-repeat center center;
    background-size: 100% auto;
  }
   .cong{
    position: absolute;
    top: 2.9999rem;
    left: 2.05rem;
    font-size: 0.725rem;
    color: #fff;
    letter-spacing: 0.046875rem;
  }
  .cong2{
    width: 100%;
    position: absolute;
    top: 4.3rem;
    text-align: center;
    font-size: 0.76875rem;
    color: #13a4ab;
  }
  .cong3{
    width: 100%;
    position: absolute;
    top: 5.3rem;
    text-align: center;
    font-size: 0.3125rem;
    color: #13a4ab;
    letter-spacing: 0.046875rem;
  }
  .bottom{
    width: 100%;
    text-align: center;
    position: absolute;
    bottom: 1.2875rem;
    font-size: 0.375rem;
    color: #fff;
    letter-spacing: 0.0625rem;
  }
}
/* 遮罩页样式 */
.mask{
  position: fixed;
  top:0;
  bottom: 0;
  left:0;
  right:0;
  background: rgba(0,0,0,0.7);
}


.fadeInUp{
-webkit-animation:fadeInUp 1s .2s ease both;
-moz-animation:fadeInUp 1s .2s ease both;}
@-webkit-keyframes fadeInUp{
0%{opacity:0;
-webkit-transform:translateY(20px)}
100%{opacity:1;
-webkit-transform:translateY(0)}
}
@-moz-keyframes fadeInUp{
0%{opacity:0;
-moz-transform:translateY(20px)}
100%{opacity:1;
-moz-transform:translateY(0)}
}