.alert-mask {
  position: fixed;
  height: 100%;
  width: 100%;
  top: 0px;
  left: 0px;
  background-color: rgba(0, 0, 0, 0.5); }

.alert-mask .alert-modal {
  position: fixed;
  top: 40%;
  left: 50%;
  transform: translate(-50%, -50%);
  width: 280px;
  background-color: #ffffff;
  border-radius: 3px;
  overflow: hidden; }

.alert-mask .alert-modal .alert-title {
  font-size: 14px;
  height: 20px;
  line-height: 20px;
  text-align: center;
  padding: 10px;
  border-bottom: 1px solid #cccccc; }

.alert-mask .alert-modal .alert-body {
