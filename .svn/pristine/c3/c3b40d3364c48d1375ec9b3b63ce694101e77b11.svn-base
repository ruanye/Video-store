@import "tobe/function";
@mixin font-dpr($fonts-size) {
    font-size: $fonts-size;
    [data-dpr="2"] & {
        font-size: $fonts-size * 2;
    }
    [data-dpr="3"] & {
        font-size: $fonts-size * 3;
    }
}

* {
    margin: 0;
    padding: 0;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
    -webkit-appearance: none;
    font-family: "Microsoft YaHei";
}

button {
    border: none;
}

button:focus {
    outline: none;
}

html,
body {
    height: 100%;
}

.wrap-page {
    height: 100%;
    background-origin: content-box;
    -webkit-background-origin: content-box;
    -webkit-overflow-scrolling: touch;
    background: #F1F1F1;
    background-size: 100% auto;
    text-align: center;
    max-width: 760px;
    width: 100%;
    margin: auto;
}

.page {
    min-height: 100%;
    width: 100%;
    display: flex;
    @include flex-direction;
}

.inner {
    width: 100%;
}

.flex-box {
    display: flex;
}

.flex-1 {
    flex: 1;
}

.flex-m {
    -webkit-box-pack: center;
    -ms-flex-pack: center;
    -webkit-justify-content: center;
    justify-content: center;
    -webkit-box-align: center;
    -ms-flex-align: center;
    -webkit-align-items: center;
    align-items: center;
    display: flex;
}

.flex-d {
    -webkit-box-pack: center;
    -ms-flex-pack: center;
    -webkit-justify-content: center;
    justify-content: center;
    -webkit-box-align: center;
    -ms-flex-align: center;
    -webkit-align-items: flex-end;
    align-items: flex-end;
    display: flex;
}

.ipt {
    outline: none;
    width: 6.573333rem;
    height: 1.093333rem;
    line-height: 1.093333rem;
    border-radius: 0;
    border: 4px solid #FFD7D2;
    color: #FFD7D2;
    -webkit-appearance: none;
    background: #fff;
    padding: 0.334375rem;
    text-align: center;
    @include font-dpr(16px);
    font-weight: 700;
}
input::-webkit-input-placeholder, textarea::-webkit-input-placeholder {
　color: #666;
}
input:-moz-placeholder, textarea:-moz-placeholder {
　color:#666;
}
input::-moz-placeholder, textarea::-moz-placeholder {
　color:#666;
}
input:-ms-input-placeholder, textarea:-ms-input-placeholder {
　color:#666;
}

.fixed-logo {
    width: 1.48rem;
    position: absolute;
    top: 0.786667rem;
    left: 0.733333rem;
}

.row:after,
.row:before {
    display: table;
    content: " ";
    clear: both
}

.btn {
    border: none;
    text-align: center;
    width: 5.986667rem;
    height: 1.013333rem;
    background: #B5000D;
    color: #ffffff;
    @include font-dpr(16px);
    margin-bottom: 0.2rem;
    &:last-child {}
}

.page1 {
    background: url(../images/b1.png)center top no-repeat, url(../images/car.png) center bottom no-repeat;
    background-size: 100% auto;
    .content {
        width: 7.266667rem;
        margin: 0 auto;
        .info-lg-title{
            color: #000000;
            @include font-dpr(52px);
            font-weight: 700;
        }
        .info-title{
            color: #C4372C;
            @include font-dpr(28px);
            font-weight: 700;
        }
        .btn-lg {
            position: relative;
            width: 6.893333rem;
            height: 1.12rem;
            line-height: 1.12rem;
            margin-bottom: 0.426667rem;
            background: #B5000D;
            text-align: center;
            @include font-dpr(20px);
            color: #ffffff;
            font-weight: 700;
            display: block;
            z-index: 1;
            &:before {
                position: absolute;
                top: -6px;
                right: -6px;
                content: "";
                border-top:2px dotted #000000;
                border-right:2px dotted #000000;
                width: 6.793333rem;
                height: 1rem;
                display: block;
                z-index: 3;
            }
            &:after {
                position: absolute;
                top: -2px;
                right: -2px;
                content: "";
                border-top:2px dotted #000000;
                border-right:2px dotted #000000;
                width: 6.693333rem;
                height: .9rem;
                display: block;
                z-index: 3;
            }
            &:nth-of-type(odd){
                margin-right:0.373333rem;
            }
            &:nth-of-type(even){
                margin-left:0.373333rem;
            }
        }
    }
}
