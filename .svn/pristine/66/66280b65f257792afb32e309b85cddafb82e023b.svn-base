  border-radius: 0.066667rem;
}

.row:after,
.row:before {
  display: table;
  content: " ";
  clear: both
}


.alert-mask {
  position: fixed;
  height: 100%;
  width: 100%;
  top: 0px;
  left: 0px;
  background-color: rgba(0, 0, 0, 0.5);
  z-index: 99999;
  .alert-modal {
    position: fixed;
    top: 40%;
    left: 50%;
    transform: translate(-50%, -50%);
    width: 280px;
    background-color: #ffffff;
    border-radius: 3px;
    overflow: hidden;
    .alert-title {
      font-size: 14px;
      height: 40px;
      line-height: 30px;
      text-align: center;
      padding: 10px;
    }
    .alert-body {
      font-size: 14px;
      height: 45px;
      text-align: center;
      border-bottom: 1px solid #cccccc;
    }
    .alert-footer {
      text-align: center;
      cursor: pointer;
      height: 40px;
      .alert-btn {
        width: 100%;
        height: 40px;
        line-height: 40px;
        padding: 0px;
        font-size: 14px;
        color: #ffffff;
        float: left;
        &:last-child {
          border-left: 1px solid #cccccc;
        }
        &.btn-cancle {
          color: #de0313;
          font-weight: 500;
          font-size:16px;
          cursor: pointer;
        }
      }
      &:after {
        content: '';
        display: block;
        height: 0;
        clear: both
      }
    }
  }
}

//confim
.confirm-mask {
  position: fixed;
  height: 100%;
  width: 100%;
  top: 0px;
  left: 0px;
  background-color: rgba(0, 0, 0, 0.5);
  z-index: 99999;
  .confirm-modal {
    position: fixed;
    top: 40%;
    left: 50%;
    transform: translate(-50%, -50%);
    width: 280px;
    background-color: #ffffff;
    border-radius: 3px;
    overflow: hidden;
    .confirm-title {
      font-size: 14px;
      height: 40px;
      line-height: 30px;
      text-align: center;
      padding: 10px;
    }
    .confirm-body {
      font-size: 14px;
      height: 45px;
      text-align: center;
      border-bottom: 1px solid #cccccc;
    }
    .confirm-footer {
      text-align: center;
      cursor: pointer;
      height: 40px;
      .confirm-btn {
        width: 49%;
        height: 40px;
        line-height: 40px;
        padding: 0px;
        font-size: 14px;
        color: #ffffff;
        float: left;
        &:last-child {
          border-left: 1px solid #cccccc;
        }
        &.btn-cancle {
          color: #000000;
          font-weight: 500;
          font-size:16px;
          cursor: pointer;
        }
        &.btn-submit {
          color: #36B9BF;
          font-weight: 500;
          font-size:16px;
          cursor: pointer;
          &:hover {
            color: #de1313;
          }
        }
      }
      &:after {
        content: '';
        display: block;
        height: 0;
        clear: both
      }
    }
  }
}

.page1 {
  background:#BCDFF3 url(../images/page-bg-stars.png) center bottom no-repeat;
  background-size:100% auto;
  .bg-bottom{
    width: 100%;
    background:url(../images/hr.png) center bottom no-repeat;
    background-size:100% auto;
  }
  .content {
    .top-content{
      position:absolute;
      width: 7.146667rem;
      height: 1.626667rem;
      text-align: center;
      top: 2.4rem;
      left: 50%;
      margin-left: -3.573333rem;
      padding:0.2rem 0;
      background:#ffffff;
      color: #000000;
      @include font-dpr(22px);
      border-top: 0.093333rem solid #E84809;
      border-left: 0.173333rem solid #E84809;
      border-top-left-radius: 0.506667rem;
      border-bottom-right-radius: 0.506667rem;
      .info-top-title{
        width: 7.146667rem;
        height: 1.626667rem;
        color: #000000;
        @include font-dpr(18px);
      }
      &:before{
        position:absolute;
        content: "";
        top: -2.5rem;
        right: 0.893333rem;
        width: 0.106667rem;
        height: 2.5rem;
        background:#ffffff;
        display: block;
      }
    }
    .ranking-content{
      position: absolute;
      width: 100%;
      height: 8.533333rem;
      margin: 0 auto;
      bottom:4.053333rem;
      .child-4{
        position:absolute;
        width:getPercent(4,16);
        display: block;
        float:left;
        bottom:0;
        text-align: center;
        &:nth-child(1){
          left: 0;
        }
        &:nth-child(2){
          left: getPercent(4,16);
        }
        &:nth-child(3){
          left: getPercent(4,16) * 2;
        }
        &:nth-child(4){
          left: getPercent(4,16) * 3;
        }
        .progress{
          position: absolute;
          left: 50%;
          bottom:0.053333rem;
          width: 1.226667rem;
          margin-left:-0.613333rem;
          .quantity{
            position: absolute;
            left: 50%;
            height: 1.5rem;
            width: 1.226667rem;
            text-align: center;
            top:-0.466667rem;
            margin-left:-0.653333rem;
            color: #62777C;
            @include font-dpr(14px);
            font-weight: 700;
          }
          &.roast-fish-bar{
            width: 1.226667rem;
            border-left:0.2rem solid #ffffff;
            background:#92B6F4; 
            animation:progressbar 2s .2s ease both;
          }
          &.powder-bar{
            width: 1.226667rem;
            border-left:0.2rem solid #ffffff;
            background:#D0EF7D; 
            animation:progressbar 2s .2s ease both;
          }
          &.hot-pot-bar{
            width: 1.226667rem;
            border-left:0.2rem solid #ffffff;
            background:#EF9875; 
            animation:progressbar 2s .2s ease both;
          }
          &.facet-bar{
            width: 1.226667rem;
            border-left:0.2rem solid #ffffff;
            background:#F2DD86; 
            animation:progressbar 2s .2s ease both;
          }
        }
      }
    }
  }
  .footer{
    position:relative;
    width: 100%;
    height: 3.453333rem;
    background: #000000;
    margin: 0 auto;
    padding:0.6rem 0.466667rem;
    .child-4{
      width:getPercent(4,16);
      height: 2.333333rem;
      display: block;
      float:left;
      .icon{
        width: 1.813333rem;
        height: 1.506667rem;
        display: block;
        vertical-align: middle;
        margin: 0.133333rem auto;
        
      }
      .btn-sm{
        width: 1.906667rem;
        height: 0.613333rem;
        line-height: 0.613333rem;
        padding: 0.066667rem 0.066667rem;
        text-align: center;
        background:#E84809;
        color: #ffffff;
        @include font-dpr(14px);
        border:none;
        border-radius:0.066667rem;
      }
    }
  }
}
