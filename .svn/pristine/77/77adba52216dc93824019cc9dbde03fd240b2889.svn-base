<!DOCTYPE html>
<html>

<head>
    <meta charset="UTF-8">
    <title>确认商品订单</title>
    <meta name="viewport" content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <script src="vendor/lib-flexible/build/flexible.js"></script>
    <link rel="stylesheet" href="css/weui/jquery-weui.css">
    <link rel="stylesheet" href="css/weui/weui.min.css">
    <link rel="stylesheet" href="css/shoporderdetail.css">
    <link href="css/selectaddress.css" rel="stylesheet" media="screen">
</head>

<body>
    <div class="onepage">
        <div class="Serial" id="address">
            <div class="address">
            </div>
        </div>
        <div class="weui_cells weui_cells_form" style="margin-top:0.15625rem">
            <div class="weui_cell">
                <div class="weui_cell_hd">
                    <label for="name" class="weui_label">支付方式</label>
                </div>
                <div class="weui_cell_bd weui_cell_primary">
                    <input class="weui_input pay" id="job" type="text" value="微信支付" readonly="">
                </div>
            </div>
        </div>
        <div class="Serial last olast">
            <h4>商品信息</h4>
            <div class="list">
                <!--     <div class="goods">
                <div class="gimg"><img src="images/shoplist/myshopimg.jpg" alt=""></div>
                <div class="gtext">
                    <p class="title">云南白药喷雾</p>
                    <p>
                        规格：120g装
                    </p>
                </div>
                <div class="gtext rtext">
                    <p class="title">¥19.90</p>
                    <p>
                        ×1
                    </p>
                </div>
            </div> -->
            </div>
            <h5 id="total"><span></span></h5>
            <div class="mark"></div>
            <p class="Remark"><span>备注：</span>
                <input type="text">
            </p>
        </div>
       
        <span id="keleyivisitorip" style="display: none"></span>
    </div>
     <div class="bottom">
            <div class="text">
                <p class="moneyi">商品金额<span class="one span1"></span></p>
                <p class="moneyi">运费<span class="span1">￥<span id="yunfei" style="margin:0">00.00</span></span>
                </p>
            </div>
            <div class="billing">
                <p class="allprice">合计<span></span></p>
                <p class="settlement" id="sureorder">提交订单</p>
            </div>
        </div>
    <!-- 地址切换 -->
    <div class="allcontainer twopage" style="display: none">
        <ul class="listcontainer">
        </ul>
        <div class="bottom" style="height: 1.09375rem;bottom:1.09375rem">
            <a id="hrefadd" href="">
             新增收货地址
                <span><i class="iconfont">&#xe60c;</i></span>
           </a>
        </div>
        <div class="bottom" style="height: 1.09375rem;">
         <a id="returnbtn" href=""  style="background-color:#40b952;color:#fff;">
             返回
            <span><i class="iconfont" style="color:#fff;">&#xe60c;</i></span>
         </a>
        </div>
    </div>
    <!-- 地址编辑或者新增 -->
    <div class="threepage" style="display:none">
        <div class="weui_cells weui_cells_form" style="margin-top: 0;">
            <div class="weui_cell">
                <div class="weui_cell_hd">
                    <label class="weui_label">收货人:</label>
                </div>
                <div class="weui_cell_bd weui_cell_primary">
                    <input class="weui_input" id="linkman" name="linkman" placeholder="收货人">
                </div>
            </div>
            <div class="weui_cell">
                <div class="weui_cell_hd">
                    <label class="weui_label">手机号码:</label>
                </div>
                <div class="weui_cell_bd weui_cell_primary">
                    <input id="linkphone" class="weui_input" name="linkphone" placeholder="手机号码">
                </div>
            </div>
            <div class="weui_cell">
                <div class="weui_cell_hd">
                    <label class="weui_label">详细地址:</label>
                </div>
                <div class="weui_cell_bd weui_cell_primary">
                    <input id="content" class="weui_input" name="content" placeholder="详细地址">
                </div>
            </div>
        </div>
        <div class='demos-content-padded' style="width: 100%;border-top:1px solid #dcdcdc;">
            <a href="javascript:0" class="weui_btn weui_btn_primary btnform" style="width: 60%;margin-top: 0.3125rem;margin-bottom: 0.3125rem;">保存</a>
        </div>
    </div>

    <script src="vendor/zepto/zepto.min.js"></script>

    <script type="text/javascript" src="http://res.wx.qq.com/open/js/jweixin-1.0.0.js"></script>
    <script type="text/javascript" src="js/static/ls.js"></script>
    <script type="text/javascript" src="js/static/ss.js"></script>
    <script type="text/javascript" src="js/static/base.js"></script>
    <script type="text/javascript" src="js/topBar.js"></script>
     
    <script type="text/javascript" src="http://tool.keleyi.com/ip/visitoriphost/"></script>
    <script type="text/javascript" src="js/static/parsec_weixin.js"></script>
    <script type="text/javascript" src="js/static/parsec_wxpay.js"></script>
    <script src="js/selectaddress.js"></script>
   
    <script src="js/sureorder.js"></script>
   
</body>

</html>
