$gridPercentSwitch: true;
@import "tobe/function";
@import "base.scss";
@import "pre.scss";
@charset "UTF-8";

body, html {
    height: 100%;
}
body {
    font: 12px/1.5 "Microsoft YaHei", tahoma, arial, 'Hiragino Sans GB', '宋体', sans-serif;
}
$bgcolor: #2c51be;
$width: 10rem;
::-webkit-input-placeholder {
    color: #fff
}
:-moz-placeholder {
    color: #fff
}
::-moz-placeholder {
    color: #fff
}
:-ms-input-placeholder {
    color: #fff
}
.wrap-page {
    height: 100%;
    width: 10.0rem;
    margin: 0 auto;
}
.page {
    width: 10rem;
    height: 100%;
   position: relative;
   background-color: #091129;
}
%rel {
    position: relative;
}
%posa {
    position: absolute;
}
%colorf {
    color: #fff;
}
.text-center {
    text-align: center;
}
img, span, input {
    @include inline-block;
}
button {
    background-color: transparent;
    outline: none;
}

.page1{
    background-image: url(../images/hulubo1.png);
    background-size: 100% 100%;
    background-repeat: no-repeat;
}
.page2{
    background-image: url(../images/huluobo2.png);
    background-size: 100% 100%;
    background-repeat: no-repeat;
}
.page3,.page4,.page10,.page6,.page7,.page8,.page9{
    background-image: url(../images/huluobo3.png);
    background-size: 100% 100%;
    background-repeat: no-repeat;
}
.btnbox{
    span{
        margin: 0.9375rem 0.40625rem 0.3125rem;
        width:3.9375rem;
        height:1.328125rem;
        img{
            width:100%;
            height:auto;
        }
    }
}
.logo{

    padding-top: 0.3125rem;
     height:0.5625rem; 
    img{
       width:3.984375rem;
        height:0.5625rem; 
        
    }
    
}
.one1{
    padding-top: 0.46875rem;
    img{
        width:4.953125rem;
        height:4.671875rem
    }
}
.one-text{
    padding-top: 0.3125rem;
    img{
        width:5.59375rem;
        height:4.53125rem;
    }
}
.two{
    img{
        width:6.515625rem;
        height:3.390625rem;
    }
    .o{
        margin-top: 2.234375rem;
    }
}
.hander{
    width:$width;
     position: absolute;
     bottom: 0;
     height:3.859375rem;
    img{
        width:1.3125rem;
        height:3.859375rem;
    }
}
.threetext{
    img{
        margin-top: 3.78125rem;
        width:8.65625rem;
        height:2.21875rem;
    }
}
.threetext2{
    img{
        margin-top: 0.484375rem;
        width:7.109375rem;
        height:1.109375rem;
    }
}
.lqbtn{
      width:$width;
     position: absolute;
     bottom: 0.46875rem;
    img{
        width:1.84375rem;
        height:2.515625rem;
     }
}
.inputbox{
    img{
        margin-top: 4.71875rem;
        width:6.25rem;
        height:0.984375rem;
    }
}
.input{
    margin-top: 0.3125rem;
    input{
        width:6.671875rem;
        height:1.109375rem;

        border: none;
        border-radius: 0.15625rem;
        background-image: url(../images/input.png);
        background-size: 100% 100%;
        background-repeat: no-repeat;
    }
}
.go{
    img{
        width:4.203125rem;
        height:1.203125rem;
        margin-top: 3.4375rem;
    }
}
.tipimg{
    img{
        margin-top: 0.625rem;
        width:7.9375rem;
        height:5.828125rem;
    }
}
.cli{
    text-align: right;
    img{
        width:5.796875rem;
        height:4.640625rem;
        margin-right: 1.03125rem;
        margin-top: 1.546875rem;

    }
}
.imgtextb{
    img{
        width:9.359375rem;
        height:1.265625rem;
        margin-top: 0.90625rem;
    }
}
.btnimg{
    display: table;
    height: 2.71875rem;
   margin-top: 0.9375rem;
    span{
        vertical-align: middle;
        display:table-cell;
        img{
            margin-left: 0.3125rem;
             width:1.5rem;
           height:1.484375rem;     
        }
    }
    .oimg{

       
           width:7.578125rem;
           height:2.71875rem;     
    }
}