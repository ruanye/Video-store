//动效
.tada{right: 0.34375rem;
-webkit-animation:tada 1s .2s ease both;
-moz-animation:tada 1s .2s ease both;}
@-webkit-keyframes tada{
0%{-webkit-transform:scale(1)}
10%,20%{-webkit-transform:scale(1.3) rotate(-3deg)}
30%,50%,70%,90%{-webkit-transform:scale(1.1) rotate(3deg)}
40%,60%,80%{-webkit-transform:scale(1.1) rotate(-3deg)}
100%{-webkit-transform:scale(1) rotate(0)}
}
@-moz-keyframes tada{
0%{-moz-transform:scale(1)}
10%,20%{-moz-transform:scale(1.3) rotate(-3deg)}
30%,50%,70%,90%{-moz-transform:scale(1.1) rotate(3deg)}
40%,60%,80%{-moz-transform:scale(1.1) rotate(-3deg)}
100%{-moz-transform:scale(1) rotate(0)}
}
//闪烁位移
.flash{
-webkit-animation:flash 1.5s .2s ease both infinite;
-moz-animation:flash 1.5s .2s ease both infinite;}
@-webkit-keyframes flash{
0%,50%,100%{opacity: 1;}
25%,75%{opacity: 0;}
}
@-moz-keyframes flash{
0%,50%,100%{opacity: 1;}
25%,75%{opacity: 0;}
}
//长度变长
.long{
-webkit-animation:long 1s .2s ease both;}
@-webkit-keyframes long{
0%{-webkit-transform-origin:top center;-webkit-transform:scaleY(0)}
100%{-webkit-transform-origin:top center;-webkit-transform:scale(1)}
}
//step
@keyframes setp {
    from { background-position: left bottom, left bottom; }
    to { background-position: right bottom, left bottom; }
}
@keyframes setp1 {
    from { background-position: left 90%, left 90%; }
    to { background-position: right 90%, left 90%; }
}
//淡入
.fadeIn{
-webkit-animation:fadeInOut 1s .2s ease both infinite;
-moz-animation:fadeInOut 1s .2s ease both infinite;}
@-webkit-keyframes fadeInOut{
0%{opacity:0}
50%{opacity:1}
100%{opacity:0}
}

//X轴翻转
.flipInX{
-webkit-animation:flipInX 1s 1.2s ease both;
-moz-animation:flipInX 1s 1.2s ease both;}
@-webkit-keyframes flipInX{
0%{-webkit-transform:perspective(400px) rotateX(90deg);
opacity:0}
40%{-webkit-transform:perspective(400px) rotateX(-10deg)}
70%{-webkit-transform:perspective(400px) rotateX(10deg)}
100%{-webkit-transform:perspective(400px) rotateX(0deg);
opacity:1}
}
@-moz-keyframes flipInX{
0%{-moz-transform:perspective(400px) rotateX(90deg);
opacity:0}
40%{-moz-transform:perspective(400px) rotateX(-10deg)}
70%{-moz-transform:perspective(400px) rotateX(10deg)}
100%{-moz-transform:perspective(400px) rotateX(0deg);
opacity:1}
}


@-webkit-keyframes rotateIn{
0%{-webkit-transform-origin:center center;
-webkit-transform:rotate(0deg);
}
100%{-webkit-transform-origin:center center;
-webkit-transform:rotate(360deg);
}
}
@-moz-keyframes rotateIn{
0%{-moz-transform-origin:center center;
-moz-transform:rotate(0deg);
}
100%{-moz-transform-origin:center center;
-moz-transform:rotate(360deg);
}
}

@-webkit-keyframes rotateIna{
0%{-webkit-transform-origin:center center;
-webkit-transform:rotate(0deg);
}
100%{-webkit-transform-origin:center center;
-webkit-transform:rotate(-180deg);
}
}
@-moz-keyframes rotateIna{
0%{-moz-transform-origin:center center;
-moz-transform:rotate(0deg);
}
100%{-moz-transform-origin:center center;
-moz-transform:rotate(-180deg);
}
}