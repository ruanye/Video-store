@import "tobe/function";
@mixin font-dpr($fonts-size) {
    font-size: $fonts-size;
    [data-dpr="2"] & {
        font-size: $fonts-size * 2;
    }
    [data-dpr="3"] & {
        font-size: $fonts-size * 3;
    }
}

* {
    margin: 0;
    padding: 0;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
    -webkit-appearance: none;
    font-family: "Microsoft YaHei";
}

button {
    border: none;
}

button:focus {
    outline: none;
}

html,
body {
    height: 100%;
    position: relative;
   background: #0b1223;
}

:-moz-placeholder {
    /* Mozilla Firefox 4 to 18 */
    color: #2ec4cb;
}

::-moz-placeholder {
    /* Mozilla Firefox 19+ */
    color:#2ec4cb;
}

input:-ms-input-placeholder,
textarea:-ms-input-placeholder {
    color:#2ec4cb;
}

input::-webkit-input-placeholder,
textarea::-webkit-input-placeholder {
    color:#2ec4cb;
}

.wrap-page {
    height: 100%;
    background-origin: content-box;
    -webkit-background-origin: content-box;
    -webkit-overflow-scrolling: touch;
    text-align: center;
    max-width: 760px;
    width: 100%;
    margin: auto;
    position: relative;
}

.page {
    min-height: 100%;
    width: 100%;
    display: flex;
     -webkit-box-orient:vertical;
    @include flex-direction;
    position: relative;
    background:#0b1223 url(../images/b1.png) no-repeat top center;background-size: 100% auto;
}
.header{height: 0.933333rem;background: #1e3788;position: absolute;top:0;left:0;right:0;img{height: 0.933333rem;margin:auto;}}
.footer{height: 0.933333rem;background: #1e3788;position: absolute;bottom:0;left:0;right:0;}
.inner {
    width: 100%;
}

.flex-box {
    display: flex;
}

.flex-1 {
    flex: 1;
}

.flex-m {
    -webkit-box-pack: center;
    -ms-flex-pack: center;
    -webkit-justify-content: center;
    justify-content: center;
    -webkit-box-align: center;
    -ms-flex-align: center;
    -webkit-align-items: center;
    align-items: center;
    display: flex;
}

.flex-d {
    -webkit-box-pack: center;
    -ms-flex-pack: center;
    -webkit-justify-content: center;
    justify-content: center;
    -webkit-box-align: center;
    -ms-flex-align: center;
    -webkit-align-items: flex-end;
    align-items: flex-end;
    display: flex;
}

.com-btn {
    min-width: 5.946667rem;
    height: 1.6rem;
    line-height: 1.6rem;
    border-radius: 0.133333rem;
    color: #fff;
    font-weight: bold;
    -webkit-appearance: none;
    border: none;
    background: #d63318;
    text-align: center;
    @include font-dpr(30px);
    margin: 0.2rem auto;
    position: relative;
    letter-spacing:2px;
}

.ipt {
    margin: 0.2rem auto;
    outline: none;
    width: 6.796875rem;
    height: 0.828125rem;
    line-height: 0.828125rem;
    border-radius: 0;
    border:0;
    color: #2ec4cb;
    -webkit-appearance: none;
    background: none;
    padding: 0.234375rem;
    text-align: center;
    @include font-dpr(18px);
    letter-spacing: 2px;

}
.clear{visibility: hidden;height: 1;clear: both;}
.info-lg{@include font-dpr(28px);color: #fff;letter-spacing:2px}
.info-xm{@include font-dpr(24px);color: #fff;letter-spacing:2px}
.info-m{@include font-dpr(14px);color: #fff;letter-spacing:2px}
.info-s{@include font-dpr(12px);color: #fff;}
.fixed-logo{position: absolute;width: 1.813333rem;top:0.666667rem;left:0.666667rem}
.fixed-bottom{position: absolute;height: 1.40625rem;background:url(../images/b2.png) no-repeat top center;background-size: 100% auto;width: 100%;bottom: 0;left:0;right:0;
    img{width: 3.0625rem;margin:0.3125rem auto auto auto;}
}
  

