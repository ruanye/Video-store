function checkNumberAndLetterInput(m_this, info) {
    if ($(m_this).val() == "") {
        return;
    }
    var reg = /^[0-9a-zA-Z]+$/;
    if ( !reg.test($(m_this).val()) ) {
        $.toast(info + "只能输入数字和字母");
        $(m_this).val("");
        return;
    }
    $(m_this).val($(m_this).val().toUpperCase());
}