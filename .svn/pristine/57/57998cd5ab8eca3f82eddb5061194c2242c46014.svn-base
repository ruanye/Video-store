$gridPercentSwitch:true;
@import "tobe/function";
@import "base.scss";
@import "pre.scss";
@charset "UTF-8";
body,
html {
    height: 100%;
}

body {
    font: 12px/1.5 "Microsoft YaHei", tahoma, arial, 'Hiragino Sans GB', '宋体', sans-serif;
}


.wrap-page {
    height: 100%;
    max-width: 10.0rem;
    margin: 0 auto;
    position:relative;
}

.page {
    height: 100%;
    background-color: #E00A14;

}
%rel{
    position: relative;
}
%posa{
    position: absolute;
}
%colorf{
    color: #fff;
}
.text-center{
    text-align: center;
}
$width: 100%;
.page1{
    @extend  %rel;
    overflow-y: scroll;
   min-height: 500px;
    background-size:100% 100%;
    background-repeat: no-repeat;
    background-position: center  center;
      background-image: url(../images/bg1.png);
    .container{

    width: 100%;
    @extend %posa;
    margin-top:4.6875rem;
    text-align: center;
   
    .text1{
        @extend %colorf;
        font-size: 0.5rem;
         }
         input,.sure{
            margin-top: 0.46875rem; 
         }
         input{
            width: 7.0625rem;
            height: 1.25rem;
            font-size: 0.5rem;
            line-height: 1.25rem;
           
         }
         .sure{
            background-color: #ecea50;
            @include inline-block;
            width: 2.25rem;
            height: 0.8125rem;
            line-height: 0.8125rem;
            color: #231714;
            text-align: center;
            font-size: 0.5rem;
            
         }
}
  .text2{
            @extend %posa;
             @extend  %colorf;
            width: $width;
            text-align: center;
            font-size: 0.375rem;
            bottom: 3.125rem;
            }
}
.page2{
    @extend %rel;
    .text1,.text2,.text3,.text4{
         @extend %colorf;
         @extend %posa;
         width: 100%;
    }
    .text1{
        font-size: 0.96875rem;
        top: 1rem;
       
    }
    .text2{
        top:4rem;
    }
    .text3{
        bottom: 2.0rem;
    }
    .text4{
        bottom: 1.6rem;
    }
  .text2,.text3.text4{
        font-size: 0.375rem;
    }
  .imghbox{
     width: 100%;
     text-align: center;
     @extend %posa;
     top: 5rem;
    img{
        width: 6.96875rem;
        height: 5.421875rem;
        @include inline-block;
        margin-left: 30px;
    }
  }
  .btn{
    @extend %posa;
    width: $width;
    top: 10.8rem;
    text-align: center;
    a{
        @include inline-block;
        width: 2.875rem;
        height: 0.84375rem;
        background-color: #ecea50;
        color: #000;
        line-height: 0.84375rem ;
        font-size: 0.5rem;
        margin-left: 15px;
    }
  }
  .car{
    @extend %posa;
    width: 2.25rem;
    height: 0.71875rem;
    left: 0.46875rem;
    bottom: 0;
  }

}
.page3{
    .text1{
         @extend %colorf;
         @extend %posa;
         width: 100%;
         font-size: 0.53125rem; 
         top: 2.84375rem;
    }
    .imghbox{
         @extend %posa;
        width: $width;
        text-align: center;
         top: 6.875rem;
        span{
           
            @include inline-block;
            width: 7.09375rem;
            height: 1.953125rem;
            line-height: 1.953125rem;
            font-size: 0.53125rem;
            background-image: url(../images/rebtn.png);
            background-size: 100% 100%;

        }
    }
    .text2{
          @extend %colorf;
         @extend %posa;
         width: $width;
         top:9.375rem;
         font-size: 0.5625rem;
         span{
            font-size: 0.75rem;
         }

    }
    .text3{
        width: $width;
        @extend %colorf;
        font-size:0.28125rem;
        @extend %posa;
        top: 10.625rem;
    }
    .bolang{
        @extend %posa;
        width: 100%;
        bottom: 0.15625rem;
        img{
            width: 100%;
            height: 2.875rem;
        }
    }
}
.page5{
    .cloud{
        width: 4.0rem;
        height: 1.71875rem;
        @extend %posa;
        left: 1.09375rem;
        top: 1.03125rem;
    }
    .car{
        width: 1.359375rem;
        height: 0.40625rem;
        @extend %posa;
        right:1.5rem;
        top: 4.578125rem;
    }
    .text1{
        width: $width;
        @extend %colorf;
        @extend %posa;
        font-size: 0.75rem;
        top: 5.28125rem;
    }
    .btnbox{
        width: $width;
        text-align: center;
         @extend %posa;
         top:6.5625rem;
    .btn{
         @include inline-block;
         width: 4.90625rem;
        height: 1.0625rem;
        line-height: 1.0625rem;
        background-color: #ecea50;
        @include border-radius(8px);
        font-size: 0.5rem;
    }
    }
    .shou{
        @extend %posa;
        width: $width;
        text-align: center;
        top: 8.09375rem;
        img{
         @include inline-block;
         width: 1.734375rem;
         height: 5.046875rem;

                }
    }
 
}
.page7{
    @extend %rel;
    .car{
        @extend %posa;
        width: 2.34375rem;
        height: 0.84375rem;
        bottom: 0;
        right: 0.46875rem;
    }
    .share{
        @extend %posa;
        width: 4.4375rem;
        height: 1.734375rem;
        right: 0.703125rem;
        top: 0.46875rem;
    }
    .text1{
        @extend %posa;
        @extend %colorf;
        font-size: 0.5625rem;
        left: 1.65625rem;
        top: 3.375rem;
        span{
            font-size: 0.9375rem;
        }

    }
    .xiaou{
        width: 6.328125rem;
        height: 5.3125rem;
        @extend %posa;
          left: 1.65625rem;
       top: 6.6rem;
    }
}
.page6{
    .textbox{
        @extend %posa;
        left: 1.25rem;
        top: 1.40625rem;
    }
    .starbox{
        width: $width;
        text-align: center;
        @extend %posa;
        top: 0.8125rem;
        z-index: 500;
  .star{
     width: 8.21875rem;
    height: 10.953125rem;
      
    }
    }
    .text1{
        text-align: left;
    @extend %colorf;
    font-size: 0.9375rem;
     }
     .text2{
        @extend .text1;
        font-size: 0.375rem;
     }
    .imghbox{
    @extend %rel;
    width: 8.109375rem;
    height: 4.75rem;
   margin-top: 3.46875rem;
    background-image: url(../images/hb1.png);
    background-size: 100% 100%;
        img{
            @extend %posa;
            &.circle{
                width: 3.375rem;
                height: 3.390625rem;
                left: 50%;
                top: 50%;
                margin-top: -1.6875rem;
                margin-left: -1.6875rem;
                @include animation(myrotate 0.3s ease infinite);
               
             

            }
            &.lq{
                width: 1.984375rem;
                height: 1.984375rem;
                  left: 50%;
                top: 50%;
                margin-left:-0.992188rem;
                margin-top: -0.992188rem;
            }
        }
    }

   
}
.logobox{
    @extend %posa;
    width: $width;
    text-align: center;
    bottom: 0;
.logo{
     @include inline-block;
    width: 4.75rem;
    height: 1.515625rem;
   
}
}
@keyframes bounce {
0%, 100%, 20%, 53%, 80% {
-webkit-transition-timing-function:cubic-bezier(0.215, .61, .355, 1);
transition-timing-function:cubic-bezier(0.215, .61, .355, 1);
-webkit-transform:translate3d(0, 0, 0);
transform:translate3d(0, 0, 0)
}
40%, 43% {
-webkit-transition-timing-function:cubic-bezier(0.755, .050, .855, .060);
transition-timing-function:cubic-bezier(0.755, .050, .855, .060);
-webkit-transform:translate3d(0, -30px, 0);
transform:translate3d(0, -30px, 0)
}
70% {
-webkit-transition-timing-function:cubic-bezier(0.755, .050, .855, .060);
transition-timing-function:cubic-bezier(0.755, .050, .855, .060);
-webkit-transform:translate3d(0, -15px, 0);
transform:translate3d(0, -15px, 0)
}
90% {
-webkit-transform:translate3d(0, -4px, 0);
transform:translate3d(0, -4px, 0)
}
}
