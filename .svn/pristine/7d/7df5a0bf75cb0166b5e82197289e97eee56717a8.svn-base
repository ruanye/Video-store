}

.number{
    display: inline-block;
    position: relative;
        top: 6px;
    .bit{
        float: left;
        position:relative;
        .v{
            color: #FFF;
        }
        .a{
            position:absolute;
            bottom:0px;
            left: 0px;
            color: #FFF;
            display: none;
            &.active{
                display: block;
                // bottom:10px;
                animation:hide 0.5s;
                animation-timing-function: cubic-bezier(0, 0.88, 1, 1);
                opacity: 0;
            }
            @keyframes hide{
                from{
                    opacity: 1;
                }
                to{
                    bottom:20px;
                    opacity: 0;
                }
            }
        }
    }
}