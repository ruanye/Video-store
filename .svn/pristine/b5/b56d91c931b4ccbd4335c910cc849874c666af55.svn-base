<!DOCTYPE html>
<html>
<head lang="en">
    <meta charset="UTF-8">
    <title></title>
    <script type="text/javascript" src="js/static/jquery/jquery-1.12.3.min.js"></script>
    <script type="text/javascript" src="js/static/libs/ss.js"></script>
    <script type="text/javascript" src="js/static/libs/ls.js"></script>
    <script src="js/static/libs/base.js"></script>
    <script>
        var openid = getQueryString(Constants.openid);
        var userid = getQueryString(Constants.userid);
        if(openid == null){
            SS.set(Constants.errorInfo, "无法获取微信openid");
            location.href = Constants.errorPage;
        }else{
            LS.set(Constants.openid, openid);
            LS.set(Constants.userid, userid);
            if(LS.get(Constants.sourceURL) != null)
                location.href = LS.get(Constants.sourceURL);
            else
                location.href = "index.html";

        }



    </script>
</head>
<body>
</body>
</html>