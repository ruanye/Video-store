<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <title>麦可思智能助教平台</title>
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="renderer" content="webkit">
    <meta name="renderer" content="webkit">
    <meta name="viewport" content="width=device-width, initial-scale=1.0" user-scalable="no">
    <meta name="keywords" content="">
    <meta name="description" content="">
    <link rel="stylesheet" type="text/css" href="css/index.css">

    <!--[if lt IE9]>
    <script src="js/html5.js"></script>
    <script src="js/IE7.js"></script>
    <![endif]-->
</head>

<body>
    <header>
        <nav id="header_menu">
        </nav>
    </header>
    <div class="header-fixed">
        <nav id="header_menu_fixed">
        </nav>
    </div>


    <section class="container">
        <div class="content">
            <div class="select-left" id="lessons-btn">
                <div class="class-system">课上系统</div>
            </div>
            <div class="select-right" id="after-class-btn">
                <div class="after-class-system">课下系统</div>
            </div>
        </div>


        <div class="course" style="display: none;">
            <div class="clock">
                <span id="Date"></span>
                <span id="hours">00</span>
                <span id="point">:</span>
                <span id="min">00</span>
                <span id="point">:</span>
                <span id="sec">00</span>
            </div>
            <div class="course-list">
                <table>
                    <thead>
                        <tr class="course-item">
                            <th class="item-td"></th>
                            <th class="item-td">星期一</th>
                            <th class="item-td">星期二</th>
                            <th class="item-td">星期三</th>
                            <th class="item-td">星期四</th>
                            <th class="item-td">星期五</th>
                            <th class="item-td">星期六</th>
                            <th class="item-td">星期日</th>
                        </tr>
                    </thead>
                    <tbody id="course-list-body">

                    </tbody>
                </table>
            </div>
        </div>
    </section>
    <script type="text/javascript" src="js/libs/jQuery/jquery-1.12.3.min.js"></script>
    <script type="text/javascript" src="js/ss.js"></script>
    <script type="text/javascript" src="js/static/src/tiem.js"></script>
    <script type="text/javascript" src="js/common.js"></script>
    <script type="text/javascript" src="js/static/src/lessons.js"></script>
</body>
</html>
