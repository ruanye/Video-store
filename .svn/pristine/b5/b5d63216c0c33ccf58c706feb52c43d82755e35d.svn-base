 $gridPercentSwitch:true;
 @import "tobe/function";
 @import "base.scss";
 @import "pre.scss";
 @charset "UTF-8";
 $s26:0.40625rem;
 $red:#cc0000;
 %fz24 {
     font-size: 0.375rem;
 }
.onepage{
    margin-bottom: 4rem;
    height: 100%;
}
 
 body {
     background-color: #f3f3f3;
     position: relative;
 }
 
 .weui_cell {
     color: #5a5a5a;
 }
 
 .weui_input {
     &.pay {
         text-align: right;
     }
 }
 
 .Serial {
     -webkit-overflow-scrolling :touch;
     .noAddresss{
    text-indent: 0.46875rem;
 }
     .mark {
         width: 100%;
         height: 0.125rem;
         background: #f3f3f3;
     }
     .address {
         position: relative;
         padding: 0.390625rem 0;
         height: 1rem;
         &:after {
            background-image: url(../images/shoplist/sanjiao1.png);
             background-size: 100% 100%;
             width: 0.265625rem;
             height: 0.453125rem;
             content: '';
             display: inline-block;
             position: absolute;
             font-family: consolas;
            
             right: 0.546875rem;
             top: 50%;
             margin-top: -0.234375rem;
             padding-top: 0.15625rem;
             color: #989898;
         }
         .addicon {
             position: absolute;
             width: 0.375rem;
             height: 0.46875rem;
             top: 50%;
             margin-top: -0.234375rem;
             left: 0.46875rem;
         }
     }
     margin-bottom: 0.09375rem;
     background-color: #fff;
     &.last {
         margin-bottom: 2.5rem;
     }
     &.olast {
         margin-bottom: 4rem;
     }
     h4 {
         height: 0.875rem;
         line-height: 0.875rem;
         border-bottom: 1px solid #e5e5e5;
     }
     h5 {
         height: 1.21875rem;
         line-height: 1.21875rem;
         border-top: 1px solid #e5e5e5;
         text-align: right;
         margin-top: 0.15625rem;
         color: #696969;
         span {
             margin-right: 0.40625rem;
         }
     }
     h4,
     h5 {
         font-size: $s26;
         font-weight: 400;
         text-indent: $s26;
         span {
             color: $red;
         }
     }
     p {
         font-size: 0.375rem;
         color: #696969;
        
         line-height: 1.03125rem;
         
         span{
            margin-left: 0.3125rem;
         }
         &.ad {
             text-indent: 1.171875rem;
             line-height: 1.5;
         }
         &.n {
             font-size: 0.40625rem;
         }
         &.Remark {
             overflow: hidden;
             clear: both;
             text-indent: 0.3125rem;
             padding: 0.21875rem 0 0.15625rem 0;
            
             span,
             input {
                 float: left;
             }
             input {
                 width: 8rem;
                 height: 0.84375rem;
                 text-indent: 0.3125rem;
                 background-color: #fff;
                 @include border-radius(0.125rem);
                 border: 1px solid #dcdcdc;
                 outline: none;
             }
         }
     }
 }
 
 .goods {
     clear: both;
     overflow: hidden;
     height: auto;
     padding: 0.1875rem 0;
     .gimg,
     .gtext {
         float: left;
     }
     .gimg {
         margin-left: 0.6875rem;
         border: 1px solid #dcdcdc;
         width: 1.5rem;
         height: 1.5rem;
         img {
             width: 1.5rem;
             height: 1.5rem;
         }
     }
     .gtext {
        margin-left: 0.15625rem;
        width:5rem;
        text-align: left;
         font-size: 0.375rem;
         p {

             text-align: left;
             line-height: 2;
             &.title {
                 font-size: $s26;
                
             }
         }
         span {
             &.red {
                 color: $red;
             }
             &.oneprice {
                 margin-left: 0.625rem;
             }
         }
     }
     .rtext {
          width:2rem;
         
         p {
            width:2rem;
            margin-right: 0.40625rem;
              word-break: break-all;
             &.title {
                text-align: right;
                 color: #c00;
             }
         }
     }
 }
 
 .note {
     position: fixed;
     width: 10.0rem;
     height: 1.671875rem;
     padding-top: 0.34375rem;
     background-color: #fff;
     bottom: 0;
     color: #666;
     font-size: 0.375rem;
     text-indent: $s26;
     span {
         color: #000;
     }
 }
 
 .bottom {
     width: 100%;
     position: fixed;
     background-color: #fff;
     bottom: 0;
     height: 3.265625rem;
     .text {
         border-top: 1px solid #dcdcdc;
         height: 1.8125rem;
     }
     .moneyi {
         line-height: 2.35;
         color: #595959;
         margin-left: 0.34375rem;
         font-size: 0.375rem;
         span {
             float: right;
             margin-right: 0.40625rem;
         }
     }
     .billing {
         width: 100%;
         height: 1.453125rem;
         border-top: 1px solid #dcdcdc;
         clear: both;
         overflow-y: hidden;
         position: relative;
         p {
             font-size: 0.46875rem;
             float: left;
             height: 1.453125rem;
             line-height: 1.453125rem;
             width: 100%;
             color: #c82121;
             text-align: center;
             &.allprice {
                 text-indent: -3.5625rem;
             }
             &.settlement {
                 width: 3.5625rem;
                 background-color: #40b952;
                 color: #fff;
                 position: absolute;
                 right: 0;
             }
         }
     }
 }
 

   .boxquery,
    .order-status {
        //border-top: 1px solid #dcdcdc;
    
    }
    .wrap-page .order-status .order-time.staustwo{
        height:auto!important;
    }
    .order-status.one{
         box-shadow: 0px 5px 10px #dcdcdc;
    }
    .order-status.two {
        padding:0.15625rem 0;
        margin-top: 10px;
        margin-bottom: 10px;
         height:auto!important;
         box-shadow: 0px 5px 10px #dcdcdc;
    }
    
    .boxquery .list.one {
        -webkit-box-shadow: none;
        box-shadow: none;
    }
    .status_bar.one{
         height: 1rem;
         line-height: 1rem;
        
        
    }
    .status_bar.one .bartext p{
           font-weight: 400!important;
    }
    

    .cusAddress{
        text-align:justify;        
        text-align-last:justify; 
        @include inline-block;
        width:2.18rem;
    }
    .myneedshadow{
        box-shadow: 0px 5px 10px #dcdcdc;
    }