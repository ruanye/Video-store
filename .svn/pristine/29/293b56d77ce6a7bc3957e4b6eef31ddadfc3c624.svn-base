    <script src="vendor/zepto/zepto.min.js"></script>
    <script type="text/javascript" src="js/main.js"> </script>
    <script>
           var tel=document.querySelector('#tel');
        var page1=document.querySelectorAll(".page1")[0];
        console.log(page1);
        tel.onfocus=function () {
          page1.classList.add('trany')
        }
           tel.onblur=function () {
          page1.classList.remove('trany')
        }
    </script>
   
</body>

</html>
