        <div class="putimg text-center"><img src="images/putimg.png" alt=""></div>
        <div class="text-center inputbox"><input type="text"></div>
        <div class="text-center btnbox"><button class="btn">确定</button></div>
        </section>
        <section class="page page2 ">
        <div class="logo text-center"><img src="images/logo.png" alt=""></div>
         <div class="cy text-center"><img src="images/cy.png" alt=""></div>
         <div class="hb text-center"><img src="images/hb.png" alt=""></div>
