$gridPercentSwitch:true;
@import "tobe/function";
@import "base.scss";
@charset "UTF-8";

html{
    background:#ffffff;
}
.w {
    width: 990px;
    margin: 0 auto;
}

#login {
    position: relative;
    width: 300px;
    margin: 25px 0 5px 0;
    height: 60px;
}

#content {
    border-top: 2px solid #de0313;
}

.login-wrap {
    position: relative;
    height: 475px;
    margin: 0 0 20px;
    z-index: 2
}

.login-wrap .login-banner {
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 475px;
    background: #032540
}

.login-wrap .i-inner {
    position: relative;
    z-index: 3;
    height: 475px;
    background: url(../images/banner-bg.png) center no-repeat;
    background-size: 100% 100%;
}

.login-wrap .login-form {
    float: right;
    top: 40px;
}

.login-form {
    position: relative;
    z-index: 4;
    width: 306px;
    background: rgba(255, 255, 255, 0.8);
    padding: 20px;
    overflow: visible;
}

.login-form .login-box .mt {
    display: inline-block;
    display: block;
    width: 306px;
    overflow: visible;
    height: 27px
}

.login-form .login-box .mt:after {
    content: ".";
    display: block;
    height: 0;
    clear: both;
    visibility: hidden
}

.login-form .login-box .mt h1 {
    position: relative;
    height: 27px;
    line-height: 27px;
    top: -5px;
    float: left;
    color: #666;
    font-family: '\5fae\8f6f\96c5\9ed1';
    font-size: 18px;
}

.login-form .login-box .mt .extra-r {
    float: right
}

.login-form .login-box .mt .regist-link {
    color: #b61d1d;
    font-size: 14px
}

.login-form .login-box .mt .regist-link a {
    color: #b61d1d
}

.login-form .login-box .mt .regist-link b {
    display: inline-block;
    zoom: 1;
    width: 16px;
    height: 16px;
    overflow: hidden;
    background: url(../images/login-icons.png) -1px -75px no-repeat;
    vertical-align: middle;
    margin-right: 5px;
}

.login-form .msg-wrap {
    min-height: 31px;
    height: auto !important;
    height: 31px;
    margin: 2px 0 5px
}

.login-form .msg-error {
    position: relative;
    background: #ffebeb;
    color: #e4393c;
    border: 1px solid #e4393c;
    padding: 3px 10px 3px 40px;
    line-height: 18px;
    min-height: 18px;
    _height: 18px
}

.login-form .msg-error b {
    position: absolute;
    top: 50%;
    left: 10px;
    display: block;
    margin-top: -8px;
    width: 16px;
    height: 16px;
    overflow: hidden;
    background: url(../images/login-icons.png) -1px -49px no-repeat
}

.login-form .msg-warn {
    position: relative;
    background: #fff6d2;
    color: #666;
    border: 1px solid #ffe57d;
    padding: 3px 10px 3px 40px;
    line-height: 18px;
    height: 18px
}

.login-form .msg-warn b {
    position: absolute;
    top: 50%;
    left: 10px;
    display: block;
    margin-top: -8px;
    width: 16px;
    height: 17px;
    overflow: hidden;
    background: url(../images/login-icons.png) -1px -23px no-repeat
}

.login-item {
    color: #7b8793;
    width: 300px;
    height: 40px;
    margin: 0 auto 20px auto;
    background: #fff;
    border: 1px solid #7b8793;
    -moz-border-radius: 0;
    -webkit-border-radius: 0;
    border-radius: 0
}

.login-item input {
    height: 16px;
    border: none;
    background: none;
    width: 250px;
    padding: 13px 0 8px 0;
    margin-left: 50px;
    line-height: 16px;
    font-size: 14px
}

.login-item lable {
    position: absolute;
    padding: 0 10px 0 30px;
    border-right: 1px solid #7b8793;
    line-height: 40px;
    height: 40px;
    display: inline-block
}

.item1 {
    background: url(../images/user.png) no-repeat 10px center
}

.item2 {
    background: url(../images/lock.png) no-repeat 10px center
}

.item3 {
    background: url(../images/shield.png) no-repeat 10px center;
    letter-spacing: 1px
}

.form .item {
    position: relative;
    margin-bottom: 20px;
    z-index: 1
}

.login-foot {
    height: 30px;
    width: 300px;
    margin: 0 auto;
    color: #000000;
    text-align: right;
    font-size: 12px;
}

.login-foot a {
    color: #666666;
}

.login-foot a:hover {
    color: #de0313;
    pointer: cursor;
}

.btn-primary.active,
.btn-primary.focus,
.btn-primary:active,
.btn-primary:focus,
.btn-primary:hover {
    color: #fff;
    background-color: #c61c1f;
    border-color: #c61c1f
}

.btn.focus,
.btn:focus,
.btn:hover {
    color: #fff;
    text-decoration: none
}

.btn-primary {
    color: #fff;
    background-color: #de0313;
    border-color: #de0313
}

.btn-primary:active {
    position: relative;
    top: 1px
}

.btn {
    display: block;
    width: 300px;
    height: 48px;
    margin: 0 auto;
    line-height: 48px;
    font-size: 18px;
    font-weight: 400;
    text-align: center;
    cursor: pointer;
    border: 1px solid transparent;
    border-radius: 3px
}

.footer {
    width: 990px;
    margin: 0 auto;
    text-align: center;
    font: 12px/150% Arial, Verdana, "宋体";
    color: #000000;
}

.footer .links a,
.footer .links span {
    margin: 0px 10px;
}

.footer .copyright {
    margin: 10px 0px;
}
