gin: auto;\r\n      height: 0.9375rem;\r\n      width: 5rem;\r\n      border: 0px;\r\n      text-align: center;\r\n      margin-bottom: .3rem;\r\n    }\r\n    >.btn{\r\n      display: block;\r\n      height: 0.9375rem;\r\n      width: 5rem;\r\n      background: #40B0A2;\r\n      color: #FFF;\r\n      font-size: 0.4375rem;\r\n      line-height: 0.9375rem;\r\n      margin: auto;\r\n      margin-bottom: .3rem;\r\n    }\r\n\r\n    >.result{\r\n      width: 7rem;\r\n      // height: 6rem;\r\n    }\r\n  }\r\n  .share{\r\n    position: absolute;\r\n    bottom:0.5rem;\r\n    left: 50%;\r\n    transform: translate(-50%,0);\r\n    text-align: right;\r\n    height: 0.9rem;\r\n    line-height: 0.9rem;\r\n    width: 5.5rem;\r\n    background: #40B0A2;\r\n    color: #FFF;\r\n    font-size: 0.4375rem;\r\n\r\n    margin: auto;\r\n    margin-bottom: .3rem;\r\n    border-radius: 8px;\r\n  }\r\n  &.active{\r\n    display: block;\r\n  }\r\n}\r\n\r\n\r\n.mask{\r\n  display: none;\r\n  position: fixed;\r\n  top:0;\r\n  left: 0;\r\n  height: 100%;\r\n  width: 100%;\r\n  background-color: rgba(0,0,0,.9);\r\n  &.active{\r\n    display: block;\r\n  }\r\n}\r\n.maskshare{\r\n  >img{\r\n    position: absolute;\r\n    width: 9rem;\r\n    top: 0px;\r\n    right: 0px;\r\n  }\r\n}\r\n"],"sourceRoot":"/source/"}