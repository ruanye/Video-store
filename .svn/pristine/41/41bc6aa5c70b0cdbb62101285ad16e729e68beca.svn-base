@import "tobe/function";
@mixin font-dpr($fonts-size) {
    font-size: $fonts-size;
    [data-dpr="2"] & {
        font-size: $fonts-size * 2;
    }
    [data-dpr="3"] & {
        font-size: $fonts-size * 3;
    }
}

* {
    margin: 0;
    padding: 0;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
    -webkit-appearance: none;
    font-family: "Microsoft YaHei";
}

button {
    border: none;
}

button:focus {
    outline: none;
}

html,
body {
    height: 100%;
}

.wrap-page {
    height: 100%;
    background-origin: content-box;
    -webkit-background-origin: content-box;
    -webkit-overflow-scrolling: touch;
    background: #F1F1F1;
    background-size: 100% auto;
    text-align: center;
    max-width: 760px;
    width: 100%;
    margin: auto;
}

.page {
    min-height: 100%;
    width: 100%;
    display: flex;
    @include flex-direction;
}

.inner {
    width: 100%;
}

.flex-box {
    display: flex;
}

.flex-1 {
    flex: 1;
}

.flex-m {
    -webkit-box-pack: center;
    -ms-flex-pack: center;
    -webkit-justify-content: center;
    justify-content: center;
    -webkit-box-align: center;
    -ms-flex-align: center;
    -webkit-align-items: center;
    align-items: center;
    display: flex;
}

.flex-d {
    -webkit-box-pack: center;
    -ms-flex-pack: center;
    -webkit-justify-content: center;
    justify-content: center;
    -webkit-box-align: center;
    -ms-flex-align: center;
    -webkit-align-items: flex-end;
    align-items: flex-end;
    display: flex;
}

.ipt {
    outline: none;
    width: 6.573333rem;
    height: 1.106667rem;
    line-height: 0.826667rem;
    border-radius: 0;
    border: 2px solid #FFD7D2;
    color: #FFD7D2;
    -webkit-appearance: none;
    background: #fff;
    padding: 0.334375rem;
    text-align: center;
    @include font-dpr(16px);
}

.fixed-logo {
    width: 4.373333rem;
    position: absolute;
    top: 0.4rem;
    left: 0.4rem;
}

.row:after,
.row:before {
    display: table;
    content: " ";
    clear: both
}

