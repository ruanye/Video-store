  <script type="text/javascript">
    $(".hongbao-img").click(function(){
      $(this).addClass("shake");
      setTimeout(function(){
        $(".hongbao-img").removeClass("shake");
        $("#page1").hide();
        $("#page2").show();
      },1500);
    }); 
    window.onload=function(){ 
      var value = 12;
      drwa(value);
      setInterval(function(){
        value++;
        drwa(value);
      },1500);
    }
    function drwa( value ){
      var orangVluae = $("#number").get(0).value || 0;
      var htmlString = "";
      $("#number").get(0).value = value;
      do{
        if( value <= 0){
          break;
        }
        var v = value%10;
        value = Math.floor( value/10);
        var ov = orangVluae%10;
        orangVluae = Math.floor( orangVluae/10);
        htmlString ='<div class="bit">'+
            '<span class="a'+((ov!=v)?' active':'')+'">'+ov+'</span>'+
            '<span class="v">'+v+'</span>'+
          '</div>'+htmlString;
      }while(true);
      $("#number").html(htmlString);
    }

  </script>
